<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="LocalDimensions" title="Local Dimensions">
    <Filter source="LocalSpec1" view="Custom_RoleRestricted_Standard" title="Local Specification 1">
      <With>
        <Card view="Custom_RoleRestricted_Full">
          <Expansions>
            <With>
              <Table view="Custom_RoleRestricted_Full">
                <Expansions minimized="true" parentSize="huge">
                  <Bind foreignKey="DimCombVersionNumberVar_DimCombVersionHeader">
                    <Hidden source="DerivedDimensions">
                      <Expansions minimized="true">
                        <With>
                          <Table view="Custom_RoleRestricted_Dimensions" title="Revisions" />
                        </With>
                      </Expansions>
                    </Hidden>
                  </Bind>
                </Expansions>
              </Table>
            </With>
          </Expansions>
          <Assistants parentSize="large">
            <Bind foreignKey="LocalSpecStructureNameVar_LocalSpec1StructureHeader">
              <Card source="LocalSpec1Structure" view="Custom_RoleRestricted_Full" title="Structure">
                <With>
                  <Table view="Custom_RoleRestricted_Full" title="Specification" />
                </With>
              </Card>
            </Bind>
            <Mount>
              <Filter source="DimensionGroupings" view="Dimension('LocalSpecification1')" title="Dimension Groupings">
                <Expansions minimized="true">
                  <With>
                    <Card view="Custom_RoleRestricted_Full" title="Dimension Grouping">
                      <With>
                        <Table view="Custom_RoleRestricted_Full" title="Specification" />
                      </With>
                    </Card>
                  </With>
                </Expansions>
              </Filter>
              <Filter source="ReportingHierarchies" view="Dimension('LocalSpecification1')" title="Reporting Hierarchies">
                <Expansions minimized="true">
                  <With>
                    <Card view="Custom_RoleRestricted_Full" title="Reporting Hierarchy">
                      <Expansions parentSize="medium">
                        <With>
                          <Table view="Custom_RoleRestricted_Full" title="Nodes" />
                        </With>
                      </Expansions>
                    </Card>
                  </With>
                </Expansions>
              </Filter>
            </Mount>
            <Restrict reversedForeignKey="LocalSpec1List_LocalSpec1Header">
              <Filter source="CompanyInfo" view="Minimal" title="Used By" />
            </Restrict>
          </Assistants>
        </Card>
      </With>
    </Filter>
