<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="TimeAndExpenseRegistration" title="Time &amp; Expenses">
    <Define>
      <Function name="isMyPlanEnabled" value="isEnvVarDefined('peopleplanner.server.host')" type="boolean" />
