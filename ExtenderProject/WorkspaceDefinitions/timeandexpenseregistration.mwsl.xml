      <Section title="Time">
        <Mount>
          <Card source="TimeRegistration" title="Week" name="Time_WeekCard" view="Full">
            <Expansions parentSize="small">
              <With>
                <Table title="Registrations">
                  <Expansions minimized="true">
                    <Bind foreignKey="primary">
                      <Card source="TimeRegistrationDetails" title="Descriptions" view="Descriptions" />
                    </Bind>
                    <Bind foreignKey="primary">
                      <Card source="TimeRegistrationDetails" title="Details" view="Details" />
                    </Bind>
