      <!-- ************ VIM, TEM and Pay Roll Interface ************** -->
      <Section title="Travel Expenses" hidden="not isEnvVarDefined('user.employeeinfo.text7')" >
        <Mount>
      	  <Card source="TimeRegistration" view="TEM" />
      	</Mount>
      </Section>
      
      <Section title="Vendor Invoices" >
        <Mount>
      	  <Card source="TimeRegistration" view="VIM" />
      	</Mount>
      </Section>
      
      <Section title="Pay Roll" >
      	<Mount>
      		<Card source="TimeRegistration" view="PayRoll" />
      	</Mount>
      </Section>
      
      <!-- ************************************************* -->
      
      <Section title="Favorites">
        <Mount>
          <Hidden source="JobFavorites">
            <With>
              <Table view="Demo" title="List of Favorites">
                <Expansions minimized="true">
                  <Bind foreignKey="primary">
                    <Hidden source="JobFavorite" />
                  </Bind>
                </Expansions>
              </Table>
            </With>
          </Hidden>
        </Mount>
      </Section>
      
