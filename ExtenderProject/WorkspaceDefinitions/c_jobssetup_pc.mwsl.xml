<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="C_JobsSetup_PC" title="Jobs Setup">
    <Formation>
      <Section title="Dimensions" name="Dimensions">
        <Mount>
          <Filter source="Activities" title="Activities" view="Custom" name="Activities">
            <With>
              <Card title="Activity" view="ReadOnly" >
                <With>
                  <Table title="Job Posting Reference Lines" view="ReadOnly" />
                </With>
              </Card>
            </With>
          </Filter>
        </Mount>
      </Section>
      <Section title="Pricing">
        <Mount>
          <Filter source="JobPriceLists" title="Price Lists" view="Standard">
            <With>
              <Card title="Price List" view="Demo">
                <With>
                  <Table title="Prices" view="Demo" />
                </With>
              </Card>
            </With>
          </Filter>
          <Card source="JobPriceGroups" title="Price Groups" view="ReadOnly" >
            <With>
              <Table title="Price Group" view="ReadOnly" />
            </With>
          </Card>
        </Mount>
      </Section>
    </Formation>
  </Workspace>
</MWSL>

