              <Table title="Cost" view="Readonly" hidden="parent.parent.CostJobPriceList = ''" />
            </With>
          </Hidden>
        </Bind>
        <Bind foreignKey="JobPriceListCurrentIssue">
          <Hidden source="JobPriceLists">
