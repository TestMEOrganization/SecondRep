<?xml version="1.0" encoding="UTF-8"?>
<!-- CSK, 24/04/09: New Posting workspace (37668).-->
<!-- MJA, 26/06/09: Rename "Original" views to "Standard" (37797).-->
<!-- CSK, 06/07/09: Use Original view instead of Standard view (37668).-->
<!-- CSK, 11/05/10: Missing views added (38485).-->
<!-- CSK, 22/11/10: More missing views added (38485).-->
<!-- CSK, 18/03/11: New Dimensions workspace (39492).-->
<!-- CSK, 10/10/11: Rename Account assistant to Dimension Combination (39492).-->
<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="Posting" title="Posting">
    <Card source="PostingDescriptions" view="Custom_RoleRestricted_Full">
      <Expansions parentSize="small">
        <With>
          <Table view="Custom_RoleRestricted_Full" />
        </With>
      </Expansions>
    </Card>
    <Filter source="PostingReferences" view="Custom_RoleRestricted_Standard" title="Posting References">
      <Expansions minimized="true">
        <With>
          <Card view="Custom_RoleRestricted_Full" title="Posting Reference">
            <Expansions parentSize="small">
              <With>
                <Table view="Custom_RoleRestricted_Full">
                  <Assistants parentSize="large">
                    <Bind foreignKey="AccountNumber_DimensionCombHeader">
                      <Card source="DimensionCombinations" view="ReadOnly" title="Dimension Combination">
                        <Expansions parentSize="large">
                          <With>
                            <Table view="ReadOnly" />
                          </With>
                        </Expansions>
                        <Assistants>
                          <Bind foreignKey="AccountNumber_Account">
                            <Card source="AccountCard" view="ReadOnly" title="Account" />
                          </Bind>
                        </Assistants>
                      </Card>
                    </Bind>
                  </Assistants>
                </Table>
              </With>
            </Expansions>
          </Card>
        </With>
      </Expansions>
    </Filter>
    <Card source="IntercompanyAccounts" view="Custom_RoleRestricted_Full">
      <Expansions parentSize="small">
        <With>
          <Table view="Custom_RoleRestricted_Full">
            <Assistants parentSize="large">
              <Bind foreignKey="ResponsibleCompanyAccount_DimensionCombHeader">
                <Card source="DimensionCombinations" view="ReadOnly" title="Posting Reference, Responsible Company">
                  <Expansions>
                    <With>
                      <Table view="ReadOnly" />
                    </With>
                  </Expansions>
                  <Assistants>
                    <Bind foreignKey="AccountNumber_Account">
                      <Card source="AccountCard" view="ReadOnly" title="Account" />
                    </Bind>
                  </Assistants>
                </Card>
              </Bind>
              <Bind foreignKey="IntercompanyCompanyAccount_DimensionCombHeader">
                <Card source="DimensionCombinations" view="ReadOnly" title="Posting Reference, Intercompany Company">
                  <Expansions>
                    <With>
                      <Table view="ReadOnly" />
                    </With>
                  </Expansions>
                  <Assistants>
                    <Bind foreignKey="AccountNumber_Account">
                      <Card source="AccountCard" view="ReadOnly" title="Account" />
                    </Bind>
                  </Assistants>
                </Card>
              </Bind>
            </Assistants>
          </Table>
        </With>
      </Expansions>
    </Card>
  </Workspace>
</MWSL>

