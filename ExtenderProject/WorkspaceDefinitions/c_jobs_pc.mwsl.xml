<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="C_Jobs_PC" title="Jobs">
    <Filter name="Jobs_Filter" source="Jobs" title="Job List" view="JobHome">
      <Formation>
        <Section title="Home">
          <Bind foreignKey="primary">
            <Workspace source="C_JobHome_PC" />
          </Bind>
         
          <!-- <Restrict reversedForeignKey="JobNumber_JobHeader">
            <Filter source="JobReallocation" title="Reallocate Job Entries">
              <With>
               <Card title="Reallocation">
		         <With>
			    	<Table title="Reallocate to..." />
			    </With>
		      </Card>
		      </With>
            </Filter>
          </Restrict> -->
        </Section>
        <Section title="Budgeting" name="Budgeting_Section">
          <Bind foreignKey="primary">
            <Workspace source="C_JobBudgeting_PC" />
          </Bind>
        </Section>
        <!-- <Section title="Job Activities" name="Execution_Section">
					<Bind foreignKey="primary">
						<Workspace source="JobExecution" />
					</Bind>
          <Bind foreignKey="primary">
            <Workspace source="JobPurchaseProcess" />
          </Bind>
          <Restrict reversedForeignKey="JobNumber_SimpleJobHeader">
            <Workspace source="JobPurchaseProcessLines" />
          </Restrict>
          <Bind foreignKey="CurrentBudgetType_JobBudget">
            <Workspace source="JobPurchaseFromBudget" />
          </Bind>
        </Section>-->
        <Section title="Progress Evaluation" hidden="(isEnvVarDefined('solutionconfiguration.UseJobProgress') and not booleanEnvVar('solutionconfiguration.UseJobProgress')) or (parent.JobGroup=jobgrouptype'\internal\ and parent.NotInvoiceable)">
          <Bind foreignKey="primary">
            <Workspace source="JobProgress" />
          </Bind>
        </Section>
        <Section title="Invoicing" hidden="parent.NotInvoiceable defaultTo false">
          <Bind foreignKey="primary">
            <Workspace source="C_JobInvoicing_PC" />
          </Bind>
	    <Mount>
	    	<Workspace source="SD_InvoiceDraftStatus" />
	    </Mount>
	    <Mount>
		    <Filter source="BlanketInvoicingOverview" view="Standard" hidden="not hasRole('Projects - Controllers')" >
		      <Expansions minimized="false">
		        <With>
		          <Card view="Full">
		            <Expansions>
		              <With>
		                <Table />
		              </With>
		            </Expansions>
		          </Card>
		        </With>
		      </Expansions>
		    </Filter>
	    </Mount>
	    <Bind foreignKey="primary">
          <Card source="JobInvoiceAllocation" view="Full" hidden="hasNoRecords() or not hasRole('Projects - Controllers')">
            <Expansions>
              <With>
                <Table />
              </With>
            </Expansions>
          </Card>
	    </Bind>
	    <Mount>
	    	<Workspace source="SD_ItemizeJobInvoiceAllocation" />
	    </Mount>
        </Section>
        <Section title="Closing">
          <Bind foreignKey="primary">
            <Card source="PendingJobActions" view="Standard">
              <With>
                <Table view="Full" />
              </With>
            </Card>
          </Bind>
        </Section>
        <Section title="Lookup">
          <Bind foreignKey="primary">
            <Workspace source="C_JobLookup_PC" />
          </Bind>
        </Section>
        <Section title="Setup" hidden="not hasRole('Project Admin') and not hasRole('Superuser')" >
          <Bind foreignKey="primary">
            <Workspace source="C_JobSetup_PC" />
          </Bind>
        </Section>
      </Formation>
    </Filter>
  </Workspace>
</MWSL>

