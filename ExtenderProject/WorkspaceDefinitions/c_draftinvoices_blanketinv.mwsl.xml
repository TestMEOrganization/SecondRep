<!-- AMC, 13/11/09: New workspace: Draft Invoice Editing (38106).-->
<!-- MJA, 09/08/10: Set names for use in ApproveInvoiceDrafts notification (38255).-->
<!-- MJA, 17/10/10: Update title (38597).-->
<!-- JSV, 04/03/11: Show the foundation as an expansion instead of an assistant (39458).-->
<!-- JSV, 21/06/11: Set parent size to small (39464).-->
<!-- JSV, 02/09/11: Rename DraftInvoiceEditing to DraftInvoices (39256).-->
<!-- SBA, 21/11/11: Use of defaultTo for hidden expression (40189).-->
<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="C_DraftInvoices_BlanketInv" title="Draft Invoices">
    <Filter source="InvoiceEditing" name="InvoiceEditing_Filter" view="Demo" title="Draft Invoices">
      <With>
        <Card name="InvoiceEditing_Card" hidden="not parent.parent.parent.ApprovedForInvoiceEditing defaultTo false" >
          <Expansions parentSize="small">
            <With>
              <Table title="Specification" />
            </With>
            <Restrict reversedForeignKey="InvoiceEditingHeader">
              <Filter source="API_DraftInvoiceEntry" view="Demo" title="Foundation" />
            </Restrict>
          </Expansions>
        </Card>
      </With>
    </Filter>
  </Workspace>
</MWSL>

