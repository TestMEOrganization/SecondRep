<!-- PSZ, 15/09/11: Name for Users filter (39939).-->
<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
	<Workspace name="C_UserManagement_ServiceDesk" title="Users">
		<Formation>
			<Section title="Users Management">
				<Mount>
					<Filter source="Users" title="Users" view="Custom" name="Users_Filter">
						<Expansions>
							<With>
								<Card title="User Information" view="Custom">
									<Assistants>
										<Bind foreignKey="primary">
											<Hidden source="ResetPassword" />
										</Bind>
									</Assistants>
								</Card>
							</With>
						</Expansions>
					</Filter>
				</Mount>
			</Section>
		</Formation>
	</Workspace>
</MWSL>
