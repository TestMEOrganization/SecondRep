<!-- KRN, 21/09/10: New workspace for supervisor time and expense approval (38894).-->
<!-- KRN/PSZ, 22/12/10: Sections added (39260).-->
<!-- PSZ, 19/05/11: Added supervisor time sheet status (39616).-->
<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="SupervisorTimeAndExpenseApproval" title="Supervisor Time and Expense Approval">
    <Formation>
      <Section title="Time" name="Time_Section">
        <Mount>
          <Card source="SupervisorTimeSheetStatus" view="Full" title="Time Sheet Status">
            <Expansions parentSize="small">
              <With>
                <Table view="Full">
                  <Expansions minimized="true">
                    <Bind foreignKey="Employee_TimeSheetHeader">
                      <Card source="TimeSheets" title="Time Sheet" view="SupervisorApproval(true)">
                        <With>
                          <Table title="Registrations" view="SupervisorApproval">
                            <Assistants parentSize="huge">
                              <Bind foreignKey="primary">
                                <Card source="TimeSheetLines" title="Descriptions" view="Descriptions" />
                              </Bind>
                              <Bind foreignKey="primary">
                                <Card source="TimeSheetLines" title="Details" view="Details" />
                              </Bind>
                            </Assistants>
                          </Table>
                        </With>
                      </Card>
                    </Bind>
                  </Expansions>
                </Table>
              </With>
            </Expansions>
          </Card>
          <Filter source="TimeSheets" name="ApprovalTimeSheets_Filter" title="List of Time Sheets" view="SupervisorApproval">
            <With>
              <Card name="ApprovalTimeSheets_Card" title="Time Sheet" view="SupervisorApproval(false)">
                <Expansions parentSize="small">
                  <With>
                    <Table title="Registrations" view="SupervisorApproval">
                      <Assistants parentSize="huge">
                        <Bind foreignKey="primary">
                          <Card source="TimeSheetLines" title="Descriptions" view="Descriptions" />
                        </Bind>
                        <Bind foreignKey="primary">
                          <Card source="TimeSheetLines" title="Details" view="Details" />
                        </Bind>
                      </Assistants>
                    </Table>
                  </With>
                </Expansions>
              </Card>
            </With>
          </Filter>
        </Mount>
        <!-- <Mount>
          <Filter source="DailyTimeSheets" name="DailyTimeSheets_Filter" title="List of Daily Time Sheets" view="SupervisorApproval">
            <With>
              <Card name="DailyTimeSheets_Card" title="Daily Time Sheet">
                <Expansions parentSize="small">
                  <With>
                    <Table title="Registrations" />
                  </With>
                </Expansions>
              </Card>
            </With>
          </Filter>
        </Mount> -->
        <Mount>
          <Card source="Jobs" view="Timesheets" title="Status" />
        </Mount>
      </Section>
      <Section title="Expense" name="Expense_Section">
        <Mount>
          <Filter source="ExpenseSheets" name="ExpenseSheets_Filter" title="List of Expense Sheets" view="ApproveExpenseSheets">
            <With>
              <Card name="ExpenseSheets_Card" title="Expense Sheet" view="ApproveExpenseSheets">
                <With>
                  <Table title="Lines" view="ApproveExpenseSheets" />
                </With>
              </Card>
            </With>
          </Filter>
        </Mount>
      </Section>
      <Section title="Mileage" name="Mileage_Section">
        <Mount>
          <Filter source="MileageSheets" name="MileageSheets_Filter" title="List of Mileage Sheets" view="ApproveMileageSheets">
            <With>
              <Card name="MileageSheets_Card" title="Mileage Sheet" view="ApproveMileageSheets">
                <With>
                  <Table title="Lines" view="ApproveMileageSheets" />
                </With>
              </Card>
            </With>
          </Filter>
        </Mount>
      </Section>
    </Formation>
  </Workspace>
</MWSL>

