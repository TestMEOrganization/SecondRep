<!-- CSK, 10/10/11: Revision layout updated (39492).-->
<MWSL xmlns="http://www.deltek.com/ns/mwsl" version="0.23">
  <Workspace name="GlobalDimensions" title="Global Dimensions">
    <Filter source="Locations" view="Custom_RoleRestricted_Standard" title="Locations">
      <With>
        <Card view="Custom_RoleRestricted_Full" title="Location">
          <Expansions minimized="true" parentSize="huge">
            <Bind foreignKey="DimCombVersionNumberVar_DimCombVersionHeader">
              <Hidden source="DerivedDimensions">
                <Expansions minimized="true">
                  <With>
                    <Table view="Custom_RoleRestricted_Dimensions" title="Revisions" />
                  </With>
                </Expansions>
              </Hidden>
            </Bind>
          </Expansions>
          <Assistants parentSize="huge">
            <Mount>
              <Card source="LocationStructure" view="Custom_RoleRestricted_Full" title="Structure">
                <Expansions parentSize="small">
                  <With>
                    <Table view="Custom_RoleRestricted_Full" title="Specification" />
                  </With>
                </Expansions>
              </Card>
              <Filter source="DimensionGroupings" view="Dimension('Location')" title="Dimension Groupings">
                <Expansions minimized="true">
                  <With>
                    <Card view="Custom_RoleRestricted_Full" title="Dimension Grouping">
                      <With>
                        <Table view="Custom_RoleRestricted_Full" title="Specification" />
                      </With>
                    </Card>
                  </With>
                </Expansions>
              </Filter>
              <Filter source="ReportingHierarchies" view="Dimension('Location')" title="Reporting Hierarchies">
                <Expansions minimized="true">
                  <With>
                    <Card view="Custom_RoleRestricted_Full" title="Reporting Hierarchy">
                      <Expansions parentSize="medium">
                        <With>
                          <Table view="Custom_RoleRestricted_Full" title="Nodes" />
                        </With>
                      </Expansions>
                    </Card>
                  </With>
                </Expansions>
              </Filter>
            </Mount>
          </Assistants>
        </Card>
      </With>
    </Filter>
