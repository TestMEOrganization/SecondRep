<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Global>
    <Style>
      <Table rowHeightMode="fitToContents" />
      <Integer justify="left" />
    </Style>
    <Define>
  	<Style backgroundColor="green" condition="shortname()='testcore'" />
  	<Style backgroundColor="red" condition="shortname()='macodev'" />
      <!-- This style can be used to left justify a group where you have a mix of types that align differently
					 (strings together with amounts as an example) and where the majority of fields are left aligned already.
					 There is a compact version that could be used in situations where you have information that do not require
					 much space  -->
      <Style name="textAlignment" justify="left" />
      <Style name="compact_textAlignment" justify="left" size="medium">
        <Integer size="small" />
        <Amount size="small" />
        <Date size="small" />
        <Real size="small" />
        <Time size="small" />
        <Boolean size="small" />
      </Style>
      <!-- This style can be used to right justify (and right anchor) a group where you have a mix of types that align differently
					 (strings together with amounts as an example) and where the majority of fields are right aligned already.
					 There is a compact version that could be used in situations where you have information that do not require
					 much space -->
      <Style name="amountAlignment" justify="right" anchor="right">
        <Link anchor="left" justify="left" />
        <Integer justify="right" />
        <!--needed because the global style is specific about integers and we need to override that -->
      </Style>
      <Style name="compact_amountAlignment" justify="right" anchor="right" size="medium">
        <Link anchor="left" justify="left" />
        <Integer justify="right" size="small" />
        <!--needed because the global style is specific about integers and we need to override that -->
        <Amount size="small" />
        <Date size="small" />
        <Real size="small" />
        <Time size="small" />
        <Boolean size="small" />
      </Style>
      <Style name="headerAlignment">
        <String justify="right" />
      </Style>
      <!-- Colors -->
      <Style name="highlightOppositeSign" foregroundColor="rgb=255,0,0" />
      <!-- Traffic light colors. The idea is to use them when you, depending on a condition
						will be showing all three (or at least two) of the colors. -->
      <!-- <Style name="trafficlightGreen" backgroundColor="rgb=215,250,180" foregroundColor="rgb=0,0,0" />
			<Style name="trafficlightYellow" backgroundColor="rgb=250,255,180" foregroundColor="rgb=0,0,0" />
			<Style name="trafficlightRed" backgroundColor="rgb=250,180,160" foregroundColor="rgb=0,0,0" /> -->
      <!-- *** Branded for Rambøll *** -->
      <Style name="trafficlightGreen" backgroundColor="rgb=161,191,54" foregroundColor="rgb=0,0,0" />
      <Style name="trafficlightYellow" backgroundColor="rgb=175,173,163" foregroundColor="rgb=0,0,0" />
      <Style name="trafficlightRed" backgroundColor="rgb=198,52,24" foregroundColor="rgb=0,0,0" />
      <!-- These traffic light styles are for highlighting the text (not the background). -->
      <Style name="trafficlightForegroundGreen" foregroundColor="rgb=161,191,54" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="trafficlightForegroundYellow" foregroundColor="rgb=175,173,163" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="trafficlightForegroundRed" foregroundColor="rgb=198,52,24" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <!-- Colors to be used in DialCharts -->
      <Style name="redDialColor" backgroundColor="rgb=198,52,24" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="greyDialColor" backgroundColor="rgb=175,173,163" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="greenDialColor" backgroundColor="rgb=161,191,54" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <!-- Attention field to be used on fields conditionally when the value is worth your attention.
						Used when there is something that you can and (most likely) should change directly such as 
						entering a value, changing a value or running an action.
						Example 1: incomplete time sheet line, in fields with a missing value.
						Example 2: draft invoice with a difference (so cannot be closed).
						Example 3: job parameter selection where new parameter different from effective parameter.
						Example 4: cash discount or exchange rate difference on highlighted line. -->
      <Style name="attentionField" backgroundColor="rgb=237,246,253" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <!-- This style could be used for all kind of messages/notifications you would like to highlight. 
						Example 1: shared setup message.-->
      <Style name="attentionText" bold="true" foregroundColor="rgb=210,0,0" anchor="both" />
      <Style name="attentionLabel">
        <Label foregroundColor="rgb=210,0,0" anchor="both" />
      </Style>
      <!-- This style should be used for comments that are not as important as attentionText.
						The purpose is purely informational, therefore the color should be less stringent.
						Example 1: Calculation date and time for G/L metrics in the company overview -->
      <Style name="noteText" foregroundColor="gray" />
      <!-- This style could be used for highlighting row[s] in table and filter panes. 
						Example 1: highlight the Origin row which is related to the selected finance entry	
						Example 2: highlight drillable lines in financial statements -->
      <Style name="highlightRow" backgroundColor="rgb=180,238,180" />
      <!-- This style could be used for highlighting row[s] in table and filter panes
	    		 and at the same time mark that something is wrong with the line. 
    			 Example 2: highlight lines in financial statements which allows drilling, but the drill result is not guaranteed -->
      <Style name="highlightIncorrectRow" backgroundColor="rgb=180,238,180" foregroundColor="rgb=255,0,0" />
      <!-- This style is used for marking columns based on indexed fields, in a filter.
					 Searching on fields without index might lead to serious performance penalties on big tables.
					 Example 1: show indexed fields in Financial Analysis workspace on the top filters
					 						on finance, customer, vendor and job entries.  -->
      <Style name="indexedField" backgroundColor="rgb=240,255,240" />
      <!-- This style is used for more warning the user about some inconsistencies due to setup issues 
			  	 Example 1: incorrect drilling in G/L reports (Financial Analysis) -->
      <Style name="warningText" foregroundColor="rgb=255,0,0" />
      <!-- This style is used to bold face and highlight summary lines in table views with tree structure -->
      <Style name="treeStructureSummaryLine" backgroundColor="rgb=220,220,220" bold="true" />
      <!-- Multiline styles -->
      <Style name="MultilineDescription" height="3" />
      <!-- Calendar status styles in time registration and supervisor time approval -->
      <Style name="calendarTimeDue" backgroundColor="rgb=198,52,24" foregroundColor="rgb=255,255,255" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="calendarTimeSubmitted" backgroundColor="rgb=175,173,163" foregroundColor="rgb=255,255,255" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="calendarTimeApproved" backgroundColor="rgb=161,191,54" foregroundColor="rgb=255,255,255" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="calendarTimeRejected" backgroundColor="rgb=121,119,102" foregroundColor="rgb=255,255,255" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <Style name="calendarTimeFullyApproved" backgroundColor="rgb=161,191,54" foregroundColor="rgb=255,255,255" />
      <!-- ******* Ændret for Rambøll branding ****** -->
      <!-- Calendar status styles for use in color explanation grid -->
      <Style name="clearGridLabel">
        <GridLabel backgroundColor="standard" />
      </Style>
      <Style name="calendarTimeDueLegend" ref="clearGridLabel">
        <!-- ******* Ændret for Rambøll branding ****** -->
        <Label backgroundColor="rgb=198,52,24" />
      </Style>
      <Style name="calendarTimeSubmittedLegend" ref="clearGridLabel">
        <!-- ******* Ændret for Rambøll branding ****** -->
        <Label backgroundColor="rgb=175,173,163" />
      </Style>
      <Style name="calendarTimeApprovedLegend" ref="clearGridLabel">
        <!-- ******* Ændret for Rambøll branding ****** -->
        <Label backgroundColor="rgb=161,191,54" />
      </Style>
      <Style name="calendarTimeRejectedLegend" ref="clearGridLabel">
        <!-- ******* Ændret for Rambøll branding ****** -->
        <Label backgroundColor="rgb=121,119,102" />
      </Style>
      <Style name="calendarTimeFullyApprovedLegend" ref="clearGridLabel">
        <!-- ******* Ændret for Rambøll branding ****** -->
        <Label backgroundColor="rgb=161,191,54" />
      </Style>
      <Style name="highlightFieldLTBlue" backgroundColor="rgb=202,229,249" />
      <!-- **** Rambøll - style for interessante felter **** -->
      <!-- <Label backgroundColor="rgb=202,229,249" />
			</Style> -->
      <Style name="highlightFieldGrey" backgroundColor="rgb=228,228,224">
        <!-- **** Rambøll - style for interessante felter **** -->
        <Label backgroundColor="rgb=228,228,224" />
      </Style>
      <!-- Approval hierarchy status styles -->
      <Style name="approvalLineApproved" ref="calendarTimeApproved" />
      <Style name="approvalLineRejected" ref="calendarTimeRejected" />
      <Style name="approvalLineNotActive" />
      <Style name="approvalLineActiveCurrentUser" ref="calendarTimeSubmitted" />
      <Style name="approvalLineActiveOtherUser" ref="calendarTimeDue" />
      <Style name="approvalLineApprovedLegend" ref="calendarTimeApprovedLegend" />
      <Style name="approvalLineRejectedLegend" ref="calendarTimeRejectedLegend" />
      <Style name="approvalLineNotActiveLegend" />
      <Style name="approvalLineActiveCurrentUserLegend" ref="calendarTimeSubmittedLegend" />
      <Style name="approvalLineActiveOtherUserLegend" ref="calendarTimeDueLegend" />
      <!-- Label style -->
      <Style name="label">
        <Label bold="true" />
      </Style>
      <!-- Grid header style
					 Default is to bold face the headers and right align.  -->
      <Style name="grid_header">
        <Label bold="true" anchor="right" justify="right" />
      </Style>
      <!-- Grid style for grid where you want a compact style 
					 Default is to have width smaller for the column widths -->
      <Style name="grid_compact" size="small" />
      <!-- Other styles -->
      <Style name="zeroSuppression" zeroSuppression="true" />
      <Style name="fullWidth" anchor="both" />
      <Style name="justifyRight" justify="right" />
      <!-- Two-column styling for Fields - for using the full width -->
      <Style name="two_columns" size="huge" />
      <Style name="applyBold" bold="true">
        <Integer bold="true" />
        <!--needed because the global style is specific about integers and we need to override that -->
        <String bold="true" />
      </Style>
      <!-- Global used groups and columns -->
      <!-- X1 user update group - Standard layout -->
      <!--			<Group name="UserUpdateGroup" title="User">-->
      <!--				<Field source="CreatedBy"/>-->
      <!--				<Field source="CreatedDate" title="Date"/>-->
      <!--				<Field source="ChangedBy"/>-->
      <!--				<Field source="ChangedDate" title="Date"/>-->
      <!--				<Field source="VersionNumber"/>-->
      <!--			</Group>-->
      <Group name="UserUpdateGroup" title="User">
        <If condition="CreatedBy != ''">
          <Description template="^1 by ^2" arguments="CreatedDate;CreatedBy" title="Created" />
        </If>
        <Else>
          <Description template="" title="Created" />
        </Else>
        <If condition="ChangedBy != ''">
          <Description template="^1 by ^2" arguments="ChangedDate;ChangedBy" title="Changed" />
        </If>
        <Else>
          <Description template="" title="Changed" />
        </Else>
        <Field source="VersionNumber" size="small" />
      </Group>
      <Columns name="UserUpdateColumns">
        <Field source="CreatedBy" />
        <Field source="CreatedDate" />
        <Field source="ChangedBy" />
        <Field source="ChangedDate" />
        <Field source="VersionNumber" />
      </Columns>
      <Columns name="UserChangedColumns">
        <Field source="ChangedBy" />
        <Field source="ChangedDate" />
        <Field source="VersionNumber" />
      </Columns>
      <Function name="isNonBillableActivity" type="boolean" parameters="activityNumber" value="isEnvVarDefined('installation.nonbillableactivities.' + activityNumber)" />
      <!-- Solution configuration -->
      <Function name="isSolutionPropertyEnabled" parameters="PropertyName" type="boolean">
        <Value><![CDATA[isEnvVarDefined('solutionconfiguration.' + PropertyName) 
				and booleanEnvVar('solutionconfiguration.' + PropertyName)]]></Value>
      </Function>
      <!-- Analytix -->
      <!-- LoginAnalytix should be the same as in maconomy.security.config -->
      <!-- analytix.server.host, analytix.server.port, analytix.server.protocol
			 and analytix.server.dateFormatMac should be set in CouplingConfiguration.mcsl.xml -->
      <!-- When changing hasAnalytix(), make sure you change in .mwsl as well -->
      <Function name="loginAnalytix" value="'BusinessObjects'" type="string" />
      <Function name="reportFolder" value="'Analytix Reporting::Standard::'" type="string" />
      <Function name="reportFolderInUrl" value="'[Analytix Reporting],[Standard]'" type="string" />
      <Function name="hasAnalytix" value="isEnvVarDefined('analytix.server.host')" type="boolean" />
      <Function name="dateToAnalytixPrompt" parameters="DatePar">
        <Value><![CDATA[if envVar('client.os')='Mac OS X' 
											 then	format(DatePar, envVar('analytix.server.dateFormatMac')) 
                 			 else format(DatePar)]]></Value>
      </Function>
      <Function name="urlOpenReport">
        <Value><![CDATA[envVar('analytix.server.protocol') + '://' + envVar('analytix.server.host') +
			     ':' + envVar('analytix.server.port') +
			     '/OpenDocument/opendoc/openDocument.jsp']]></Value>
      </Function>
      <Function name="urlInfoView">
        <Value><![CDATA[envVar('analytix.server.protocol') + '://' + envVar('analytix.server.host') +
			     ':' + envVar('analytix.server.port') +
			     '/InfoViewApp']]></Value>
      </Function>
      <!-- PeoplePlanner -->
      <Function name="isMyPlanEnabled" value="isEnvVarDefined('peopleplanner.server.host')" type="boolean" />
      <Function name="urlOpenMyPlan">
        <Value><![CDATA[envVar('peopleplanner.server.protocol') + '://' + envVar('peopleplanner.server.host') +
			     ':' + envVar('peopleplanner.server.port') +
			     '/MyPlan/']]></Value>
      </Function>
      <!-- Maconomy Webserver definition - For integrated browser pages - Definition located in couplingconfiguration -->
      <Function name="isMacWebserverEnabled" value="isEnvVarDefined('macwebserver.server.host')" type="boolean" />
      <Function name="urlOpenMacPortal">
        <Value><![CDATA[envVar('macwebserver.server.protocol') + '://' + envVar('macwebserver.server.host') +
			     ':' + envVar('macwebserver.server.port') 
			     + envVar('macwebserver.server.portal')]]></Value>
      </Function>
      <Function name="urlOpenMacMScript">
        <Value><![CDATA[envVar('macwebserver.server.protocol') + '://' + envVar('macwebserver.server.host') +
			     ':' + envVar('macwebserver.server.port') 
			     + envVar('macwebserver.server.mscript')]]></Value>
      </Function>
      <Function name="urlOpenMacWebsite">
        <Value><![CDATA[envVar('macwebserver.server.protocol') + '://' + envVar('macwebserver.server.host') +
			     ':' + envVar('macwebserver.server.port')]]></Value>
      </Function>
      <!-- External reporting server definition - For integrated browser pages - Definition located in couplingconfiguration -->
      <Function name="isReportingServerEnabled" value="isEnvVarDefined('reporting.server.host')" type="boolean" />
      <Function name="urlOpenReportingServer">
        <Value><![CDATA[envVar('reporting.server.protocol') + '://' + envVar('reporting.server.host') +
			     ':' + envVar('reporting.server.port') + envVar('reporting.server.reportViewer')]]></Value>
      </Function>
      <Function name="urlOpenEVReportingServer">
        <Value><![CDATA[envVar('reporting_ev.server.protocol') + '://' + envVar('reporting_ev.server.host') + 
        		':' + envVar('reporting_ev.server.port')]]></Value>
      </Function>
      <!-- TEM Servers - DK/Fi -->
      <Function name="urlOpenTEM_DKServer">
        <Value><![CDATA[envVar('tem.server.protocol') + '://' + envVar('tem.server.host_dk') +
			     ':' + envVar('tem.server.port_dk')]]></Value>
      </Function>
      <Function name="urlOpenTEM_FIServer">
        <Value><![CDATA[envVar('tem.server.protocol') + '://' + envVar('tem.server.host_fi') +
			     ':' + envVar('tem.server.port_fi')]]></Value>
      </Function>
      <!-- VIM Servers -->
      <Function name="urlOpenVIMServer">
        <Value><![CDATA[envVar('vim.server.protocol') + '://' + envVar('vim.server.host') +
			     envVar('vim.server.login')]]></Value>
      </Function>
          
      <Function name="HasPMReadOnly" value="hasRole('Projects - Project Manager read only') and not hasRole('Projects - Project Manager')" type="boolean" />
                       
    </Define>
    <Actions>
      <Action source="ExportDataSet" appearance="icon" icon="Excel" tooltip="Export to Excel">
        <Argument parameter="ExportAllRows" value="true" />
        <Argument parameter="ExportFormat" valueString="application/vnd.ms-excel" />
        <Argument parameter="IncludedFields" value="visibleFields()" />
        <Argument parameter="FieldTitles" value="visibleFieldTitles()" />
      </Action>
    </Actions>
  </Global>
</MDML>

