            <Field source="CostPriceForReallocationVar" title="Cost for Reallocation" />
            <Field source="QuantityAForReallocationVar" title="Quantity for Reallocation" />
            <Field source="QuantityBForReallocationVar" title="Quantity 2 for Reallocation" />
          </Group>
          <Group ref="DimensionGroup" />
        </Column>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" >
        <Exclude>
            <Action source="ApproveReentry" />
          </Exclude>
            <Action source="ApproveReallocation" />
        </Actions>
        <!--         <Exclude> -->
        <!--         <Action name="ApproveReentry"/> -->
        <!--         </Exclude> -->
        <!--         <Action title="(MDK) Approve Reallocation"  name="MDK_ApproveReallocation"/> -->
        <!--         </Actions> -->
        <Columns>
          <!--         searchOption="parent.jobnumber"  -->
          <If condition="hasRole('ReallocateAll')">
            <Field source="JobNumber" title="Job No.!" searchView="ReallocationUnlimited" searchOption="MineOnly" suggestions="onDemand" />
          </If>
          <Else>
            <Field source="JobNumber" title="Job No.!" searchView="Reallocation" searchOption="MineOnly" suggestions="onDemand" />
          </Else>
          <Field source="TaskName" title="Task" />
          <Field source="ActivityNumber" title="Act. No." />
          <Field source="ItemNumber" title="Item No." />
          <Field source="EmployeeNumber" title="Empl." />
          <Field source="Text" title="Description{Text}" />
          <Field source="NumberOriginalEntry" title="Quantity{Number}" />
          <Field source="CostPriceTotalOriginalEntry" title="Total Cost" />
          <Field source="OriginalBillingPriceTotalVar" title="Billing Price Total" />
