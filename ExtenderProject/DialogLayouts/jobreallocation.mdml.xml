          </Group>
          <Group title="G/L Accrual">
            <Field source="AccrualType" />
            <Field source="FromAccrualPeriod" />
            <Field source="ToAccrualPeriod" />
          </Group>
          <Group title="Company">
            <Reference foreignKey="CompanyNumber_CompanyInformation" keySource="CompanyNumber" />
            <Field source="BaseCurrencyVar" title="Base Currency" />
          </Group>
          <Group title="Executing Company">
            <Reference foreignKey="ExecutingCompanyNumber_CompanyInformation" keySource="ExecutingCompanyNumber" />
          </Group>
        </Column>
        <Column>
          <Group title="Entry{Posting}" style="amountAlignment">
            <Field source="NumberRegistered" />
            <Field source="CostPrice" />
            <Field source="CostPriceReg" />
            <Field source="BillingPriceRegCurrency" />
            <Field source="Currency" />
            <Field source="MarkUpPercentage" title="Gross Margin %" />
            <Field source="OvertimeType" />
            <Field source="FromSubscriptionPeriod" />
            <Field source="ToSubscriptionPeriod" />
            <Field source="TransactionNumber" />
            <Field source="TransactionType" title="Trans. Type" />
            <Field source="EntryDate" title="Entry Date" />
            <Field source="InvoiceNumber" />
            <Field source="Closed" />
            <Field source="NumberInvoiced" />
            <Field source="BillingPriceInvoicedCurrency" />
            <Field source="NumberUpDown" title="Quantity, Up/Down" />
            <Field source="BillingPriceUpDownCurrency" title="Billing Price, Up/Down" />
            <Field source="NumberTransferredTo" />
            <Field source="CostPriceTransferredTo" />
            <Field source="BillingPriceTransfToCurrency" />
            <Field source="QuantityA" />
            <Field source="QuantityB" />
            <Field source="StdBillingPriceTotalBase" title="Standard Billing Price, Base" />
            <Field source="StdUpDownBase" />
          </Group>
        </Column>
        <Column>
          <Group title="Reallocation" style="amountAlignment">
            <Field source="NumberReallocated" title="Amount Reallocated{Previous}" />
            <Field source="CostPriceReallocatedVar" title="Cost Reallocated{Previous}" />
            <Field source="QuantityAReallocated" title="Quantity Reallocated To Date" />
