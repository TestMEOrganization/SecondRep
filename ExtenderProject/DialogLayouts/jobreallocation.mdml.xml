<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobReallocation">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="Job No. ^1, Task '^2', Empl. No. '^3', ^4, ^5" arguments="JobNumber;TaskName;EmployeeNumber;EntryDate;Text" />
        </Compact>
        <Columns>
          <Field source="TransactionNumber" />
          <Field source="EntryDate" />
          <Field source="TypeOfEntry" />
          <Field source="Origin" />
          <Field source="JobNumber" />
          <Field source="TaskName" />
          <Field source="ActivityNumber" />
          <Field source="EmployeeNumber" />
          <Field source="Text" />
          <Field source="CostPriceReg" />
          <Field source="BillingPriceRegCurrency" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Full">
        <Actions all="true">
          <Exclude>
            <Action source="ApproveReentry" />
            <!-- <Action source="ApproveReallocation" /> -->
          </Exclude>
          <Action source="ApproveReallocation" />
          <!-- <Action title="Approve Reallocation" source="MDK_ApproveReallocation" /> -->
        </Actions>
        <Column>
          <Group title="Entry Information{3}">
            <Reference foreignKey="JobNumber_JobHeader" descriptionSource="CardJobName" />
            <Field source="TransactionNumber" />
            <Field source="TypeOfEntry" />
            <Field source="Origin" />
            <Field source="ActivityNumber" />
            <Field source="TaskName" />
            <Field source="Text" />
            <Reference foreignKey="EmployeeNumber_Employee" descriptionSource="CardEmployeeName" />
            <Reference foreignKey="SupplierNumber_Vendor" />
            <Field source="VendorInvoiceNumber" title="Vendor Invoice" />
            <Field source="ItemNumber" />
            <Field source="CardItemText" title="Description{Item}" />
            <Field source="Remark" />
            <Field source="DailyDescription" />
            <Field source="ReallocatedFromJobNumber" />
            <Field source="ReallocatedFromTaskName" />
          </Group>
          <Group title="G/L Accrual">
            <Field source="AccrualType" />
            <Field source="FromAccrualPeriod" />
            <Field source="ToAccrualPeriod" />
          </Group>
          <Group title="Company">
            <Reference foreignKey="CompanyNumber_CompanyInformation" keySource="CompanyNumber" />
            <Field source="BaseCurrencyVar" title="Base Currency" />
          </Group>
          <Group title="Executing Company">
            <Reference foreignKey="ExecutingCompanyNumber_CompanyInformation" keySource="ExecutingCompanyNumber" />
          </Group>
        </Column>
        <Column>
          <Group title="Entry{Posting}" style="amountAlignment">
            <Field source="NumberRegistered" />
            <Field source="CostPrice" />
            <Field source="CostPriceReg" />
            <Field source="BillingPriceRegCurrency" />
            <Field source="Currency" />
            <Field source="MarkUpPercentage" title="Gross Margin %" />
            <Field source="OvertimeType" />
            <Field source="FromSubscriptionPeriod" />
            <Field source="ToSubscriptionPeriod" />
            <Field source="TransactionNumber" />
            <Field source="TransactionType" title="Trans. Type" />
            <Field source="EntryDate" title="Entry Date" />
            <Field source="InvoiceNumber" />
            <Field source="Closed" />
            <Field source="NumberInvoiced" />
            <Field source="BillingPriceInvoicedCurrency" />
            <Field source="NumberUpDown" title="Quantity, Up/Down" />
            <Field source="BillingPriceUpDownCurrency" title="Billing Price, Up/Down" />
            <Field source="NumberTransferredTo" />
            <Field source="CostPriceTransferredTo" />
            <Field source="BillingPriceTransfToCurrency" />
            <Field source="QuantityA" />
            <Field source="QuantityB" />
            <Field source="StdBillingPriceTotalBase" title="Standard Billing Price, Base" />
            <Field source="StdUpDownBase" />
          </Group>
        </Column>
        <Column>
          <Group title="Reallocation" style="amountAlignment">
            <Field source="NumberReallocated" title="Amount Reallocated{Previous}" />
            <Field source="CostPriceReallocatedVar" title="Cost Reallocated{Previous}" />
            <Field source="QuantityAReallocated" title="Quantity Reallocated To Date" />
            <Field source="QuantityBReallocated" title="Quantity 2 Reallocated To Date" />
            <Field source="ReallocationDateVar" title="Reallocation Date" />
            <Field source="NumberForReallocationVar" title="Amount to be Reallocated" />
            <Field source="CostPriceForReallocationVar" title="Cost for Reallocation" />
            <Field source="QuantityAForReallocationVar" title="Quantity for Reallocation" />
            <Field source="QuantityBForReallocationVar" title="Quantity 2 for Reallocation" />
          </Group>
          <Group ref="DimensionGroup" />
        </Column>
      </Form>
      <Form name="Custom">
        <Actions all="true">
          <!-- <Action title="Approve Reallocation" source="MDK_ApproveReallocation" /> -->
        </Actions>
        <Column>
          <Group title="Entry Information{3}">
            <Reference foreignKey="JobNumber_JobHeader" descriptionSource="CardJobName" />
