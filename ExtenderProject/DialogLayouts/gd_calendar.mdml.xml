<!-- JSV, 17/11/10: Calendar group for both daily and weekly time registration (38848).-->
<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Fragment name="GD_Dimensions">
    <Define>
      <Group name="Calendar">
        <Calendar source="DateVar" inputField="false" autoSelection="true" appearance="maximized">
          <Style fontSize="medium">
            <Switch source="TimeSheetStatusVar">
              <Case char="1">
                <!-- Changed for RambÃ¸ll to correspond to their naming convention Due -> Not Submitted -->
                <Tooltip text="Not Submitted" />
                <Style ref="calendarTimeDue" />
              </Case>
              <Case char="2">
                <Tooltip text="Submitted" />
                <Style ref="calendarTimeSubmitted" />
              </Case>
              <Case char="3">
                <Tooltip text="Approved" />
                <Style ref="calendarTimeApproved" />
              </Case>
              <Case char="4">
                <Tooltip text="Line Rejected by Project Manager" />
                <Style ref="calendarTimeRejected" />
              </Case>
              <Case char="5">
                <Tooltip text="Line Rejected by Project Manager" />
                <Style ref="calendarTimeRejected" />
              </Case>
              <Case char="6">
                <Tooltip text="Line Rejected by Project Manager" />
                <Style ref="calendarTimeRejected" />
              </Case>
            </Switch>
          </Style>
        </Calendar>
      </Group>
    </Define>
  </Fragment>
</MDML>

