<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="EmployeeControl">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobGroup" />
          <Field source="NotInvoiceable" />
          <Field source="InternalJob" />
          <Field source="Status" />
          <Field source="ConvertedBy" />
          <Field source="ConversionDate" />
          <Field source="ProjectManagerNumber" />
          <Field source="SalesPersonNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Wizard name="CopyEmployeeListWizard" title="Copy Employee List">
        <Page form="CopyEmployeeListWizardPage1" title="Copy Employee List" description="Choose a job to copy from" />
      </Wizard>
      <Wizard name="AddEmployeesWizard" title="Add Employees">
        <Page form="AddEmployeesWizardPage1" title="Add Employees" description="Choose a primary employee category" />
      </Wizard>
      <Form name="Full">
        <Actions all="true">
          <Action source="CopyEmployeeList" wizard="CopyEmployeeListWizard" />
          <Action source="AddEmployees" wizard="AddEmployeesWizard" />
        </Actions>
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="JobNumber_JobHeader" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
            </Group>
            <Group title="Job Description">
              <Field source="Description1" appearance="maximized" />
            </Group>
            <Group title="Customer">
              <Reference foreignKey="CustomerNumber_Customer" descriptionSource="Name1" />
            </Group>
          </Column>
          <Column>
            <Group title="Employee Control">
              <Field source="EmployeeControlActivatedVar" title="Enabled" />
            </Group>
            <Group title="Hours">
              <Field source="TotalBudgetedHoursVar" title="Budgeted" />
              <Field source="TotalExpectedHoursVar" title="Assigned{Expected Hours}" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="CopyEmployeeListWizardPage1">
        <Actions all="true" />
        <Group>
          <Reference foreignKey="Card_CopiesFromJobNumberVar_JobHeader" keyMandatory="true" />
        </Group>
      </Form>
      <Form name="AddEmployeesWizardPage1">
        <Actions all="true" />
        <Group>
          <Field source="AddPrimaryEmployeeCategoryNumberVar" mandatory="true" />
        </Group>
      </Form>
      <Form name="Standard">
        <Actions all="true" collapseOrder="other default">
          <Action source="CopyEmployeeList" wizard="CopyEmployeeListWizard" />
          <Action source="AddEmployees" wizard="AddEmployeesWizard" />
          <Order>
            <Group name="default" grouped="never">
              <Action ref="ActivateEmployeeControl" />
              <Action ref="DeactivateEmployeeControl" />
              <Action ref="CopyEmployeeList" />
              <Action ref="AddEmployees" />
            </Group>
            <Group name="other" title="Other Actions" grouped="always">
              <Action ref="CopyEmployeesFromTimeBudgetResources" />
              <Action ref="RemoveEmployees" />
            </Group>
          </Order>
        </Actions>
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="ProjectManagerNumber_Employee" title="Project Manager" />
              <Field source="EmployeeControlActivatedVar" title="Employee Control Enabled" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Custom">
        <Actions all="not HasPMReadOnly()" >
        <Exclude>
        	<Action source="AddEmployees" />
        </Exclude>
          <Action source="CopyEmployeeList" wizard="CopyEmployeeListWizard" disabled="HasPMReadOnly()" />
              <Action source="CopyEmployeesFromTimeBudgetResources" disabled="HasPMReadOnly()" />
              <Action source="RemoveEmployees" title="Remove All Employees" disabled="HasPMReadOnly()" />
          <Order>
            <Group name="default" grouped="never">
              <Action ref="ActivateEmployeeControl" />
              <Action ref="DeactivateEmployeeControl" />
              <Action ref="CopyEmployeeList" />
              <Action ref="RemoveEmployees" />
              <Action ref="CopyEmployeesFromTimeBudgetResources" />
            </Group>
          </Order>
        </Actions>
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="ProjectManagerNumber_Employee" title="Project Manager" />
              <Field source="EmployeeControlActivatedVar" title="Employee Control Enabled" />
            </Group>
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="EmployeeNumber" />
          <Field source="JobEmployeeNameVar" />
          <Field source="ExpectedHours" />
          <Field source="DescriptionForEmployee" />
          <Field source="CompletedByEmployee" />
          <Field source="StandardTaskName" />
          <Field source="TaskDescriptionVar" />
          <Field source="ValidFromDate" />
          <Field source="ValidToDate" />
          <Field source="Approved" />
          <Field source="ApprovedBy" />
          <Field source="ApprovalDate" />
          <Field source="PrimaryEmployeeCategoryNumberVar" title="Primary Empl Cat. No." />
          <Field source="PrimaryEmployeeCategoryNameVar" title="Primary Empl. Cat. Name" />
          <Field source="CompanyNumberVar" title="Company No." />
          <Field source="CompanyNameVar" title="Company Name" />
          <Columns ref="DimensionVarDescriptionColumns" />
        </Columns>
      </Table>
      <Table name="Standard">
        <Actions all="true">
          <Exclude>
            <Insert />
          </Exclude>
          <Create title="Add Employee to Job" appearance="all">
            <Add title="Add Employee to Job" appearance="all" />
          </Create>
        </Actions>
        <Columns>
          <Field source="EmployeeNumber" />
          <Field source="JobEmployeeNameVar" />
          <Field source="ValidFromDate" />
          <Field source="ValidToDate" />
          <Field source="ExpectedHours" />
          <Field source="TaskDescriptionVar" />
          <Field source="NotifyEmployee" />
        </Columns>
      </Table>
      <Table name="Custom">
        <Actions all="true">
          <Exclude>
            <Insert />
          </Exclude>
          <Create title="Add Employee to Job" appearance="all">
            <Add title="Add Employee to Job" appearance="all" />
          </Create>
        </Actions>
        <Columns>
          <Field source="EmployeeNumber" />
          <Field source="JobEmployeeNameVar" />
          <Field source="ValidFromDate" />
          <Field source="ValidToDate" />
          <Field source="ExpectedHours" />
          <Field source="NotifyEmployee" />
        </Columns>
      </Table>
    </LowerPane>
  </Layout>
</MDML>

