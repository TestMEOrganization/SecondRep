                  <PeriodYearInterval firstPeriodSource="FromPeriodMonthVar" firstYearSource="FromPeriodYearVar" lastPeriodSource="ToPeriodMonthVar" lastYearSource="ToPeriodYearVar" title="Fiscal Year" />
