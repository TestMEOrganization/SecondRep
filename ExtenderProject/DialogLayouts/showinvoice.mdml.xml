                <Field source="ChargeVAT" title="Total Tax on Charges" />
                <Field source="ChargeVAT1Var" title="Tax on Charges 1" />
                <Field source="ChargeVAT2Var" title="Tax on Charges 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="ChargeVAT3Var" title="Tax on Charges 3" />
                </If>
              </If>
              <Else>
                <Field source="ChargeVAT" title="Tax on Charges" />
              </Else>
              <Field source="OnAccountItemSumCurrency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATCurrency" title="Total Tax on Account" />
                <Field source="OnAccountVAT1CurrencyVar" title="Tax on Account 1" />
                <Field source="OnAccountVAT2CurrencyVar" title="Tax on Account 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3CurrencyVar" title="Tax on Account 3" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATCurrency" title="Tax on Account" />
              </Else>
              <Field source="TotalCurrency" title="Balance Due" />
              <Field source="StandardCurrency" />
              <Field source="TotalStandard" />
            </Group>
            <Group title="Customer Information{1}">
              <Field source="DepartmentNumber" />
              <Field source="InventoryNumber" />
              <Field source="Reference" />
              <Field source="VATNumber" />
            </Group>
            <Group title="Sales Orders{General}">
              <Field source="MainOrderNumber" />
            </Group>
          </Column>
          <Column>
            <Group title="Ship to Address">
              <Reference foreignKey="CustomerNumber_Customer" />
              <Field source="Name2" appearance="maximized" />
              <Field source="Name3" appearance="maximized" />
              <ZipCity citySource="PostalDistrict" zipSource="ZipCode" appearance="maximized" />
              <Field source="Name4" appearance="maximized" />
              <Field source="Name5" appearance="maximized" />
              <Field source="Attention" />
              <Field source="Country" title="Country{Show_Invoice}" />
            </Group>
