<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="ShowInvoice">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="Invoice No. ^1, ^2" arguments="InvoiceNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="InvoiceNumber" />
          <Field source="OrderNumber" />
          <Field source="LastDeliveryNoteNumber" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="InvoiceName" />
          <Field source="MainOrderNumber" />
          <Field source="CustomerNumber" />
          <Field source="Name1" />
        </Columns>
      </Filter>
      <Filter name="SalesOrders">
        <ControlBar>
          <Selection default="MyInvoices">
            <Option title="My Invoices" name="MyInvoices" restriction="SalesPersonNumber = userEmployeeNumber()" />
            <Option title="Invoices for My Jobs" name="MyJobsInvoices" restriction="ProjectManagerNumber = userEmployeeNumber()" />
            <Option title="All Invoices" name="AllSalesOrders" />
          </Selection>
        </ControlBar>
        <Compact>
          <Description template="Invoice No. ^1, ^2" arguments="InvoiceNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="InvoiceNumber" />
          <Field source="Name1" title="Customer" />
          <Field source="OrderNumber" />
          <Field source="SalesPerson" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="ProjectManagerName" />
          <Field source="LastDeliveryNoteNumber" />
        </Columns>
      </Filter>
      <Filter name="SubscriptionOrders">
        <ControlBar>
          <Selection default="SubscriptionInvoices">
            <Option name="SubscriptionInvoices" title="Subscription Invoices" restriction="SubscriptionOrderNumber != 0" />
          </Selection>
        </ControlBar>
        <Compact>
          <Description template="Invoice No. ^1, ^2, Subscr. Order. No. ^3" arguments="InvoiceNumber;Name1;SubscriptionOrderNumber" />
        </Compact>
        <Columns>
          <Field source="SubscriptionOrderNumber" />
          <Field source="InvoiceNumber" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer" />
          <Field source="PaymentCustomer" />
          <Field source="OrderNumber" />
          <Field source="InvoiceDate" />
          <Field source="SalesPerson" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="ProjectManagerName" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Billing Address">
              <Reference foreignKey="PaymentCustomer_Customer" />
              <Field source="PayerName2" appearance="maximized" />
              <Field source="PayerName3" appearance="maximized" />
              <ZipCity citySource="PayerPostalDistrict" zipSource="PayerZipCode" appearance="maximized" />
              <Field source="PayerName4" appearance="maximized" />
              <Field source="PayerName5" appearance="maximized" />
              <Field source="PayerAttention" title="Attn." />
              <Field source="PayerCountry" title="Ctry{Show_Invoice}" />
            </Group>
            <Group title="Settling Company">
              <Reference foreignKey="SettlingCompanyVar_CompanyInformation" />
            </Group>
            <Group title="Price">
              <Field source="Currency" />
              <Field source="ExchangeRateDate" />
              <Field source="ItemSumCurrency" />
              <Field source="InvoiceDiscountPercentage" />
              <Field source="InvoiceDiscountCurrency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATCurrency" title="Total Tax on Sales" />
                <Field source="VAT1CurrencyVar" title="Tax on Sales 1" />
                <Field source="VAT2CurrencyVar" title="Tax on Sales 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VAT3CurrencyVar" title="Tax on Sales 3" />
                </If>
              </If>
              <Else>
                <Field source="VATCurrency" title="Tax on Sales" />
              </Else>
              <Field source="Charge1Currency" titleValue="Charge1Text" />
              <Field source="Charge2Currency" titleValue="Charge2Text" />
              <Field source="Charge3Currency" titleValue="Charge3Text" />
              <Field source="Charge4Currency" titleValue="Charge4Text" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="ChargeVAT" title="Total Tax on Charges" />
                <Field source="ChargeVAT1Var" title="Tax on Charges 1" />
                <Field source="ChargeVAT2Var" title="Tax on Charges 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="ChargeVAT3Var" title="Tax on Charges 3" />
                </If>
              </If>
              <Else>
                <Field source="ChargeVAT" title="Tax on Charges" />
              </Else>
              <Field source="OnAccountItemSumCurrency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATCurrency" title="Total Tax on Account" />
                <Field source="OnAccountVAT1CurrencyVar" title="Tax on Account 1" />
                <Field source="OnAccountVAT2CurrencyVar" title="Tax on Account 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3CurrencyVar" title="Tax on Account 3" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATCurrency" title="Tax on Account" />
              </Else>
              <Field source="TotalCurrency" title="Balance Due" />
              <Field source="StandardCurrency" />
              <Field source="TotalStandard" />
            </Group>
          </Column>
          <Column>
            <Group title="Ship to Address">
              <Reference foreignKey="CustomerNumber_Customer" />
              <Field source="Name2" appearance="maximized" />
              <Field source="Name3" appearance="maximized" />
              <ZipCity citySource="PostalDistrict" zipSource="ZipCode" appearance="maximized" />
              <Field source="Name4" appearance="maximized" />
              <Field source="Name5" appearance="maximized" />
              <Field source="Attention" />
              <Field source="Country" title="Ctry{Show_Invoice}" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
            </Group>
            <Group title="Invoice{noun}">
              <Field source="InvoiceNumber" />
              <Boolean source="Printed" />
              <Field source="OrderNumber" />
              <Field source="LastDeliveryNoteNumber" />
              <Field source="VersionNumber" />
              <Field source="InvoiceDate" />
              <Field source="DueDate" />
              <Field source="PrintedBy" />
              <Field source="PrintingDate" />
            </Group>
            <Group ref="DimensionGroup" />
            <Group title="Customer Information{1}">
              <Field source="DepartmentNumber" />
              <Field source="InventoryNumber" />
              <Field source="Reference" />
              <Field source="VATNumber" />
            </Group>
            <Group title="Sales Orders{General}">
              <Field source="MainOrderNumber" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Group title="Remarks{pl}">
            <Field source="InternalRemark1" appearance="maximized" />
            <Field source="InternalRemark2" appearance="maximized" />
            <Field source="InternalRemark3" appearance="maximized" />
          </Group>
        </Row>
        <Row>
          <Column>
            <Group title="Extra Fields">
              <Field source="CustomerPopup1" />
              <Field source="CustomerPopup2" />
              <Field source="CustomerPopup3" />
              <Field source="CustomerPopup4" />
              <Field source="CustomerPopup5" />
            </Group>
          </Column>
          <Column>
            <Group title="Credit Card">
              <Field source="CreditCardNumber" />
              <Field source="CreditCardExpiryDate" />
              <Field source="AuthorizationAmount" />
              <Field source="AuthorizationCode" />
              <Field source="DateOfAuthorization" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Standard">
        <Actions all="true">
          <If condition="ManualInvoice = false and InvoiceOnAccount = false and BasedOnInvoiceNumber = 0 and not hasWorkspace('SubscriptionHome')">
            <Action source="primary-&gt;InvoiceToCreditOrder.CreateCreditOrder" title="Generate Credit Order" name="GenerateCreditOrder" />
          </If>
        <Order>
            <Group name="default" title="Actions" grouped="always">
              <Action ref="GenerateCreditOrder" />
            </Group>
        </Order>
        </Actions>
        <Row>
          <Column>
            <Group title="Price">
              <UnitField source="ItemSumCurrency" unitSource="Currency" />
              <UnitField source="InvoiceDiscountPercentage" unitTitle="%" />
              <UnitField source="InvoiceDiscountCurrency" unitSource="Currency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <UnitField source="VATCurrency" title="Total Tax on Sales" unitSource="Currency" />
                <UnitField source="VAT1CurrencyVar" title="Tax on Sales 1" unitSource="Currency" />
                <UnitField source="VAT2CurrencyVar" title="Tax on Sales 2" unitSource="Currency" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <UnitField source="VAT3CurrencyVar" title="Tax on Sales 3" unitSource="Currency" />
                </If>
              </If>
              <Else>
                <UnitField source="VATCurrency" title="Tax on Sales" unitSource="Currency" />
              </Else>
              <If condition="Charge1Text != ''">
                <UnitField source="Charge1Currency" titleValue="Charge1Text" unitSource="Currency" />
              </If>
              <If condition="Charge2Text != ''">
                <UnitField source="Charge2Currency" titleValue="Charge2Text" unitSource="Currency" />
              </If>
              <If condition="Charge3Text != ''">
                <UnitField source="Charge3Currency" titleValue="Charge3Text" unitSource="Currency" />
              </If>
              <If condition="Charge4Text != ''">
                <UnitField source="Charge4Currency" titleValue="Charge4Text" unitSource="Currency" />
              </If>
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <UnitField source="ChargeVAT" title="Total Tax on Charges" unitSource="Currency" />
                <UnitField source="ChargeVAT1Var" title="Tax on Charges 1" unitSource="Currency" />
                <UnitField source="ChargeVAT2Var" title="Tax on Charges 2" unitSource="Currency" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <UnitField source="ChargeVAT3Var" title="Tax on Charges 3" unitSource="Currency" />
                </If>
              </If>
              <Else>
                <UnitField source="ChargeVAT" title="Tax on Charges" unitSource="Currency" />
              </Else>
              <UnitField source="OnAccountItemSumCurrency" unitSource="Currency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <UnitField source="OnAccountVATCurrency" title="Total Tax on Account" unitSource="Currency" />
                <UnitField source="OnAccountVAT1CurrencyVar" title="Tax on Account 1" unitSource="Currency" />
                <UnitField source="OnAccountVAT2CurrencyVar" title="Tax on Account 2" unitSource="Currency" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <UnitField source="OnAccountVAT3CurrencyVar" title="Tax on Account 3" unitSource="Currency" />
                </If>
              </If>
              <Else>
                <UnitField source="OnAccountVATCurrency" title="Tax on Account" unitSource="Currency" />
              </Else>
              <UnitField source="TotalCurrency" title="Balance Due" unitSource="Currency" />
              <If condition="Currency != StandardCurrency">
                <UnitField source="TotalStandard" unitSource="StandardCurrency" />
              </If>
              <Field source="ExchangeRateDate" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" title="Company" />
            </Group>
            <Group title="Settling Company">
              <Reference foreignKey="SettlingCompanyVar_CompanyInformation" title="Company" />
            </Group>
          </Column>
          <Column>
            <Group title="Billing Address">
              <Reference foreignKey="PaymentCustomer_Customer" title="Bill to Customer" />
              <Field source="PayerName2" title="Address" shadowTitle="Street" />
              <Field source="PayerName3" title="" />
              <ZipCity citySource="PayerPostalDistrict" zipSource="PayerZipCode" title="" cityShadowTitle="Postal District" zipShadowTitle="Zip Code" />
              <Field source="PayerName4" title="" />
              <Field source="PayerName5" title="" />
              <Field source="PayerAttention" title="Attn." />
              <Field source="PayerCountry" title="Ctry{Show_Invoice}" />
            </Group>
            <Group title="Ship to Address">
              <Reference foreignKey="CustomerNumber_Customer" title="Ship to Customer" />
              <Field source="Name2" title="Address" shadowTitle="Street" />
              <Field source="Name3" title="" />
              <ZipCity citySource="PostalDistrict" zipSource="ZipCode" title="" cityShadowTitle="Postal District" zipShadowTitle="Zip Code" />
              <Field source="Name4" title="" />
              <Field source="Name5" title="" />
              <Field source="Attention" />
              <Field source="Country" title="Ctry{Show_Invoice}" />
            </Group>
            <Group title="Customer Information{1}">
              <Field source="DepartmentNumber" />
              <Field source="InventoryNumber" />
              <Field source="Reference" />
              <Field source="VATNumber" />
            </Group>
          </Column>
          <Column>
            <Group title="Invoice{noun}">
              <Field source="InvoiceNumber" />
              <Boolean source="Printed" />
              <If condition="SubscriptionOrderNumber = 0 or booleanSysInfo('ShowSubscriptionSalesOrders')">
                <Link workspace="SalesOrders" template="^1" arguments="OrderNumber" title="Order No." icon="WorkspaceLink">
                  <Target pane="SalesOrders_Filter">
                    <Restriction template="Selected Sales Order" expression="OrderNumber=target.OrderNumber" />
                    <Focus>
                      <Match field="OrderNumber" value="OrderNumber" />
                    </Focus>
                  </Target>
                </Link>
              </If>
              <Field source="LastDeliveryNoteNumber" />
              <Field source="VersionNumber" />
              <Field source="InvoiceDate" />
              <Field source="DueDate" />
              <Field source="PrintedBy" />
              <Field source="PrintingDate" />
              <If condition="BasedOnInvoiceNumber != 0 and (SubscriptionOrderNumber = 0 or booleanSysInfo('ShowSubscriptionSalesOrders'))">
                <Link workspace="SalesOrders" template="^1" arguments="BasedOnInvoiceNumber" title="Converted to Credit Order" style="attentionLabel" icon="WorkspaceLink">
                  <Target pane="CreditOrders">
                    <Restriction template="Selected Credit Order" expression="BasedOnInvoiceNumber=target.OrderNumber" />
                    <Focus>
                      <Match field="OrderNumber" value="BasedOnInvoiceNumber" />
                    </Focus>
                  </Target>
                </Link>
              </If>
            </Group>
            <Group ref="DimensionGroup" />
          </Column>
        </Row>
      </Form>
