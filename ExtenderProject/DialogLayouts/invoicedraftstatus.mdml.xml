<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="InvoiceDraftStatus">
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Selection Criteria">
              <Boolean source="ShowApproved" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
              <Interval firstSource="FromJobNumber" lastSource="ToJobNumber" title="Job No." />
              <Interval firstSource="FromCompanyNumber" lastSource="ToCompanyNumber" title="Company No." />
              <Interval firstSource="FromSettlingCompanyNumber" lastSource="ToSettlingCompanyNumber" title="Settling Company No." />
              <Interval firstSource="FromCustomerNumber" lastSource="ToCustomerNumber" title="Customer No." />
              <Field source="CustomerGroup" appearance="interval" />
              <Interval firstSource="FromPaymentCustomerNumber" lastSource="ToPaymentCustomerNumber" title="Payment Customer" />
              <Field source="DepartmentNumber" appearance="interval" />
              <Field source="SalesPersonNumber" appearance="interval" />
              <Field source="EmployeeNumber1" title="Employee 1 No." appearance="interval" />
              <Field source="EmployeeNumber2" title="Employee 2 No." appearance="interval" />
              <Field source="EmployeeNumber3" title="Employee 3 No." appearance="interval" />
              <Field source="EmployeeNumber4" title="Employee 4 No." appearance="interval" />
              <Field source="EmployeeNumber5" title="Employee 5 No." appearance="interval" />
              <Field source="EmployeeNumber6" title="Employee 6 No." appearance="interval" />
              <Field source="EmployeeNumber7" title="Employee 7 No." appearance="interval" />
              <Field source="EmployeeNumber8" title="Employee 8 No." appearance="interval" />
              <Field source="EmployeeNumber9" title="Employee 9 No." appearance="interval" />
              <Field source="EmployeeNumber10" title="Employee 10 No." appearance="interval" />
            </Group>
            <Group ref="DimensionsTargetGroupIntervals" />
          </Column>
          <Column>
            <Group title="Totals">
              <Grid style="grid_compact">
                <Define>
                  <Style name="AlertWriteUp" ref="trafficlightGreen" condition="fieldValue() gt 0" />
                  <Style name="AlertWriteDown" ref="trafficlightRed" condition="fieldValue() lt 0" />
                  <Style name="amounts" size="small" />
                </Define>
                <Header cells="|Invoice|;|Up/Down|" title="" style="grid_header" />
                <Scope>
                  <Style ref=";AlertWriteUp,AlertWriteDown" />
                  <Row cells="TotalAmountCurrentEnterpriseVar+EnterpriseCurrencyVar;TotalUpDownCurrentEnterpriseVar+EnterpriseCurrencyVar" title="Current" />
                  <Row cells="TotalAmountPriorEnterpriseVar+EnterpriseCurrencyVar;TotalUpDownPriorEnterpriseVar+EnterpriseCurrencyVar" title="Prior" />
                  <Row cells="TotalInvoiceAmountEnterpriseVar+EnterpriseCurrencyVar;TotalUpDownEnterpriseVar+EnterpriseCurrencyVar" title="Total" />
                </Scope>
              </Grid>
              <EmptyLabel />
              <Field source="EnterpriseCurrencyVar" title="Enterprise Currency" />
              <Field source="NumberOfInvoicesToApproveVar" title="Invoice Drafts to Approve" />
              <Field source="NumberOfDraftsToPrintVar" title="Invoice Drafts to Print" />
            </Group>
          </Column>
        </Row>
      </Form>
