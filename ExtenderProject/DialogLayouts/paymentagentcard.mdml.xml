<!-- CSK, 11/10/11: Possible to edit account in DimensionOrigin (39547).-->
<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="PaymentAgentCard">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1" arguments="PaymentAgentName" />
        </Compact>
        <Columns>
          <Field source="PaymentAgentName" />
          <Field source="Address1" />
          <Field source="Address2" />
          <Field source="Address3" />
          <Field source="Address4" />
          <Field source="YourTelephone" />
          <Field source="YourContactPerson" />
          <Field source="OurTelephone" />
          <Field source="OurContactPerson" />
          <Field source="Currency" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Payment Agent">
              <Field source="PaymentAgentName" title="Name" />
              <Field source="Address1" title="Address" />
              <Field source="Address2" title="" />
              <Field source="Address3" title="" />
              <Field source="Address4" title="" />
              <Field source="YourTelephone" />
              <Field source="YourContactPerson" />
              <Field source="OurTelephone" />
              <Field source="OurContactPerson" />
              <Field source="Currency" />
              <Field source="StandardBankRegistrationNumber" title="Bank Registration No." />
            </Group>
            <Group title="Payment Control">
              <Field source="PaymentFormat" />
            </Group>
            <Group title="Customer Payment Control">
              <Field source="CustomerPaymentFormat" />
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Field source="CustomerPaymentTransactionNumberSeries" />
              <Boolean source="CustomerPaymentConsecutiveTransactionNumber" />
              <Field source="EntryTextCustomerPayment" />
              <Boolean source="CustomerPaymentCalculateEntryDate" />
              <Boolean source="CustomerPaymentTransferRegistrationDate" />
              <Boolean source="CustomerPaymentTransferDimensions" />
              <Boolean source="CustomerPaymentCreateErrorEntries" />
              <Boolean source="SeparateJournalForErrorEntries" />
              <Field source="CustomerPaymentChargeAccount" />
            </Group>
          </Column>
          <Column>
            <Group title="Payment Information">
              <Field source="LastProductionDate" />
              <Field source="LastVendorPaymentNumber" />
              <Field source="NextSystemNumber1" title="Next System No. 1" />
              <Field source="NextSystemNumber2" title="Next System No. 2" />
              <Field source="NextSystemNumber3" title="Next System No. 3" />
            </Group>
            <Group title="Customer Payment Information">
              <Field source="ProductionDateCollection" title="Last Production Date" />
              <Field source="LastImportDate" />
              <Field source="LastCollectionNumber" />
              <Field source="NextSystemNumber1Collection" title="Next System No. 1" />
              <Field source="NextSystemNumber2Collection" title="Next System No. 2" />
              <Field source="NextSystemNumber3Collection" title="Next System No. 3" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
        </Row>
      </Form>
      <Form name="Custom_RoleRestricted_DimensionOrigin" open="false">
        <Actions>
          <Update />
          <Print />
          <PrintThis />
        </Actions>
        <Row>
        <If condition="hasRole('Finance Admin')" >
          <Column style="fullWidth">
            <Group title="Payment Agent">
              <Field source="PaymentAgentName" title="Name" />
              <Field source="Address1" title="Address" />
              <Field source="Address2" title="" />
              <Field source="Address3" title="" />
              <Field source="Address4" title="" />
              <Field source="YourTelephone" />
              <Field source="YourContactPerson" />
              <Field source="OurTelephone" />
              <Field source="OurContactPerson" />
              <Field source="Currency" />
              <Field source="StandardBankRegistrationNumber" title="Bank Registration No." />
            </Group>
            <Group title="Payment Control">
              <Field source="PaymentFormat" />
            </Group>
            <Group title="Customer Payment Control">
              <Field source="CustomerPaymentFormat" />
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Field source="CustomerPaymentTransactionNumberSeries" />
              <Field source="EntryTextCustomerPayment" />
              <Field source="CustomerPaymentChargeAccount" style="highlightRow" open="true" />
              <Field source="CustomerPaymentConsecutiveTransactionNumber" />
              <Field source="CustomerPaymentCalculateEntryDate" />
              <Field source="CustomerPaymentTransferRegistrationDate" />
              <Field source="CustomerPaymentTransferDimensions" />
              <Field source="CustomerPaymentCreateErrorEntries" />
              <Field source="SeparateJournalForErrorEntries" />
            </Group>
          </Column>
          <Column>
            <Group title="Payment Information">
              <Field source="LastProductionDate" />
              <Field source="LastVendorPaymentNumber" />
              <Field source="NextSystemNumber1" title="Next System No. 1" />
              <Field source="NextSystemNumber2" title="Next System No. 2" />
              <Field source="NextSystemNumber3" title="Next System No. 3" />
            </Group>
            <Group title="Customer Payment Information">
              <Field source="ProductionDateCollection" title="Last Production Date" />
              <Field source="LastImportDate" />
              <Field source="LastCollectionNumber" />
              <Field source="NextSystemNumber1Collection" title="Next System No. 1" />
              <Field source="NextSystemNumber2Collection" title="Next System No. 2" />
              <Field source="NextSystemNumber3Collection" title="Next System No. 3" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
          </If>
          <Else>
          <Column style="fullWidth">
            <Group title="Payment Agent">
              <Field source="PaymentAgentName" title="Name" />
              <Field source="Address1" title="Address" />
              <Field source="Address2" title="" />
              <Field source="Address3" title="" />
              <Field source="Address4" title="" />
              <Field source="YourTelephone" />
              <Field source="YourContactPerson" />
              <Field source="OurTelephone" />
              <Field source="OurContactPerson" />
              <Field source="Currency" />
              <Field source="StandardBankRegistrationNumber" title="Bank Registration No." />
            </Group>
            <Group title="Payment Control">
              <Field source="PaymentFormat" />
            </Group>
            <Group title="Customer Payment Control">
              <Field source="CustomerPaymentFormat" />
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Field source="CustomerPaymentTransactionNumberSeries" />
              <Field source="EntryTextCustomerPayment" />
              <Field source="CustomerPaymentChargeAccount" style="highlightRow" open="false" />
              <Field source="CustomerPaymentConsecutiveTransactionNumber" />
              <Field source="CustomerPaymentCalculateEntryDate" />
              <Field source="CustomerPaymentTransferRegistrationDate" />
              <Field source="CustomerPaymentTransferDimensions" />
              <Field source="CustomerPaymentCreateErrorEntries" />
              <Field source="SeparateJournalForErrorEntries" />
            </Group>
          </Column>
          <Column>
            <Group title="Payment Information">
              <Field source="LastProductionDate" />
              <Field source="LastVendorPaymentNumber" />
              <Field source="NextSystemNumber1" title="Next System No. 1" />
              <Field source="NextSystemNumber2" title="Next System No. 2" />
              <Field source="NextSystemNumber3" title="Next System No. 3" />
            </Group>
            <Group title="Customer Payment Information">
              <Field source="ProductionDateCollection" title="Last Production Date" />
              <Field source="LastImportDate" />
              <Field source="LastCollectionNumber" />
              <Field source="NextSystemNumber1Collection" title="Next System No. 1" />
              <Field source="NextSystemNumber2Collection" title="Next System No. 2" />
              <Field source="NextSystemNumber3Collection" title="Next System No. 3" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
          </Else>
        </Row>
      </Form>
      <Form name="DimensionOrigin" open="false">
        <Actions>
          <Update />
          <Print />
          <PrintThis />
        </Actions>
        <Row>
          <Column style="fullWidth">
            <Group title="Payment Agent">
              <Field source="PaymentAgentName" title="Name" />
              <Field source="Address1" title="Address" />
              <Field source="Address2" title="" />
              <Field source="Address3" title="" />
              <Field source="Address4" title="" />
              <Field source="YourTelephone" />
              <Field source="YourContactPerson" />
              <Field source="OurTelephone" />
              <Field source="OurContactPerson" />
              <Field source="Currency" />
              <Field source="StandardBankRegistrationNumber" title="Bank Registration No." />
            </Group>
            <Group title="Payment Control">
              <Field source="PaymentFormat" />
            </Group>
            <Group title="Customer Payment Control">
              <Field source="CustomerPaymentFormat" />
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Field source="CustomerPaymentTransactionNumberSeries" />
              <Field source="EntryTextCustomerPayment" />
              <Field source="CustomerPaymentChargeAccount" style="highlightRow" open="true" />
              <Field source="CustomerPaymentConsecutiveTransactionNumber" />
              <Field source="CustomerPaymentCalculateEntryDate" />
              <Field source="CustomerPaymentTransferRegistrationDate" />
              <Field source="CustomerPaymentTransferDimensions" />
              <Field source="CustomerPaymentCreateErrorEntries" />
              <Field source="SeparateJournalForErrorEntries" />
            </Group>
          </Column>
          <Column>
            <Group title="Payment Information">
              <Field source="LastProductionDate" />
              <Field source="LastVendorPaymentNumber" />
              <Field source="NextSystemNumber1" title="Next System No. 1" />
              <Field source="NextSystemNumber2" title="Next System No. 2" />
              <Field source="NextSystemNumber3" title="Next System No. 3" />
            </Group>
            <Group title="Customer Payment Information">
              <Field source="ProductionDateCollection" title="Last Production Date" />
              <Field source="LastImportDate" />
              <Field source="LastCollectionNumber" />
              <Field source="NextSystemNumber1Collection" title="Next System No. 1" />
              <Field source="NextSystemNumber2Collection" title="Next System No. 2" />
              <Field source="NextSystemNumber3Collection" title="Next System No. 3" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
        </Row>
      </Form>
    </UpperPane>
  </Layout>
</MDML>

