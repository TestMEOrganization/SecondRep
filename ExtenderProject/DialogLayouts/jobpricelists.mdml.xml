<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobPriceLists">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1, Issue ^2" arguments="JobPriceListName;Issue" />
        </Compact>
        <Columns>
          <Field source="JobPriceListName" />
          <Field source="Issue" />
          <Field source="PeriodStartingDate" />
          <Field source="PeriodEndingDate" />
          <Field source="Description" />
          <Field source="Currency" />
          <Field source="CostPriceCurrency" />
          <Field source="IntercompanyPriceCurrency" />
          <Columns ref="UserUpdateColumns" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Wizard name="CopyJobPriceListWizard" title="Copy Price List">
        <Page form="CopyJobPriceListWizardPage1" title="Select a Price List to Copy" description="Choose a price list to copy lines from" />
      </Wizard>
      <Wizard name="NewIssueWizard" title="New Issue">
        <Page form="NewIssueWizardPage1" title="Select a Start Date" description="Choose a start date for the new price list issue" />
      </Wizard>
      <Form name="Full">
        <Actions all="true">
          <Action source="CopyJobPriceList" wizard="CopyJobPriceListWizard" />
          <Action source="NewIssue" wizard="NewIssueWizard" />
        </Actions>
        <Row>
          <Column>
            <Group title="Job Price List">
              <Field source="JobPriceListName" />
              <Field source="Issue" />
              <Interval firstSource="PeriodStartingDate" lastSource="PeriodEndingDate" title="Period" />
              <Field source="Description" />
            </Group>
            <Group title="Access Level">
              <Reference foreignKey="AccessLevelName_AccessLevelHeader" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            </Group>
          </Column>
          <Column>
            <Group ref="UserUpdateGroup" />
            <Group title="Currency">
              <Field source="CostPriceCurrency" />
              <Field source="IntercompanyPriceCurrency" />
              <Field source="Currency" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Group title="Adjust Job Price List">
            <Field source="NewMarkUpPercentageVar" title="New Markup %" appearance="interval" />
            <Field source="NewStandardMarkUpPercentageVar" title="New Standard Markup %" appearance="interval" />
            <Field source="NewIntercompanyPricePercentageVar" title="New Intercompany Price %" appearance="interval" />
            <Grid style="grid_compact">
              <Header cells="|New Price|;|Adjustment, %|;|Rounding|" title="" style="grid_header" />
              <Row cells="NewCostPriceVar;AdjustmentCostPriceVar;RoundingCostPriceVar" title="Cost{Price}" />
              <Row cells="NewIntercompanyPriceVar;AdjustmentIntercompanyPriceVar;RoundingIntercompanyPriceVar" title="Intercompany Price" />
              <Row cells="NewBillingPriceVar;AdjustmentBillingPriceVar;RoundingBillingPriceVar" title="Billing Price" />
              <Row cells="NewStandardBillingPriceVar;AdjustmentStandardBillingPriceVar;RoundingStandardBillingPriceVar" title="Standard Billing Price" />
            </Grid>
          </Group>
        </Row>
        <Row>
          <Group title="Adjust Overtime Charge">
            <Field source="NewOvertimeChgPercentageCostVar" title="New Charge %, Cost" appearance="interval" />
            <Field source="NewOvertimeChgPercentageIntercompanyVar" title="New Charge %, Intercompany Price" appearance="interval" />
            <Field source="NewOvertimeChgPercentageSalesVar" title="New Charge %, Billing Price" appearance="interval" />
            <Grid style="grid_compact">
              <Header cells="|New Charge|;|Adjustment, %|;|Rounding|" title="" style="grid_header" />
              <Row cells="NewOvertimeChgAmountCostVar;AdjustmentOvertimeChgAmountCostVar;RoundingOvertimeChgAmountCostVar" title="Charge, Cost" />
              <Row cells="NewOvertimeChgAmountIntercompanyVar;AdjustmentOvertimeChgAmountIntercompanyVar;RoundingOvertimeChgAmountIntercompanyVar" title="Charge, Intercompany Price" />
              <Row cells="NewOvertimeChgAmountSalesVar;AdjustmentOvertimeChgAmountSalesVar;RoundingOvertimeChgAmountSalesVar" title="Charge, Billing Price" />
            </Grid>
          </Group>
        </Row>
        <Row>
          <Group title="Selection Criteria">
            <Interval firstSource="FromActivityNumberVar" lastSource="ToActivityNumberVar" title="Activity No." />
            <Field source="ActivityTypeVar" title="Activity Type" appearance="interval" />
            <Interval firstSource="FromActivityPriceGroupVar" lastSource="ToActivityPriceGroupVar" title="Price Group, Act." />
            <Interval firstSource="FromEmployeeNumberVar" lastSource="ToEmployeeNumberVar" title="Employee No." />
            <Field source="EmployeeTypeVar" title="Employee Type" appearance="interval" />
            <Interval firstSource="FromEmployeePriceGroupVar" lastSource="ToEmployeePriceGroupVar" title="Price Group, Empl." />
            <Interval firstSource="FromTaskNameVar" lastSource="ToTaskNameVar" title="Task" />
            <Interval firstSource="FromTaskPriceGroupVar" lastSource="ToTaskPriceGroupVar" title="Price Group, Task" />
            <Field source="OvertimeTypeVar" title="Overtime Spec." appearance="interval" />
            <Interval firstSource="FromCostPriceVar" lastSource="ToCostPriceVar" title="Cost{Price}" />
            <Interval firstSource="FromBillingPriceVar" lastSource="ToBillingPriceVar" title="Billing Price" />
            <Interval firstSource="FromIntercompanyPriceVar" lastSource="ToIntercompanyPriceVar" title="Intercompany Price" />
            <Interval firstSource="FromMarkUpPercentageVar" lastSource="ToMarkUpPercentageVar" title="Markup %" />
            <Boolean source="ShowOnlySelectedVar" title="Show Only Marked" />
          </Group>
          <Group ref="DimensionsVarTargetGroupIntervals" />
        </Row>
      </Form>
      <Form name="CopyJobPriceListWizardPage1">
        <Actions all="true" />
        <Group>
          <Field source="CopyFromJobPriceList" title="Job Price List" mandatory="true" />
          <Field source="CopyFromIssue" title="Issue" mandatory="true" />
        </Group>
      </Form>
      <Form name="NewIssueWizardPage1">
        <Actions all="true" />
        <Group>
          <Field source="NewStartingDateVar" title="Start Date" mandatory="true" />
        </Group>
      </Form>
      <Form name="Demo">
        <Actions all="true">
          <Exclude>
            <Action source="MarkLines" />
            <Action source="UnmarkLines" />
            <Action source="AdjustMarked" />
            <Action source="RemoveMarked" />
          </Exclude>
          <Create appearance="all" />
          <Action source="CopyJobPriceList" wizard="CopyJobPriceListWizard" />
          <Action source="NewIssue" wizard="NewIssueWizard" />
        </Actions>
        <Row>
          <Column>
            <Group title="Job Price List">
              <Field source="JobPriceListName" />
              <Field source="Issue" />
              <Interval firstSource="PeriodStartingDate" lastSource="PeriodEndingDate" title="Period" />
              <Field source="Description" />
            </Group>
            <Group title="Access Level">
              <Reference foreignKey="AccessLevelName_AccessLevelHeader" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            </Group>
          </Column>
          <Column>
            <Group ref="UserUpdateGroup" />
            <Group title="Currency">
              <Field source="CostPriceCurrency" />
              <Field source="IntercompanyPriceCurrency" />
              <Field source="Currency" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Group title="Adjust Job Price List">
            <Field source="NewMarkUpPercentageVar" title="New Markup %" appearance="interval" />
            <Field source="NewStandardMarkUpPercentageVar" title="New Standard Markup %" appearance="interval" />
            <Field source="NewIntercompanyPricePercentageVar" title="New Intercompany Price %" appearance="interval" />
            <Grid style="grid_compact">
              <Header cells="|New Price|;|Adjustment, %|;|Rounding|" title="" style="grid_header" />
              <Row cells="NewCostPriceVar;AdjustmentCostPriceVar;RoundingCostPriceVar" title="Cost{Price}" />
              <Row cells="NewIntercompanyPriceVar;AdjustmentIntercompanyPriceVar;RoundingIntercompanyPriceVar" title="Intercompany Price" />
              <Row cells="NewBillingPriceVar;AdjustmentBillingPriceVar;RoundingBillingPriceVar" title="Billing Price" />
              <Row cells="NewStandardBillingPriceVar;AdjustmentStandardBillingPriceVar;RoundingStandardBillingPriceVar" title="Standard Billing Price" />
            </Grid>
          </Group>
        </Row>
        <Row>
          <Group title="Adjust Overtime Charge">
            <Field source="NewOvertimeChgPercentageCostVar" title="New Charge %, Cost" appearance="interval" />
            <Field source="NewOvertimeChgPercentageIntercompanyVar" title="New Charge %, Intercompany Price" appearance="interval" />
            <Field source="NewOvertimeChgPercentageSalesVar" title="New Charge %, Billing Price" appearance="interval" />
            <Grid style="grid_compact">
              <Header cells="|New Charge|;|Adjustment, %|;|Rounding|" title="" style="grid_header" />
              <Row cells="NewOvertimeChgAmountCostVar;AdjustmentOvertimeChgAmountCostVar;RoundingOvertimeChgAmountCostVar" title="Charge, Cost" />
              <Row cells="NewOvertimeChgAmountIntercompanyVar;AdjustmentOvertimeChgAmountIntercompanyVar;RoundingOvertimeChgAmountIntercompanyVar" title="Charge, Intercompany Price" />
              <Row cells="NewOvertimeChgAmountSalesVar;AdjustmentOvertimeChgAmountSalesVar;RoundingOvertimeChgAmountSalesVar" title="Charge, Billing Price" />
            </Grid>
          </Group>
        </Row>
        <Row>
          <Group title="Selection Criteria">
            <Interval firstSource="FromActivityNumberVar" lastSource="ToActivityNumberVar" title="Activity No." />
            <Field source="ActivityTypeVar" title="Activity Type" appearance="interval" />
            <Interval firstSource="FromActivityPriceGroupVar" lastSource="ToActivityPriceGroupVar" title="Price Group, Act." />
            <Interval firstSource="FromEmployeeNumberVar" lastSource="ToEmployeeNumberVar" title="Employee No." />
            <Field source="EmployeeTypeVar" title="Employee Type" appearance="interval" />
            <Interval firstSource="FromEmployeePriceGroupVar" lastSource="ToEmployeePriceGroupVar" title="Price Group, Empl." />
            <Interval firstSource="FromTaskNameVar" lastSource="ToTaskNameVar" title="Task" />
            <Interval firstSource="FromTaskPriceGroupVar" lastSource="ToTaskPriceGroupVar" title="Price Group, Task" />
            <Field source="OvertimeTypeVar" title="Overtime Spec." appearance="interval" />
            <Interval firstSource="FromCostPriceVar" lastSource="ToCostPriceVar" title="Cost{Price}" />
            <Interval firstSource="FromBillingPriceVar" lastSource="ToBillingPriceVar" title="Billing Price" />
            <Interval firstSource="FromIntercompanyPriceVar" lastSource="ToIntercompanyPriceVar" title="Intercompany Price" />
            <Interval firstSource="FromMarkUpPercentageVar" lastSource="ToMarkUpPercentageVar" title="Markup %" />
            <Boolean source="ShowOnlySelectedVar" title="Show Only Marked" />
          </Group>
          <Group ref="DimensionsVarTargetGroupIntervals" />
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
