<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobPriceInformation">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobGroup" />
          <Field source="NotInvoiceable" />
          <Field source="InternalJob" />
          <Field source="Status" />
          <Field source="ConvertedBy" />
          <Field source="ConversionDate" />
          <Field source="ProjectManagerNumber" />
          <Field source="SalesPersonNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="JobNumber_JobHeader" />
              <Field source="JobGroup" />
              <Boolean source="Template" />
            </Group>
          </Column>
          <Column>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
              <Field source="BaseCurrency" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Group title="Price">
            <Field source="Currency" />
            <Reference foreignKey="CostJobPriceListCurrentIssue" title="Job Price List, Cost" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            <Reference foreignKey="IntercompanyJobPriceListCurrentIssue" title="Job Price List, Intercomp." keyShadowTitle="Name" descriptionShadowTitle="Description" />
            <Reference foreignKey="JobPriceListCurrentIssue" title="Job Price List, Sales" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            <Reference foreignKey="StandardBillingPriceListCurrentIssue" title="Job Price List, Standard Sales" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            <Field source="IntercompanyPriceListNumber" />
            <Field source="IntercompanyPriceLevel" title="Price Step" />
            <Field source="PriceListNumber" />
            <Field source="PriceLevel" title="Price Step" />
            <Boolean source="FixedMarkUpPercentage" />
            <Field source="MarkUpPercentage" />
            <Field source="InvoiceDiscountPercentage" />
            <Field source="JobSurchargeRuleName" />
            <Field source="QuotationPriceCurrency" />
          </Group>
        </Row>
        <Row>
          <Group title="Selection Criteria">
            <Interval firstSource="FromActivityNumberVar" lastSource="ToActivityNumberVar" title="Act. No." />
            <Interval firstSource="FromActivityPriceGroupVar" lastSource="ToActivityPriceGroupVar" title="Price Group, Act." />
            <Field source="ActivityTypeVar" title="Activity Type" appearance="interval" />
            <Interval firstSource="FromEmployeeNumberVar" lastSource="ToEmployeeNumberVar" title="Employee No." />
            <Field source="EmployeeTypeVar" title="Employee Type" appearance="interval" />
            <Interval firstSource="FromEmployeePriceGroupVar" lastSource="ToEmployeePriceGroupVar" title="Price Group, Empl." />
            <Interval firstSource="FromTaskNameVar" lastSource="ToTaskNameVar" title="Task" />
            <Interval firstSource="FromTaskPriceGroupVar" lastSource="ToTaskPriceGroupVar" title="Price Group, Task" />
            <Field source="OvertimeTypeVar" title="Overtime Spec." appearance="interval" />
          </Group>
          <Group ref="DimensionsVarTargetGroupIntervals" />
        </Row>
      </Form>
      <Form name="Standard">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Price Information">
              <Field source="Currency" title="Job Currency" />
              <Field source="InvoiceCurrency" />
              <Field source="FixedMarkUpPercentage" />
              <Field source="MarkUpPercentage" />
              <Field source="InvoiceDiscountPercentage" />
              <Field source="JobSurchargeRuleName" />
            </Group>
          </Column>
          <Column>
            <Group title="Price Lists?!?!?">
              <Reference foreignKey="CostJobPriceListCurrentIssue" title="Cost" keyShadowTitle="Name" descriptionShadowTitle="Description" />
              <If condition="CostJobPriceList != ''">
                <Field source="CostJobPriceListCurrentIssue-&gt;JobPriceLists.CostPriceCurrency" />
              </If>
              <Reference foreignKey="IntercompanyJobPriceListCurrentIssue" title="Intercompany" keyShadowTitle="Name" descriptionShadowTitle="Description" />
              <If condition="IntercompanyJobPriceList != ''">
                <Field source="IntercompanyJobPriceListCurrentIssue-&gt;JobPriceLists.IntercompanyPriceCurrency" />
              </If>
              <Reference foreignKey="JobPriceListCurrentIssue" title="Sales!!" keyShadowTitle="Name" descriptionShadowTitle="Description" />
              <If condition="JobPriceList != ''">
                <Field source="JobPriceListCurrentIssue-&gt;JobPriceLists.Currency" />
              </If>
              <Reference foreignKey="StandardBillingPriceListCurrentIssue" title="Standard Sales" keyShadowTitle="Name" descriptionShadowTitle="Description" />
              <If condition="StandardBillingPriceList != ''">
                <Field source="StandardBillingPriceListCurrentIssue-&gt;JobPriceLists.Currency" />
              </If>
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Custom">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Price Information">
              <Field source="Currency" title="Job Currency" style="highlightFieldLTBlue" />
              <Field source="InvoiceCurrency" style="highlightFieldLTBlue" />
            </Group>
          </Column>
          <Column>
            <Group title="Price Lists">
              <Grid>
                <Define>
                  <Style name="largeCellStyle" size="large" />
                   <Style name="MediumCellStyle" size="medium" />
                  <Style name="SmallCellStyle" size="small" /> 
                  <Style name="TinyCellStyle" size="tiny" /> 
                  <Style name="VastCellStyle" size="vast" />
                  <Description name="SalesCurrencyC" template="^1" arguments="JobPriceListCurrentIssue-&gt;JobPriceLists.Currency" />
                </Define>
                	<Row title="Sales" cells="JobPriceList;SalesCurrencyC" >
                	<Style ref="VastCellStyle;TinyCellStyle"/>         
                	</Row>
                	<Row title="Item" cells="PriceListNumber" >
                	<Style ref="VastCellStyle"/>         
                	</Row>
              </Grid>
            </Group>
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="LineNumber"/>
          <Field source="ActivityNumber" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="ActivityType" />
          <Field source="ActivityPriceGroupNumber" />
          <Field source="EmployeeNumber" title="Employee Information" />
          <Field source="EmployeeNameVar" title="Employee Name" />
          <Field source="EmployeeType" />
          <Field source="EmployeePriceGroupNumber" />
          <Field source="TaskName" />
          <Field source="ActivityNumber" />
          <Field source="TaskTextVar" title="Task Description{1}" />
          <Field source="TaskPriceGroupNumber" />
          <Columns ref="DimensionColumns" />
          <Field source="CostPrice" />
          <Field source="CostPerDayVar" title="Cost, Per Day" />
          <Field source="BaseSalaryRate" />
          <Field source="OverheadCostRate" />
          <Field source="IntercompanyPrice" />
          <Field source="IntercompanyPricePerDayVar" title="Intercompany Price, Per Day" />
          <Field source="InterCompanyPricePercentage" />
          <Field source="BillingPriceCurrency" title="Billing Price" />
          <Field source="BillingPricePerDayVar" title="Billing Price, Per Day" />
          <Field source="MarkUpPercentage" />
          <Field source="BaseSalaryMarkUpPercentage" />
          <Field source="AdjustmentPercentage" title="Adjustment %" />
          <Field source="OvertimeType" />
          <Field source="OvertimeChgPercentageCost" />
          <Field source="OvertimeChgAmountCost" />
          <Field source="OvertimeChgPercentageIntercompany" />
          <Field source="OvertimeChgAmountIntercompany" />
          <Field source="OvertimeChgPercentageSales" />
          <Field source="OvertimeChgAmountSales" />
          <Field source="CurrencyCostPrice" />
          <Field source="CurrencyIntercompanyPrice" />
          <Field source="CurrencySalesPrice" />
        </Columns>
      </Table>
      <Table name="Standard">
        <Actions all="true" />
        <Columns>
          <Field source="LineNumber"  />
          <Field source="BillingPriceCurrency" title="Billing Price" />
          <Field source="AdjustmentPercentage" title="Adjustment %" />
          <Field source="CostPrice" />
          <Field source="MarkUpPercentage" />
          <Field source="IntercompanyPrice" />
          <Field source="InterCompanyPricePercentage" />
          <Field source="TaskName" />
           <Field source="ActivityNumber" />
          <Field source="TaskTextVar" title="Task Description{1}" />
          <Field source="ActivityType" />
          <Field source="EmployeeNumber" title="Employee Information" />
          <Field source="EmployeeNameVar" title="Employee Name" />
        </Columns>
      </Table>
      <Table name="Custom">
        <Actions all="true" />
        <Columns>
          <Field source="LineNumber" />
          <Field source="ActivityType" />
          <Field source="TaskName" />
           <Field source="ActivityNumber" mandatory="true" />
          <Field source="BillingPriceCurrency" title="Billing Price" />
          <Field source="MarkUpPercentage" />
          <Field source="EmployeeNameVar" title="Employee Name" />
          <Field source="ActivityPriceGroupNumber" />
          <Field source="TaskTextVar" title="Task Description{1}" />
          <Field source="OvertimeType" />
          <Field source="OvertimeChgPercentageSales" />
        </Columns>
      </Table>
    </LowerPane>
  </Layout>
</MDML>

