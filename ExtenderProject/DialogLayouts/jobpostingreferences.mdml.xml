    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2)" arguments="Description;ReferenceName" />
        </Compact>
        <Columns>
          <Field source="ReferenceName" />
          <Field source="Description" />
          <Field source="AccessLevelName" />
          <Columns ref="UserUpdateColumns" />
        </Columns>
      </Filter>
