<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobOverview">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobGroup" />
          <Field source="NotInvoiceable" />
          <Field source="InternalJob" />
          <Field source="Status" />
          <Field source="ConvertedBy" />
          <Field source="ConversionDate" />
          <Field source="ProjectManagerNumber" />
          <Field source="SalesPersonNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <Pane>
      <Define>
        <!-- **** RambÃ¸ll - GD_Dimensions added for Black Box pane using on the definitions **** -->
        <Include fragment="GD_Dimensions" />
        <Function name="NonApprovedHoursWithBillingPrice" value="TimeNonApprovedBillingPriceCurrencyVar gt 0" />
        <Style name="NonApprovedHoursWithBillingPrice" ref="trafficlightRed" condition="NonApprovedHoursWithBillingPrice()" />
        <Function name="InvoicingIsWrittenDown" value="TotalBillingPriceUpDownCurrencyVar lt 0" />
        <Style name="InvoicingIsWrittenDown" ref="trafficlightRed" condition="InvoicingIsWrittenDown()" />
        <Function name="InvoicingIsWrittenUp" value="TotalBillingPriceUpDownCurrencyVar gt 0" />
        <Style name="InvoicingIsWrittenUp" ref="trafficlightGreen" condition="InvoicingIsWrittenUp()" />
        <Function name="InvoicingPlanLineDue" value="NextInvoicingPlanLineDateVar le currentDate() and NextInvoicingPlanLineDateVar != date(nulldate)" />
        <Style name="InvoicingPlanLineDue" ref="trafficlightRed" condition="InvoicingPlanLineDue()" />
        <Function name="HasProjectManagerTimeApproval" value="ApprovalProjectManager = ApprovalPrincipleType'Optional or ApprovalProjectManager = ApprovalPrincipleType'Mandatory" />
        <Function name="HasProjectManagerExpenseApproval" value="ApprProjectManagerExpenses = ApprovalPrincipleType'Optional or ApprProjectManagerExpenses = ApprovalPrincipleType'Mandatory" />
        <Widget name="RealizedFactorDialChart" source="DialChartWidget">
          <Dimension start="0" end="3">
            <Region start="0" end="1.5" style="redDialColor" />
            <Region start="1.5" end="2.0" style="greyDialColor" />
            <Region start="2.0" end="3" style="greenDialColor" />
          </Dimension>
        </Widget>
        <Function name="deltaxy" value="y-x" parameters="x y" />
        <Function name="deltaXYZ" value="x-y-z" parameters="x y z" />
        <Function name="sumXYZ" value="x+y+z" parameters="x y z" />
        <Function name="divXY" value="x/y" parameters="x y" />
        <Function name="profitability" value="(x-y)/z" parameters="x y z" />
        <Style name="HighlightNegative" ref="trafficlightForegroundRed" condition="fieldValue() lt 0" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="JobNumber_JobHeader" descriptionSuggestions="none" keySuggestions="none" />
              <Field source="Currency" />
              <Field source="JobInvoiceableVar" />
              <Field source="JobGroup" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
              <Reference foreignKey="MainJobNumber_JobHeader" title="Main Job" />
              <Field source="NumberOfSubJobs" />
              <Field source="Closed" />
            </Group>
          </Column>
          <Column>
            <Group title="Customer">
              <Reference foreignKey="CustomerNumber_Customer" descriptionSource="Name1" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
              <Field source="BaseCurrency" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group title="Dates">
              <Field source="StartingDate" />
              <Field source="ExpectedEndingDate" />
              <Field source="ActualEndingDate" />
            </Group>
            <Group title="Price">
              <Field source="InvoicingPrincipleVar" />
              <Field source="FixedPriceBasisVar" />
              <Field source="FixedPriceBudgetTypeVar" />
              <Field source="FixedPriceCurrencyVar" />
              <Field source="QuotationPriceCurrency" />
            </Group>
          </Column>
          <Column>
            <Group title="Current Budget">
              <Field source="CurrentBudgetCreatedVar" title="Created" />
              <Field source="CurrentBudgetTypeVar" title="Budget Type" />
              <Field source="CurrentBudgetRevisionNumberVar" title="Revision No." />
              <Field source="CurrentBudgetCostPriceBaseVar" title="Cost Price" />
              <Field source="CurrentBudgetBillingPriceCurrencyVar" title="Billing Price, Currency" />
              <Field source="CurrentBudgetBillingPriceBaseVar" title="Billing Price, Base" />
              <Field source="CurrentBudgetGrossMargin1BaseVar" title="Gross Margin, Base" />
              <Field source="CurrentBudgetPercGrossMargin1Var" title="Gross Margin Percentage" />
              <Field source="CurrentBudgetMarkUpPercentageVar" title="Markup Percentage" />
              <Field source="CurrentBudgetSubmittedVar" title="Submitted" />
              <Field source="CurrentBudgetSubmittedByVar" title="Submitted by" />
              <Field source="CurrentBudgetDateSubmittedVar" title="Date Submitted" />
              <Field source="CurrentBudgetApprovedVar" title="Approved" />
              <Field source="CurrentBudgetApprovedByVar" title="Approved by" />
              <Field source="CurrentBudgetApprovalDateVar" title="Date Approved" />
              <If condition="hasAddOn(128) and IntegrationStatusLastSentVar != ''">
                <Field source="IntegrationStatusLastSentVar" />
              </If>
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group title="Time Registrations">
              <Field source="TimeNumberRegisteredVar" />
              <Field source="TimeCostPriceRegBaseVar" title="Cost Price Reg., Base" />
              <Field source="TimeBillingPriceRegCurrencyVar" title="Billing Price Reg., Currency" />
              <Field source="LatestTimeSheetRegistrationsVar" />
              <Field source="LastDateProjectManagerTimeApprovalVar" />
              <Label title="Invoiced" style="label" appearance="maximized" />
              <Field source="TimeNumberInvoicedVar" />
              <Field source="TimeNumberUpDownVar" />
              <Field source="TimeBillingPriceInvoicedCurrencyVar" title="Billing Price Invoiced, Currency" />
              <Field source="TimeBillingPriceUpDownCurrencyVar" title="Billing Price Up/Down, Currency" />
              <Field source="TimeCostPriceInvoicedBaseVar" title="Cost Price Invoiced, Base" />
              <Label title="Open" style="label" appearance="maximized" />
              <Field source="TimeNumberOpenVar" />
              <Field source="TimeBillingPriceOpenCurrencyVar" title="Billing Price, Currency" />
              <Field source="TimeCostPriceOpenBaseVar" title="Cost Price, Base" />
              <Label title="Non Approved Time Sheets" style="label" appearance="maximized" />
              <Field source="TimeNonApprovedBillingPriceCurrencyVar" title="Billing Price, Currency" />
              <Label title="Non Submitted Time Sheets" style="label" appearance="maximized" />
              <Field source="TimeNumberOfNotSubmittedVar" title="Number of Hours" />
            </Group>
          </Column>
          <Column>
            <Group title="Amount Registrations">
              <Field source="AmountCostPriceRegBaseVar" title="Cost Price Reg., Base" />
              <Field source="AmountBillingPriceRegCurrencyVar" title="Billing Price Reg., Currency" />
              <Field source="LatestExpenseSheetRegistrationsVar" />
              <Field source="LatestMileageSheetRegistrationsVar" />
              <Field source="LastDateProjectManagerExpenseApprovalVar" />
              <Label title="Invoiced" style="label" appearance="maximized" />
              <Field source="AmountBillingPriceInvoicedCurrencyVar" title="Billing Price Invoiced, Currency" />
              <Field source="AmountBillingPriceUpDownCurrencyVar" title="Billing Price Up/Down, Currency" />
              <Field source="AmountCostPriceInvoicedBaseVar" title="Cost Price Invoiced, Base" />
              <Label title="Open Entries" style="label" appearance="maximized" />
              <Field source="AmountBillingPriceOpenCurrencyVar" title="Open Billing Price, Currency" />
              <Field source="AmountCostPriceOpenBaseVar" title="Open Cost Price, Base" />
              <Label title="Non Approved Expense Sheets" style="label" appearance="maximized" />
              <Field source="AmountNonApprovedBillingPriceCurrencyVar" title="Billing Price, Currency" />
              <Field source="AmountNonApprovedCostPriceExVATBaseVar" title="Cost Price Excl. Tax, Base" />
              <Label title="Non Approved Mileage Sheets" style="label" appearance="maximized" />
              <Field source="MileageNonApprovedBillingPriceCurrencyVar" title="Billing Price, Currency" />
              <Field source="MileageNonApprovedCostPriceExVATBaseVar" title="Cost Price Excl. Tax, Base" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group title="Billing">
              <Field source="TotalBillingPriceUpDownCurrencyVar" />
            </Group>
            <If condition="InvoiceAllocation">
              <Group title="Job Invoice Allocation">
                <Field source="InvoiceAllocationBillingPriceCurrencyVar" title="Allocated, Total, Currency" />
                <Label title="Gross Margin" style="label" appearance="maximized" />
                <Field source="InvoiceAllocationGrossMarginBaseVar" title="Gross Margin, Base" />
                <Field source="InvoiceAllocationGrossMarginPercentageVar" title="Gross Margin Percentage" />
                <Field source="InvoiceAllocationMarkUpPercentageVar" title="Markup Percentage" />
              </Group>
            </If>
            <If condition="ToBeInvoicedOnMainJobVar">
              <Group title="Invoiced on Main Job">
                <Field source="OpenForInvoiceSelection" title="Open for Selection" />
                <Field source="ApprovedForInvoicingOnMainJob" title="Appr. for Inv. on Main Job" />
                <Field source="ApprovedForInvoicingBy" title="Approved by" />
                <Field source="ApprovedForInvoicingDate" title="Approval Date" />
              </Group>
            </If>
            <Else>
              <Group title="Invoice Selection Status">
                <Field source="OpenForInvoiceSelection" title="Open for Selection" />
                <Field source="IncludesSubJobsVar" />
                <Field source="CarryForwardCurrencyVar" />
                <Pair firstSource="ApprovedForInvoiceEditing" secondSource="NumberOfDraftsForEditingVar" title="Drafts for Editing" />
                <Pair firstSource="ApprovedForInvoicing" secondSource="NumberOfApprovedDraftsVar" title="Approved for Invoicing" />
                <Field source="ApprovedForInvoicingBy" title="Approved by" />
                <Field source="ApprovedForInvoicingDate" title="Approval Date" />
              </Group>
            </Else>
            <Group title="Revenue Recognition">
              <Field source="RevenueRecognitionJobParameterVar" title="Job Parameter Setup" />
              <Field source="TotalRevenueRecognizedCurrencyVar" title="Revenue Recognized, Currency" />
              <If condition="RecognizeRevenueByCompletionPctVar">
                <Label title="Completion Percentage" style="label" appearance="maximized" />
                <Field source="LastDateOfRevenueRecognizedVar" />
                <Field source="CompletionPctBudgetTypeVar" title="Budget Type" />
                <Field source="PercentageRevenueRecognizedVar" title="Completion Percentage" />
                <Field source="RevenueRecognizedByCompletionPctCurrencyVar" title="Revenue Recognized" />
              </If>
              <If condition="EvaluationOfWorkInProgressVar">
                <Label title="Evaluation of WIP" style="label" appearance="maximized" />
                <Field source="WIPEvaluationDateVar" />
                <Field source="ApprovedWorkInProgressCurrencyVar" />
              </If>
            </Group>
          </Column>
          <Column>
            <Group title="Invoicing">
              <Field source="LastInvoiceDate" />
              <Field source="InvoiceBillingPriceCurrencyVar" />
              <Label title="Invoicing on Account" style="label" appearance="maximized" />
              <Field source="TotalInvoicedOnAccountCurrencyVar" />
              <Field source="JobNetOnAccountCurrencyVar" />
              <Field source="CollectionNetOnAccountCurrencyVar" />
              <Field source="NextInvoicingPlanLineDateVar" />
              <Field source="NextInvoicingPlanLinePhaseVar" />
              <Label title="Gross Margin" style="label" appearance="maximized" />
              <Field source="InvoiceGrossMarginBaseVar" />
              <Field source="InvoiceGrossMarginPercentageVar" />
              <Field source="InvoiceMarkUpPercentageVar" />
            </Group>
            <Group title="Customer Balance (Incl. Tax)">
              <Field source="JobCustomersBalanceBaseVar" />
              <Field source="JobCustomersBalanceDueBaseVar" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group title="Factor">
              <Field source="CurrentBudgetFactorVar" />
              <Field source="JobFactorRegVar" />
              <Field source="JobFactorInvoicedVar" />
              <Field source="JobFactorRevenueRecognizedVar" />
            </Group>
            <Group title="Purchase Orders">
              <Field source="CommittedPurchaseOrdersCurrencyVar" title="Committed, Currency" />
            </Group>
          </Column>
          <Column>
            <Group title="Latest Approved Progress Evaluation">
              <Field source="JobProgressUnitOfMeasureVar" />
              <Field source="JobProgressShowOriginalFiguresVar" />
              <Field source="JobProgressEstimateDateVar" />
              <Field source="JobProgressPlannedVar" />
              <Field source="JobProgressExecutedVar" />
              <Field source="JobProgressEstimatedTimeToCompVar" />
              <Field source="JobProgressEstimatedTimeToCompManDaysVar" />
              <Field source="JobProgressTotalVar" />
              <Field source="JobProgressDeviationVar" />
              <Field source="JobProgressAccumulatedDeviationVar" />
              <Field source="JobProgressBaselineVar" />
              <Field source="JobProgressBaselineDeviationVar" />
              <Field source="JobProgressCompletionPercentageVar" />
              <Field source="JobProgressRemarkVar" style="MultilineDescription" />
              <Field source="JobProgressEvaluationStatusVar" />
            </Group>
          </Column>
        </Row>
      </Form>
      <!-- **** RambÃ¸ll customization - Changes made to look and feel of overview **** -->
      <Form name="Standard" style="compact_amountAlignment">
        <Actions>
          <Refresh />
        </Actions>
        <Row>
          <Column>
            <Group title="Basic Information">
              <Description title="Job Name" template="^1 (^2)" arguments="JobName;JobNumber" size="huge" />
              <Description title="Customer" template="^1 (^2)" arguments="Name1;CustomerNumber" size="huge" />
            </Group>
            <Group title="Overall KPI">
              <Style anchor="left" justify="left" />
              <Description title="Profitability" template="" />
              <Element source="JobFactorRevenueRecognizedVar" widget="RealizedFactorDialChart" title="Profitability" appearance="maximized" />
            </Group>
          </Column>
          <Column>
            <Group title="Project Financials">
              <Grid>
                <Define>
                  <!-- <Description name="CostPriceTime_Bud" template="^1" arguments="parent.primary->JobBudgets_Card.ShownCostPriceTimeVar" />
					<Description name="CostPriceAmount_Bud" template="^1" arguments="parent.primary->JobBudgets_Card.CostPriceAmountVar" /> -->
                  <Description name="CostPriceTime_Bud" template="^1" arguments="parent.TimeBudgetedCostPriceBase" />
                  <Description name="CostPriceAmount_Bud" template="^1" arguments="parent.AmountBudgetedCostPriceBase" />
                  <!-- <Description name="RemainRevBud" template="^1" arguments="deltaxy(TotalRevenueRecognizedBaseVar,BaselineBudgetBillingPriceCurrencyVar)" /> -->
                  <Description name="RemainRevBud" template="^1" arguments="deltaxy(TotalRevenueRecognizedBaseVar,BaselineBudgetBillingPriceBaseVar)" />
                  <Description name="RemainTimeCostBud" template="^1" arguments="deltaxy(TimeCostPriceRegBaseVar,parent.TimeBudgetedCostPriceBase)" />
                  <Description name="RemainAmountCostBud" template="^1" arguments="deltaxy(AmountCostPriceRegBaseVar,parent.AmountBudgetedCostPriceBase)" />
                  <Description name="Result_Actuals" template="^1" arguments="deltaXYZ(TotalRevenueRecognizedBaseVar,TimeCostPriceRegBaseVar,AmountCostPriceRegBaseVar)" />
                  <Description name="Result_Bud" template="^1" arguments="deltaXYZ(BaselineBudgetBillingPriceBaseVar,parent.TimeBudgetedCostPriceBase,parent.AmountBudgetedCostPriceBase)" />
                  <Description name="Result_RemainBud" template="^1" arguments="deltaXYZ(deltaxy(TotalRevenueRecognizedBaseVar,BaselineBudgetBillingPriceBaseVar),deltaxy(TimeCostPriceRegBaseVar,parent.TimeBudgetedCostPriceBase),deltaxy(AmountCostPriceRegBaseVar,parent.AmountBudgetedCostPriceBase))" />
                  <Description name="FF_Actuals" template="^1" arguments="divXY(TimeRevenueRecognizedBaseVar,TimeCostPriceRegBaseVar)" />
                  <Description name="FF_Budget" template="^1" arguments="divXY(BaselineBudgetBillingPriceBaseVar,parent.TimeBudgetedCostPriceBase)" />
                  <Description name="Profit_Actuals" template="^1" arguments="profitability(TotalRevenueRecognizedBaseVar,AmountCostPriceRegBaseVar,TimeCostPriceRegBaseVar)" />
                  <Description name="Profit_Budget" template="^1" arguments="profitability(BaselineBudgetBillingPriceBaseVar,parent.AmountBudgetedCostPriceBase,parent.TimeBudgetedCostPriceBase)" />
                  <Description name="CompletePercent" template="^1%" arguments="PercentageRevenueRecognizedVar" />
                  <Description name="FeePerHourVar" template="^1" arguments="divXY(TotalRevenueRecognizedBaseVar,TimeNumberRegisteredVar)" />
                </Define>
                <Row title="" cells="|Actuals|;|Budget|;|Remaining budget|" style="grid_header" />
                <Row title="Project revenue" cells="TotalRevenueRecognizedBaseVar;BaselineBudgetBillingPriceBaseVar;RemainRevBud" />
                <Row title="Cost, time" cells="TimeCostPriceRegBaseVar;CostPriceTime_Bud;RemainTimeCostBud" />
                <Row title="Cost, amount" cells="AmountCostPriceRegBaseVar;CostPriceAmount_Bud;RemainAmountCostBud" />
                <!-- <Row cells="||||______________________________________________________________________________________||||" /> -->
                <Row title="Project result       " cells="Result_Actuals;Result_Bud;Result_RemainBud" style="highlightFieldGrey" />
                <Row title="" cells="_;_;_" />
                <Row title="Fee Factor" cells="FF_Actuals;FF_Budget;_" />
                <Row title="Profitability" cells="Profit_Actuals;Profit_Budget;_" />
                <Row title="Fee per hour" cells="FeePerHourVar" />
                <Row title="Completion Pct." cells="CompletePercent;_;_" />
                <Row title="(This Comp. % is based on the calculation of POC per Task)" cells="_;_;_" />
              </Grid>
              <!-- 
					<Field source="TimeRevenueRecognizedBase" />
					<Field source="BaselineBudgetBillingPriceBaseVar" /> -->
              <Link workspace="Jobs" template="Budget Details" icon="WorkspaceLink" appearance="maximized">
                <Style linkForegroundColor="rgb=161,191,54">
                  <Label linkForegroundColor="rgb=161,191,54" />
                </Style>
                <Waypoint pane="Jobs_Filter">
                  <Restriction template="Selected Job" expression="JobNumber = target.JobNumber" />
                  <Focus>
                    <Match value="JobNumber" field="JobNumber" />
                  </Focus>
                </Waypoint>
                <Target pane="JobBudgets_Card" />
              </Link>
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group title="Time">
              <Field source="TimeNumberRegisteredVar" />
              <UnitField source="TimeBillingPriceOpenBaseVar" unitSource="BaseCurrency" title="Open Billing Price, Time" />
              <UnitField source="TimeCostPriceOpenBaseVar" unitSource="BaseCurrency" title="Open Cost Price, Time" />
              <UnitField source="TimeBillingPriceRegBaseVar" unitSource="BaseCurrency" title="Billing Price Registered, Time" />
              <UnitField source="TimeCostPriceRegBaseVar" unitSource="BaseCurrency" title="Cost Price Registered, Time" />
              <Link template="Time Report" icon="WorkspaceLink" appearance="maximized">
                <Style linkForegroundColor="rgb=161,191,54">
                  <Label linkForegroundColor="rgb=161,191,54" />
                </Style>
		        <Url value="urlOpenReportingServer() + '/_layouts/ReportServer/RSViewerPage.aspx'">
		          <Query field="rv:RelativeReportUrl" value="/Reports/Time.rdl" />
		          <!-- <Query field="rv:ToolBarItemsDisplayMode" value="^1" arguments="223" />
		          <Query field="rv:ParamMode" value="^1" arguments="'hidden'" /> -->
		          <Query field="rv:HeaderArea" value="^1" arguments="'none'" />
		          <Query field="rp:Project" value="^1" arguments="parent.JobNumber" />
		          <Query field="rp:Employee" value="^1" arguments="username()" />
		        </Url>
              </Link>
            </Group>
            <!-- <Group title="Non Approved Time">
						<If condition="HasProjectManagerTimeApproval()">
							<Link workspace="Jobs" template="Time Approval" icon="WorkspaceLink" appearance="maximized">
								<Waypoint pane="Jobs_Filter">
									<Restriction template="Selected Job" expression="JobNumber = target.JobNumber" />
									<Focus>
										<Match value="JobNumber" field="JobNumber" />
									</Focus>
								</Waypoint>
								<Target pane="ApproveTimeSheetLines_Card" />
							</Link>
						</If>
						<UnitField source="TimeNonApprovedBillingPriceBaseVar" unitSource="BaseCurrency" title="Billing Price, Time" />
					</Group> -->
            <Group title="Expenses">
