<?xml version="1.0" encoding="UTF-8"?>
<!-- RMA, 06/04/09: Create empty forms for every dialog (37642).-->
<!-- JSV, 02/07/09: Insert Original layout converted from mdl and filter layout (37802).-->
<!-- MJA, 27/10/09: Update Original layout and remove Empty layout (37802).-->
<!-- CSK, 04/11/10: New field "Automatic Approval" (38565).-->
<!-- JSV, 29/11/10: Compact block for Original filter (39106).-->
<!-- CSK, 26/07/11: New Standard form with action wizards (39789).-->
<!-- JSV, 24/08/11: Use reference element where possible (39874).-->
<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="AssetSale">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="DescriptionOfAsset;AssetNumber;AssetGroup" />
        </Compact>
        <Columns>
          <Field source="AssetNumber" />
          <Field source="AssetGroup" />
          <Field source="AssetSubGroup" />
          <Field source="DescriptionOfAsset" />
          <Field source="Responsible" />
          <Field source="SeriesNumber" />
          <Field source="InternalSerialNumber" />
          <Field source="AcquisitionDate" />
          <Field source="CompanyNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Wizard name="MDK_ApproveAssetSaleWizard" title="Asset Sale">
        <Page form="MDK_ApproveAssetSaleWizardPage1" title="" description="Please enter the date, remark and the percentage sold" />
      </Wizard>
      <Wizard name="ApproveAssetSaleWizard" title="Approve Asset Sale">
        <Page form="ApproveAssetSaleWizardPage1" title="" description="Please enter the date, remark and the percentage sold" />
      </Wizard>
      <Wizard name="MDK_ApproveTransferWizard" title="Asset Transfer">
        <Page form="MDK_ApproveTransferWizardPage1" title="" description="Please enter the date, remark and the new asset group" />
      </Wizard>
      <Wizard name="ApproveTransferWizard" title="Approve Asset Transfer">
        <Page form="ApproveTransferWizardPage1" title="" description="Please enter the date, remark and the new asset group" />
      </Wizard>
      <Form name="Full">
        <Actions all="true" />
        <Column>
          <Group title="Asset">
            <Field source="AssetNumber" />
            <Field source="AssetGroup" />
            <Field source="AssetSubGroup" />
          </Group>
          <Group title="Company">
            <Reference foreignKey="CompanyNumber_CompanyInformation" />
            <Field source="BaseCurrencyVar" title="Base Currency" />
          </Group>
          <Group title="Asset Information">
            <Field source="DescriptionOfAsset" />
            <Field source="Model" />
            <Field source="AssetLocation" />
          </Group>
          <Group title="Asset Adjustment">
            <Boolean source="ApproveAssetAdjustmentVar" title="Automatic Approval" />
          </Group>
          <Group title="Sales{Process}">
            <Field source="SalesAmountVar" title="Amount" />
            <Field source="SalesPercentageVar" title="Pct. Sold" mandatory="true" />
            <Field source="SalesDateVar" title="Date of Sale" />
            <Field source="SalesTransactionTypeVar" title="Trans. Type" />
            <Field source="SalesTransactionNumberSeriesVar" title="Number Series{sg}" />
            <Field source="SalesTransactionNumberVar" title="Trans. No." />
            <Field source="SalesRemarkVar" title="Entry Description" />
          </Group>
          <Group title="Transfer{noun}">
            <Field source="NewAssetGroupVar" title="New Asset Group{1}" />
            <Field source="NewAssetNumberVar" title="New Asset Number" />
            <Field source="TransferDateVar" title="Date Transferred" />
            <Field source="TransferTransactionTypeVar" title="Trans. Type" />
            <Field source="TransferTransactionSeriesVar" title="Number Series{sg}" />
            <Field source="TransferTransactionNumberVar" title="Trans. No." />
            <Field source="TransferRemarkVar" title="Entry Description" />
          </Group>
        </Column>
      </Form>
      <Form name="MDK_ApproveAssetSaleWizardPage1">
        <Actions all="true" />
        <Group title="" style="fullWidth">
          <Field source="SalesAmountVar" title="Sales Price" />
          <Field source="SalesPercentageVar" title="Percentage Sold" />
          <Field source="SalesDateVar" title="Date of Sale" mandatory="true" />
          <Field source="SalesRemarkVar" title="Remark" mandatory="true" />
        </Group>
      </Form>
      <Form name="ApproveAssetSaleWizardPage1">
        <Actions all="true" />
        <Group title="" style="fullWidth">
          <Field source="SalesAmountVar" title="Amount Sold" />
          <Field source="SalesPercentageVar" title="Percentage Sold" />
          <Field source="SalesDateVar" title="Date of Sale" mandatory="true" />
          <Field source="SalesTransactionTypeVar" title="Transaction Type" />
          <Field source="SalesTransactionNumberSeriesVar" title="Number Series{sg}" />
          <Field source="SalesTransactionNumberVar" title="Transaction No." />
          <Field source="SalesRemarkVar" title="Remark" mandatory="true" />
        </Group>
      </Form>
      <Form name="MDK_ApproveTransferWizardPage1">
        <Actions all="true" />
        <Group title="" style="fullWidth">
          <Field source="NewAssetGroupVar" title="New Asset Group{1}" mandatory="true" />
          <Field source="TransferDateVar" title="Date Transferred" mandatory="true" />
          <Field source="TransferRemarkVar" title="Remark" mandatory="true" />
        </Group>
      </Form>
      <Form name="ApproveTransferWizardPage1">
        <Actions all="true" />
        <Group title="" style="fullWidth">
          <Field source="NewAssetGroupVar" title="New Asset Group{1}" mandatory="true" />
          <Field source="NewAssetNumberVar" title="New Asset Number" />
          <Field source="TransferDateVar" title="Date Transferred" mandatory="true" />
          <Field source="TransferTransactionTypeVar" title="Transaction Type" />
          <Field source="TransferTransactionSeriesVar" title="Number Series{sg}" />
          <Field source="TransferTransactionNumberVar" title="Transaction No." />
          <Field source="TransferRemarkVar" title="Remark" mandatory="true" />
        </Group>
      </Form>
      <Form name="Custom">
        <Actions all="true">
          <Action source="ApproveAssetSale" wizard="MDK_ApproveAssetSaleWizard" />
          <Action source="ApproveTransfer" wizard="MDK_ApproveTransferWizard" />
        </Actions>
        <Column>
          <Group title="Asset">
            <Field source="AssetNumber" />
            <Field source="AssetGroup" />
            <Field source="AssetSubGroup" />
            <Field source="DescriptionOfAsset" />
            <Field source="Model" />
            <Field source="AssetLocation" />
          </Group>
        </Column>
        <Column>
          <Group title="Company">
            <Reference foreignKey="CompanyNumber_CompanyInformation" title="Company" />
            <Field source="BaseCurrencyVar" title="Base Currency" />
          </Group>
          <Group title="Asset Adjustment">
            <Boolean source="ApproveAssetAdjustmentVar" title="Automatic Approval" />
          </Group>
        </Column>
      </Form>
      <Form name="Standard">
        <Actions all="true">
          <Action source="ApproveAssetSale" wizard="ApproveAssetSaleWizard" />
          <Action source="ApproveTransfer" wizard="ApproveTransferWizard" />
        </Actions>
        <Column>
          <Group title="Asset">
            <Field source="AssetNumber" />
            <Field source="AssetGroup" />
            <Field source="AssetSubGroup" />
            <Field source="DescriptionOfAsset" />
            <Field source="Model" />
            <Field source="AssetLocation" />
          </Group>
        </Column>
        <Column>
          <Group title="Company">
            <Reference foreignKey="CompanyNumber_CompanyInformation" title="Company" />
            <Field source="BaseCurrencyVar" title="Base Currency" />
          </Group>
          <Group title="Asset Adjustment">
            <Boolean source="ApproveAssetAdjustmentVar" title="Automatic Approval" />
          </Group>
        </Column>
      </Form>
    </UpperPane>
  </Layout>
</MDML>

