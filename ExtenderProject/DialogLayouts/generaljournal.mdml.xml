<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="GeneralJournal">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="Journal No. ^1" arguments="JournalNumber" />
        </Compact>
        <Columns>
          <Field source="JournalNumber" />
          <Field source="JournalType" />
          <Field source="IntercompanyBalance" />
          <Field source="NameOfUser" />
          <Field source="CreatedDate" />
          <Field source="Period" />
          <Field source="PeriodEnd" />
          <Field source="Balance" />
          <Field source="BaseCurrency" />
          <Field source="BalanceEnterprise" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Wizard name="CreateJournal" title="Create General Journal">
        <Page form="CreateJournalWizardPage1" title="General Journal Information" description="Please enter the company (cannot be changed afterwards)" />
        <If condition="KeepLinesWhenPosting">
          <Page form="CreateJournalWizardPage2" title="Template Information" description="Please enter the template name and a new entry date" />
        </If>
        <If condition="CreateRevJournal">
          <Page form="CreateJournalWizardPage3" title="Reversing Journal Information" description="Please enter a new entry date" />
        </If>
      </Wizard>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Journal{Draft}">
              <Field source="JournalNumber" />
              <Field source="NameOfUser" />
              <Field source="CreatedDate" title="Date" />
              <Field source="SubmittedBy" />
              <Field source="DateSubmitted" title="Date" />
              <Field source="PeriodNumber" />
              <Field source="Period" title="Period Start" />
              <Field source="PeriodEnd" title="Period End" />
              <Field source="FiscalYear" />
              <Field source="Balance" />
              <Field source="BalanceEnterprise" />
              <Field source="PostEntryPeriod" />
              <Boolean source="AutomaticVoucherVar" title="Auto Trans. No." />
              <Boolean source="ToBePosted" />
              <Boolean source="ReuseEntryInformationVar" title="Reuse Entry Info." />
            </Group>
            <Group title="Template{0}">
              <Boolean source="KeepLinesWhenPosting" />
              <Field source="TemplateName" title="Name" />
              <Field source="NewEntryDateWhenCopying" />
              <Field source="IntervalForCopying" />
              <Field source="NewTransactionNumberWhenCopying" />
              <Field source="NewTransactionTypeWhenCopying" />
              <Boolean source="InvertEntriesWhenCopying" />
            </Group>
            <Group title="Transaction Numbering">
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Field source="TransactionNumberSeries" />
            </Group>
            <Group title="Posting">
              <Field source="LayoutNameVar" title="Layout" />
            </Group>
          </Column>
          <Column>
            <Group title="Offsetting Entries">
              <Grid>
                <Header cells="_;|Total{Sum}|;|Balance{Saldo}|" />
                <Row cells="OffsetCode1;OffsetEntrySum1Var;OffsetEntryBalance1Var" />
                <Row cells="OffsetCode2;OffsetEntrySum2Var;OffsetEntryBalance2Var" />
                <Row cells="OffsetCode3;OffsetEntrySum3Var;OffsetEntryBalance3Var" />
                <Row cells="OffsetCode4;OffsetEntrySum4Var;OffsetEntryBalance4Var" />
                <Row cells="OffsetCode5;OffsetEntrySum5Var;OffsetEntryBalance5Var" />
              </Grid>
            </Group>
            <Group title="Batch Total">
              <Field source="EntryTotalControl" title="Control" />
              <Field source="EntryTotalCurrent" title="Current" />
              <Field source="EntryTotalDifferenceVar" title="Balance{Difference}" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
              <Field source="BaseCurrency" />
            </Group>
            <Group ref="DimensionGroup" />
            <Group title="Journal Reversion">
              <Boolean source="CreateRevJournal" title="Create Rev. Journal" />
              <Boolean source="PostRevJournal" title="Post Rev. Journal" />
              <Field source="ReversingEntryDate" title="New Entry Date" />
              <Field source="ReversingTransactionNumber" title="New Trans. No." />
              <Field source="ReversingTransactionType" title="New Trans. Type" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="CreateJournalWizardPage1">
        <Actions all="true" />
        <Group style="fullWidth">
          <Reference foreignKey="CompanyNumber_CompanyInformation" keyMandatory="true" />
          <Field source="DefaultTransactionType" title="Transaction Type" />
          <EmptyLabel />
          <Boolean source="KeepLinesWhenPosting" title="Template" />
          <Boolean source="CreateRevJournal" title="Reverse" />
        </Group>
      </Form>
      <Form name="CreateJournalWizardPage2">
        <Actions all="true" />
        <Group style="fullWidth">
          <Field source="TemplateName" title="Template Name" />
          <Field source="NewEntryDateWhenCopying" mandatory="true" />
          <Field source="IntervalForCopying" />
          <Field source="NewTransactionNumberWhenCopying" />
          <Field source="NewTransactionTypeWhenCopying" />
          <EmptyLabel />
          <Boolean source="InvertEntriesWhenCopying" />
        </Group>
      </Form>
      <Form name="CreateJournalWizardPage3">
        <Actions all="true" />
        <Group title="">
          <Field source="ReversingEntryDate" title="New Entry Date" mandatory="true" />
          <Field source="ReversingTransactionNumber" title="New Trans. No." />
          <Field source="ReversingTransactionType" title="New Trans. Type" />
          <EmptyLabel />
          <Boolean source="PostRevJournal" title="Post Reversing Journal" />
        </Group>
      </Form>
      <Form name="QuickRegistration">
        <Actions all="true">
          <Exclude>
            <Create />
          </Exclude>
        </Actions>
        <Row>
          <Column>
            <Group title="Journal{Draft}">
              <Field source="JournalNumber" style="textAlignment" />
              <If condition="NameOfUser != ''">
                <Description template="^1 by ^2" arguments="CreatedDate;NameOfUser" title="Created" />
              </If>
              <Else>
                <Description template="" title="Created" />
              </Else>
              <If condition="SubmittedBy != ''">
                <Description template="^1 by ^2" arguments="DateSubmitted;SubmittedBy" title="Submitted" />
              </If>
              <Else>
                <Description template="" title="Submitted" />
              </Else>
              <If condition="Period!=date(nulldate)">
                <Description template="^1 .. ^2" arguments="Period;PeriodEnd" title="Period" />
              </If>
              <Field source="FiscalYear" />
              <Description template="^1 ^2" arguments="Balance;BaseCurrency" title="Balance" />
            </Group>
            <Group title="Transaction Numbering" style="fullWidth">
              <Field source="DefaultTransactionType" title="Transaction Type" />
              <Boolean source="AutomaticVoucherVar" title="Auto Trans. No." />
              <Boolean source="ReuseEntryInformationVar" title="Reuse Entry Info." />
            </Group>
            <Group title="Posting">
              <Field source="LayoutNameVar" title="Layout" />
              <Field source="PostEntryPeriod" />
              <Boolean source="ToBePosted" />
            </Group>
          </Column>
          <Column>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
            </Group>
            <Group ref="DimensionGroup" />
            <If condition="OffsetCode1!=OffsetCodeType'nil                              or OffsetCode2!=OffsetCodeType'nil                                                  or OffsetCode3!=OffsetCodeType'nil                                                  or OffsetCode4!=OffsetCodeType'nil                                                  or OffsetCode5!=OffsetCodeType'nil">
              <Group title="Offsetting Entries">
                <Grid>
                  <Header cells="_;|Total{Sum}|;|Balance{Saldo}|" style="grid_header" />
                  <If condition="OffsetCode1!=OffsetCodeType'nil">
                    <Row cells="OffsetCode1;OffsetEntrySum1Var;OffsetEntryBalance1Var" />
                  </If>
                  <If condition="OffsetCode2!=OffsetCodeType'nil">
                    <Row cells="OffsetCode2;OffsetEntrySum2Var;OffsetEntryBalance2Var" />
                  </If>
                  <If condition="OffsetCode3!=OffsetCodeType'nil">
                    <Row cells="OffsetCode3;OffsetEntrySum3Var;OffsetEntryBalance3Var" />
                  </If>
                  <If condition="OffsetCode4!=OffsetCodeType'nil">
                    <Row cells="OffsetCode4;OffsetEntrySum4Var;OffsetEntryBalance4Var" />
                  </If>
                  <If condition="OffsetCode5!=OffsetCodeType'nil">
                    <Row cells="OffsetCode5;OffsetEntrySum5Var;OffsetEntryBalance5Var" />
                  </If>
                </Grid>
              </Group>
            </If>
          </Column>
        </Row>
      </Form>
      <Form name="Template">
        <Actions>
          <Update />
        </Actions>
        <Row>
          <Group title="" style="fullWidth">
            <Boolean source="KeepLinesWhenPosting" />
            <Boolean source="InvertEntriesWhenCopying" />
            <Field source="TemplateName" title="Name" size="medium" />
            <Field source="NewEntryDateWhenCopying" />
            <Field source="IntervalForCopying" />
            <Field source="NewTransactionNumberWhenCopying" />
            <Field source="NewTransactionTypeWhenCopying" />
          </Group>
        </Row>
      </Form>
      <Form name="ReverseJournal">
        <Actions>
          <Update />
        </Actions>
        <Row>
          <Group title="">
            <Boolean source="CreateRevJournal" title="Create Rev. Journal" />
            <Boolean source="PostRevJournal" title="Post Rev. Journal" />
            <Field source="ReversingEntryDate" title="New Entry Date" />
            <Field source="ReversingTransactionNumber" title="New Trans. No." />
            <Field source="ReversingTransactionType" title="New Trans. Type" />
          </Group>
        </Row>
      </Form>
      <Form name="BatchTotal">
        <Actions>
          <Update />
        </Actions>
        <Row>
          <Group title="">
            <Field source="EntryTotalControl" title="Control" />
            <Field source="EntryTotalCurrent" title="Current" />
            <Field source="EntryTotalDifferenceVar" title="Balance{Difference}" />
          </Group>
        </Row>
      </Form>
      <Form name="Offset">
        <Actions>
          <Update />
        </Actions>
        <Row>
          <Group title="">
            <Grid>
              <Header cells="_;|Total{Sum}|;|Balance{Saldo}|" style="grid_header" />
              <Row cells="OffsetCode1;OffsetEntrySum1Var;OffsetEntryBalance1Var" />
              <Row cells="OffsetCode2;OffsetEntrySum2Var;OffsetEntryBalance2Var" />
              <Row cells="OffsetCode3;OffsetEntrySum3Var;OffsetEntryBalance3Var" />
              <Row cells="OffsetCode4;OffsetEntrySum4Var;OffsetEntryBalance4Var" />
              <Row cells="OffsetCode5;OffsetEntrySum5Var;OffsetEntryBalance5Var" />
            </Grid>
          </Group>
        </Row>
      </Form>
    </UpperPane>
