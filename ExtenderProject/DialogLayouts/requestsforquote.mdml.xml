<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="RequestsForQuote">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="Request for Quote No. ^1" arguments="RequestForQuoteNumber" />
        </Compact>
        <Columns>
          <Field source="RequestForQuoteNumber" />
          <Field source="PurchaseOrderStatusCode" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="PurchaseProcessNumber" />
          <Field source="RequisitionNumber" />
          <Field source="SupplierNumber" />
          <Field source="Name1" />
          <Field source="Name2" />
          <Field source="Name3" />
        </Columns>
      </Filter>
      <Filter name="InVendorContext">
        <Compact>
          <Description template="Request for Quote No. ^1" arguments="RequestForQuoteNumber" />
        </Compact>
        <Columns>
          <Field source="RequestForQuoteNumber" />
          <Field source="PurchaseProcessNumber" />
          <Field source="RequisitionNumber" />
          <Field source="PurchaseOrderStatusCode" />
          <Field source="JobNumber" />
          <Field source="JobName" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Custom">
        <Actions all="not (workspace() like 'Vendors')                 and not (workspace() like 'CompanyVendors')" />
        <Column>
          <Group title="Request for Quote">
            <Field source="RequestForQuoteNumber" title="Request for Quote No." />
            <Field source="PurchaseOrderStatusCode" />
            <Field source="OurReference" title="Our Ref." />
            <Reference foreignKey="PurchaserEmployeeNumber_Employee" />
            <Reference foreignKey="RequisitionerEmployeeNumber_Employee" />
            <Pair firstSource="JobName" secondSource="JobNumber" secondSize="small" title="Job" />
        	<Field source="Purchaser" shadowTitle="On behalf of" />
            <Field source="VendorPopup5" title="Terms and Conditions" />
          </Group>
          <Group title="Quote">
            <Field source="QuoteExpiryDate" title="Expiry Date" />
          </Group>
          <Group title="Remarks for print">
            <Field source="Remark1" appearance="maximized" shadowTitle="Remark 1" >
            	<Style height="3" />
            </Field>
            <Field source="Remark2" appearance="maximized" shadowTitle="Remark 2" >
            	<Style height="3" />
            </Field>
            <Field source="Remark3" appearance="maximized" shadowTitle="Remark 3" >
            	<Style height="3" />
            </Field>
          </Group>
          <Group title="Price, Currency" style="textAlignment">
            <Field source="Currency" />
            <Field source="QuoteSumCurrency" title="Line Sum" />
            <Field source="QuoteDiscountPercentage" />
            <Field source="QuoteDiscountCurrency" title="Quote Discount" />
            <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
              <Field source="QuoteVATVar" title="Quote Total Tax" />
              <Field source="SumVAT1CurrencyVar" title="Quote Tax 1" />
              <Field source="SumVAT2CurrencyVar" title="Quote Tax 2" />
              <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                <Field source="SumVAT3CurrencyVar" title="Quote Tax 3" />
              </If>
            </If>
            <Else>
              <Field source="QuoteVATVar" title="Quote Tax" />
            </Else>
            <Field source="QuoteAmountVar" title="Quote Sum" />
          </Group>
          <Group title="Terms and Conditions">
            <Field source="VendorPopup5" title="Purchase Terms" size="vast" />
            <Field source="DeliveryTerms" title="Delivery Terms" size="vast" />
            <Field source="DeliveryMode" size="vast" />
            <Field source="PaymentTerms" title="Payment Terms" size="vast" />
            <Field source="PreferredDeliveryDate" title="Pref. Deliv. Date" size="vast" />
