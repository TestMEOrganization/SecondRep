<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobInvoiceOverview">
    <FilterPane>
      <Define>
        <Function name="IsCreditMemo" value="DebitCredit like 'Credit'" />
        <Function name="InvoiceOrCreditMemo" value="if(DebitCredit like 'Credit') then 'Credit Memo No.' else 'Invoice No.'" type="string" parameters="DebitCredit" />
        <Style name="HighlightCreditMemo" ref="highlightOppositeSign" condition="IsCreditMemo()" />
      </Define>
      <Filter name="Standard" style="HighlightCreditMemo">
      	<ControlBar>
      		<Selection>
      			<Option title="My Invoices" name="MyInvoices" restriction="ProjectManagerNumber like userEmployeeNumber()" />
      			<Option title="Department Invoices" name="DptInvoices" restriction="LocationName like locationName()" />
      			<Option title="All Invoices" name="AllInvoices" />
      			<!-- <Option title="My Draft Invoices" name="MyDraftInvoices" restriction="ProjectManagerNumber like userEmployeeNumber() and DraftCreatedByVar != '' and ApprovedbyVar != ''" />
      			<Option title="Department Draft Invoices" name="DptDraftInvoices" restriction="LocationName like locationName() and DraftCreatedByVar != '' and ApprovedbyVar != ''" />
      			<Option title="All Draft Invoices" name="AllDraftInvoices" restriction="DraftCreatedByVar != '' and ApprovedbyVar != ''" /> -->
      		</Selection>
      	</ControlBar>
        <Compact>
          <Description template="^1 ^2, Job: ^3 (^4), ^5" arguments="InvoiceOrCreditMemo(DebitCredit);InvoiceNumber;JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="InvoiceNumber" />
          <Field source="DebitCredit" />
          <Field source="InvoiceOnAccount" />
          <Field source="InvoiceDate" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="Currency" size="tiny" />
          <UnitField source="ItemSumCurrency" unitSource="Currency" size="small" />
          <UnitField source="InvoiceDiscountCurrency" unitSource="Currency" size="small" />
          <UnitField source="VatCurrency" unitSource="Currency" title="Tax" size="small" />
          <UnitField source="OnAccountItemSumCurrency" unitSource="Currency" title="Net on Account" size="small" />
          <UnitField source="OnAccountVATCurrency" unitSource="Currency" title="Tax on Account" size="small" />
          <UnitField source="TotalCurrency" unitSource="Currency" size="small" />
          <Field source="BasedOnInvoiceNumber" style="zeroSuppression" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
        <Function name="IsOverdue" value="PaymentStatusVar = 'Overdue'" />
        <Function name="IsSettled" value="PaymentStatusVar = 'Reconciled'" />
        <Function name="IsOpen" value="PaymentStatusVar = 'Due' or PaymentStatusVar = 'Not Reconciled'" />
        <Style name="AlertOverdue" ref="trafficlightRed" condition="IsOverdue()" />
        <Style name="AlertSettled" ref="trafficlightGreen" condition="IsSettled()" />
        <Style name="AlertOpen" ref="trafficlightYellow" condition="IsOpen()" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Billing Address">
              <Reference foreignKey="PaymentCustomer_Customer" />
              <Field source="PayerName2" appearance="maximized" />
              <Field source="PayerName3" appearance="maximized" />
              <ZipCity citySource="PayerPostalDistrict" zipSource="PayerZipCode" appearance="maximized" />
              <Field source="PayerName4" appearance="maximized" />
              <Field source="PayerName5" appearance="maximized" />
              <Field source="PayerAttention" title="Attn." />
              <Field source="PayerCountry" title="Country" />
              <Field source="PayerElectronicMailAddress" title="E-mail{native}" />
              <Field source="PayerTelephone" title="Phone" />
              <Field source="PayerTelefax" title="Fax" />
              <Field source="PayerTelex" title="Telex" />
              <Field source="PaymentCustomerDepartment" title="Department" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
            </Group>
            <Group title="Price">
              <!-- Invoice-->
              <Field source="Currency" title="Invoice Currency" />
              <Field source="ItemSumCurrency" title="Billing Price Total" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATCurrency" title="Total Tax" />
                <Field source="VAT1InvoiceCurrencyVar" title="Tax 1" />
                <Field source="VAT2InvoiceCurrencyVar" title="Tax 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VAT3InvoiceCurrencyVar" title="Tax 3" />
                </If>
              </If>
              <Else>
                <Field source="VATCurrency" title="Tax{VAT}" />
              </Else>
              <Field source="OnAccountItemSumCurrency" title="Net on Account" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATCurrency" title="Total Tax on Account, Currency" />
                <Field source="OnAccountVAT1CurrencyVar" title="Tax 1 on Account, Currency" />
                <Field source="OnAccountVAT2CurrencyVar" title="Tax 2 on Account, Currency" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3CurrencyVar" title="Tax 3 on Account, Currency" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATCurrency" />
              </Else>
              <Field source="TotalCurrency" />
              <Field source="VATExemptItemSumCurrency" />
              <Field source="InvoiceDiscountCurrency" />
              <Field source="JobInvoiceExchangeRate" />
              <!-- Job-->
              <Field source="JobCurrency" />
              <Field source="ItemSumJobCurrency" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATJobCurrency" title="Total Tax" />
                <Field source="VAT1CurrencyVar" title="Tax 1" />
                <Field source="VAT2CurrencyVar" title="Tax 2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VAT3CurrencyVar" title="Tax 3" />
                </If>
              </If>
              <Else>
                <Field source="VATJobCurrency" title="Tax{VAT}" />
              </Else>
              <Field source="TotalJobCurrency" />
              <Field source="JobInvoicedTimeJobCurrency" />
              <Field source="JobInvoicedAmountJobCurrency" />
              <!-- Basis-->
              <Field source="BaseCurrency" />
              <Field source="CostOfSales" title="Total Cost" />
              <Field source="ItemSumBase" title="Sales Price Total, Base" />
              <Field source="InvoiceDiscountBase" />
              <Field source="TotalBase" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATBase" title="Total Tax, Base" />
                <Field source="VAT1BaseVar" title="Tax 1, Base" />
                <Field source="VAT2BaseVar" title="Tax 2, Base" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VAT3BaseVar" title="Tax 3, Base" />
                </If>
              </If>
              <Else>
                <Field source="VATBase" />
              </Else>
              <Field source="VATExemptItemSumBase" />
              <Field source="OnAccountItemSumBase" title="Net on Account, Base" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATBase" title="Total Tax on Account, Base" />
                <Field source="OnAccountVAT1BaseVar" title="Tax 1 on Account, Base" />
                <Field source="OnAccountVAT2BaseVar" title="Tax 2 on Account, Base" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3BaseVar" title="Tax 3 on Account, Base" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATBase" />
              </Else>
              <Field source="AmountRoundingBase" />
              <Field source="ExchangeRateVarianceBase" />
              <!-- Enterprise-->
              <Field source="CostOfSalesEnterprise" title="Total Cost, Enterprise" />
              <Field source="ItemSumEnterprise" title="Billing Price Total, Enterprise" />
              <Field source="InvoiceDiscountEnterprise" />
              <Field source="TotalEnterprise" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATEnterprise" title="Total Tax, Enterprise" />
                <Field source="VAT1EnterpriseVar" title="Tax 1, Enterprise" />
                <Field source="VAT2EnterpriseVar" title="Tax 2, Enterprise" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VAT3EnterpriseVar" title="Tax 3, Enterprise" />
                </If>
              </If>
              <Else>
                <Field source="VATEnterprise" />
              </Else>
              <Field source="OnAccountItemSumEnterprise" title="Net On Account, Enterprise" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATEnterprise" title="Total Tax on Account, Enterprise" />
                <Field source="OnAccountVAT1EnterpriseVar" title="Tax 1 on Account, Enterprise" />
                <Field source="OnAccountVAT2EnterpriseVar" title="Tax 2 on Account, Enterprise" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3EnterpriseVar" title="Tax 3 on Account, Enterprise" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATEnterprise" />
              </Else>
              <Field source="AmountRoundingEnterprise" />
              <Field source="ExchangeRateVarianceBalancingEnterprise" />
              <Field source="ExchangeRateVarianceEnterprise" />
              <Field source="VATExemptItemSumEnterprise" title="Total Tax Exempt Billing Price, Enterprise" />
              <!-- Standard-->
              <Field source="TotalStandard" />
              <Field source="ExchangeRateVarianceStandardCurrency" />
              <Field source="InvoiceDiscountPercentage" />
              <Boolean source="VAT" />
              <Field source="ExchangeRateDate" />
            </Group>
          </Column>
          <Column>
            <Group title="Ship to Address">
              <Reference foreignKey="CustomerNumber_Customer" />
              <Field source="Name2" appearance="maximized" />
              <Field source="Name3" appearance="maximized" />
              <ZipCity citySource="PostalDistrict" zipSource="ZipCode" appearance="maximized" />
              <Field source="Name4" appearance="maximized" />
              <Field source="Name5" appearance="maximized" />
              <Field source="Attention" />
              <Field source="Country" />
              <Field source="ElectronicMailAddress" />
              <Field source="Telephone" />
              <Field source="Telefax" />
              <Field source="Telex" />
            </Group>
            <Group title="Settling Company">
              <Reference foreignKey="SettlingCompany_CompanyInformation" />
            </Group>
            <Group title="Invoice{noun}">
              <Field source="InvoiceNumber" />
              <Field source="JobNumber" />
              <Field source="JobName" />
              <Field source="InvoiceName" />
              <Field source="InvoiceDate" />
              <Field source="DueDate" />
              <Field source="DepartmentNumber" />
              <Field source="Reference" />
              <Field source="VATNumber" />
              <Field source="CustomerGroup" />
              <Field source="EndCustomerNumber" />
              <Field source="ActivityNumber" />
              <Field source="TaskName" />
              <Field source="Enterprise" />
              <Field source="Segment" />
              <Field source="Area" />
              <Field source="InternalRemark1" />
              <Field source="InternalRemark2" />
              <Field source="InternalRemark3" />
              <Field source="Appropriation" />
              <Field source="SalesPerson" />
              <Field source="SalesPersonNumber" />
              <Field source="PaymentTerms" />
              <Field source="ToBeBlanketInvoiced" />
              <Field source="CustomerPaymentMode" />
              <Field source="InvoiceOnAccount" />
              <Field source="LastDeliveryNoteNumber" />
              <Field source="BasedOnInvoiceNumber" />
              <Field source="Origin" />
              <Field source="CompanyVATCode" />
              <Field source="DebitCredit" />
              <Field source="Posted" />
              <Field source="InvoiceManualApproved" />
              <Boolean source="Printed" />
              <Field source="PrintedBy" />
              <Field source="PrintingDate" />
              <Field source="VersionNumber" />
            </Group>
            <Group ref="DimensionGroup" />
            <Group title="Customer{Debitor}">
              <Field source="CustomerPopup1" />
              <Field source="CustomerPopup2" />
              <Field source="CustomerPopup3" />
              <Field source="CustomerPopup4" />
              <Field source="CustomerPopup5" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Overview" title="Invoice">
        <Actions all="true" />
        <Column>
          <If condition="DebitCredit like 'Credit'">
            <Group title="Credit Memo" style="textAlignment">
              <Field source="InvoiceNumber" />
              <Field source="InvoiceDate" />
              <Field source="DueDate" />
              <Field source="DebitCredit" style="highlightOppositeSign" />
              <Field source="InvoiceTypeVar" />
              <Field source="ToBeBlanketInvoiced" />
            </Group>
          </If>
          <Else>
            <Group title="Invoice" style="textAlignment">
              <Field source="InvoiceNumber" />
              <Field source="InvoiceDate" />
              <Field source="DueDate" />
              <Field source="DebitCredit" />
              <Field source="InvoiceTypeVar" />
              <Field source="ToBeBlanketInvoiced" />
            </Group>
          </Else>
          <Group title="Job" style="textAlignment">
            <Reference foreignKey="JobNumber_JobHeader" />
            <Reference foreignKey="ProjectManager_Employee" />
          </Group>
        </Column>
        <Column>
          <Group title="Price Breakdown" style="compact_amountAlignment">
            <Field source="PaymentStatusVar" style="AlertOverdue,AlertSettled,AlertOpen" />
            <Field source="Currency" />
            <Field source="ItemSumCurrency" />
            <If condition="not InvoiceOnAccount">
              <Field source="InvoiceDiscountCurrency" />
            </If>
            <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
              <Field source="VATCurrency" title="Total Tax" />
              <Field source="VAT1InvoiceCurrencyVar" title="Tax 1" />
              <Field source="VAT2InvoiceCurrencyVar" title="Tax 2" />
              <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                <Field source="VAT3InvoiceCurrencyVar" title="Tax 3" />
              </If>
            </If>
            <Else>
              <Field source="VATCurrency" title="Tax{VAT}" />
            </Else>
            <If condition="InvoiceOnAccount">
              <Field source="ReconciledAmountCurrencyVar" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="ReconciledVATCurrencyVar" />
                <Field source="ReconciledVAT1CurrencyVar" />
                <Field source="ReconciledVAT2CurrencyVar" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="ReconciledVAT3CurrencyVar" />
                </If>
              </If>
              <Else>
                <Field source="ReconciledVATCurrencyVar" />
              </Else>
              <Field source="ReconciledVar" />
            </If>
            <Else>
              <Field source="OnAccountItemSumCurrency" title="Net on Account" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="OnAccountVATCurrency" title="Total Tax on Account, Currency" />
                <Field source="OnAccountVAT1CurrencyVar" title="Tax 1 on Account, Currency" />
                <Field source="OnAccountVAT2CurrencyVar" title="Tax 2 on Account, Currency" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="OnAccountVAT3CurrencyVar" title="Tax 3 on Account, Currency" />
                </If>
              </If>
              <Else>
                <Field source="OnAccountVATCurrency" title="Tax on Account" />
              </Else>
            </Else>
            <Field source="TotalCurrency" />
          </Group>
        </Column>
        <Column>
          <Group title="Workflow">
            <Description title="Draft Created" template="^1 by ^2" arguments="DraftCreatedDateVar;DraftCreatedByVar" />
            <Description title="Invoice Approved" template="^1 by ^2" arguments="ApprovalDateVar;ApprovedByVar" />
            <Description title="Invoice Printed" template="^1 by ^2" arguments="PrintingDate;PrintedBy" />
          </Group>
          <Group title="Contact Information">
            <Field source="PayerAttention" title="Name" />
            <Field source="PayerElectronicMailAddress" title="E-mail" />
            <Field source="PayerTelephone" title="Phone" />
            <Field source="PayerTelefax" title="Fax" />
          </Group>
        </Column>
        <Column>
          <Group title="Billing Address">
            <Reference foreignKey="PaymentCustomer_Customer" />
            <Field source="PayerName2" title="Address" />
            <Field source="PayerName3" title="" />
            <ZipCity citySource="PayerPostalDistrict" zipSource="PayerZipCode" title="" />
            <Field source="PayerAttention" title="Attn." />
            <Field source="PayerCountry" title="Country" />
          </Group>
        </Column>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
        <Function name="IsWrittenUp" value="BillingPriceUpDownCurrency gt 0" />
        <Function name="IsWrittenDown" value="BillingPriceUpDownCurrency lt 0" />
        <Style name="AlertWriteUp" ref="trafficlightGreen" condition="IsWrittenUp()" />
        <Style name="AlertWriteDown" ref="trafficlightRed" condition="IsWrittenDown()" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="JobNumberVar" title="Job No." />
          <Field source="EntryNumberVar" title="Entry No." />
          <Field source="JournalNumberVar" title="Journal No." />
          <Field source="JournalLineNumberVar" title="Journal Line No." />
          <Field source="PostingDateVar" title="Date Posted" />
          <Field source="TransactionNumberVar" title="Trans. No." />
          <Field source="TransactionTypeVar" title="Trans. Type" />
          <Field source="EntryDateVar" title="Date" />
          <Field source="ActivityNumberVar" title="Activity No." />
          <Field source="ActivityTypeVar" title="Activity Type" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="TaskNameVar" title="Task" />
          <Field source="TextVar" title="Description{Text}" />
          <Field source="EmployeeNumberVar" title="Employee No." />
          <Field source="CurrencyVar" title="Currency" />
          <Field source="OriginalCurrencyVar" title="Original Currency" />
          <Field source="BillingPriceCurrencyVar" title="Billing Price, Curr." />
          <Field source="BillingPriceRegCurrencyVar" title="Billing Price, Reg., Currency" />
          <Field source="BillingPriceOpenCurrencyVar" title="Open Sales Price, Currency" />
          <Field source="BillingPriceInvoicedCurrencyVar" title="Billing Price, Inv., Currency" />
          <Field source="BillingPriceTotalCurrencyVar" title="Billing Price, Total, Currency" />
          <Field source="BillingPriceForInvoicingCurrencyVar" title="Billing Price for Inv., Currency" />
          <Field source="BillingPriceUpDownCurrencyVar" title="Billing Price, Up/Down, Currency" />
          <Field source="BillingPriceUpDownUponCloseCurrencyVar" title="Billing Price Up/Down Upon Cl., Currency" />
          <Field source="CostPriceVar" title="Cost, Base" />
          <Field source="CostPriceRegVar" title="Cost, Reg. Base" />
          <Field source="CostPriceOpenVar" title="Open Cost, Base" />
          <Field source="CostPriceInvoicedVar" title="Cost, Inv., Base" />
          <Field source="CostPriceTotalVar" title="Cost, Total, Base" />
          <Field source="CostPriceForInvoicingVar" title="Cost for Inv., Base" />
          <Field source="IncomingCostPriceExclVATVar" title="Receivable Cost Excl. Tax, Base" />
          <Field source="IncomingCostPriceInclVATVar" title="Receivable Cost Incl. Tax, Base" />
          <Field source="BillingPriceBaseVar" title="Billing Price, Base" />
          <Field source="BillingPriceRegBaseVar" title="Billing Price, Reg., Base" />
          <Field source="BillingPriceOpenBaseVar" title="Open Sales Price, Base" />
          <Field source="BillingPriceInvoicedBaseVar" title="Billing Price, Inv., Base" />
          <Field source="BillingPriceTotalBaseVar" title="Billing Price, Total, Base" />
          <Field source="BillingPriceForInvoicingBaseVar" title="Billing Price for Inv., Base" />
          <Field source="BillingPriceUpDownBaseVar" title="Billing Price, Up/Down, Base" />
          <Field source="BillingPriceUpDownUponCloseBaseVar" title="Billing Price Up/Down Upon Cl., Base" />
          <Field source="CostPriceEnterpriseVar" title="Cost, Enterprise" />
          <Field source="CostPriceRegEnterpriseVar" title="Cost, Reg. Enterprise" />
          <Field source="CostPriceOpenEnterpriseVar" title="Open Cost, Enterprise" />
          <Field source="CostPriceForInvoicingEnterpriseVar" title="Cost for Inv., Enterprise" />
          <Field source="CostPriceInvoicedEnterpriseVar" title="Cost, Inv. Enterprise" />
          <Field source="CostPriceExecCompanyEnterpriseVar" title="Cost, Exec. Company, Enterprise" />
          <Field source="IncomingCostPriceEVATEnterpriseVar" title="Receivable Cost Excl. Tax, Enterprise" />
          <Field source="IncomingCostPriceIVATEnterpriseVar" title="Receivable Cost Incl. Tax, Enterprise" />
          <Field source="BillingPriceEnterpriseVar" title="Billing Price, Enterprise" />
          <Field source="BillingPriceRegEnterpriseVar" title="Billing Price, Reg., Enterprise" />
          <Field source="BillingPriceOpenEnterpriseVar" title="Open Billing Price, Enterprise" />
          <Field source="BillingPriceInvoicedEnterpriseVar" title="Billing Price, Inv., Enterprise" />
          <Field source="BillingPriceUpDownEnterpriseVar" title="Billing Price Up/Down, Enterprise" />
          <Field source="BillingPriceForInvoicingEnterpriseVar" title="Billing Price for Inv., Enterprise" />
          <Field source="BillingPriceUpDownUponCloseEnterpriseVar" title="Billing Price Up/Down Upon Cl., Enterprise" />
          <Field source="OriginalCostPriceVar" title="Original Cost" />
          <Field source="NumberOpenVar" title="Quantity Open" />
          <Field source="NumberInvoicedVar" title="Quantity Inv." />
          <Field source="NumberForInvoicingVar" title="Invoice Quantity" />
          <Field source="NumberUpDownVar" title="Quantity, Up/Down." />
          <Field source="NumberRegisteredVar" title="Quantity, Reg." />
          <Field source="NumberOfUpDownUponClosingVar" title="Quantity, Up/Down at Closing" />
          <Field source="BillingPriceExecutingCompanyVar" title="Billing Price, Exec. Company" />
          <Field source="BillingPriceExecCompanyEnterpriseVar" title="Billing Price, Exec. Company, Enterprise" />
          <Field source="CompanyNumberVar" title="Company No." />
          <Field source="ExecutingCompanyNumberVar" title="Exec. Company No." />
          <Field source="CostPriceExecutingCompanyVar" title="Cost, Exec. Company" />
          <Field source="BaseCurrencyExecutingCompanyVar" title="Base Currency, Exec. Company" />
          <Field source="StdBillingPriceCurrencyVar" title="Standard Billing Price, Currency" />
          <Field source="StdBillingPriceBaseVar" title="Standard Billing Price, Base" />
          <Field source="StdBillingPriceEnterpriseVar" title="Standard Billing Price, Enterprise" />
          <Field source="StdBillingPriceTotalCurrencyVar" title="Total Standard Billing Price, Currency{1}" />
          <Field source="StdBillingPriceTotalBaseVar" title="Standard Billing Price Total, Base" />
          <Field source="StdBillingPriceTotalEnterpriseVar" title="Standard Billing Price Total, Enterprise" />
          <Field source="StdUpDownCurrencyVar" title="Standard Up/Down, Currency" />
          <Field source="StdUpDownBaseVar" title="Standard Up/Down, Base" />
          <Field source="StdUpDownEnterpriseVar" title="Standard Up/Down, Enterprise" />
          <Field source="AccrualEnterpriseVar" title="Accrual, Enterprise" />
          <Field source="TypeOfEntryVar" title="Entry Type{Posting}" />
          <Field source="MarkUpPercentageVar" title="Markup %" />
          <Field source="ToBeClosedVar" title="Close{Finish}" />
          <Field source="ClosedVar" title="Closed{Finish}" />
          <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
            <Field source="VATCodeVar" title="Tax Code 1" />
            <Field source="VATCode2Var" title="Tax Code 2" />
            <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
              <Field source="VATCode3Var" title="Tax Code 3" />
