<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="Activities">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2)" arguments="ActivityText;ActivityNumber" />
        </Compact>
        <Columns>
          <Field source="ActivityNumber" />
          <Field source="ActivityText" />
          <Field source="ActivityType" />
          <Field source="CostType" />
          <Field source="ToBeInvoiced" />
          <Field source="ExternalInvoice" />
          <Field source="Blocked" />
          <Field source="CostPrice" />
          <Field source="IntercompanyPrice" />
          <Field source="BillingPrice" />
        </Columns>
      </Filter>
      <Filter name="Custom">
        <Compact>
          <Description template="^1 (^2)" arguments="ActivityText;ActivityNumber" />
        </Compact>
        <Columns>
          <Field source="ActivityNumber" />
          <Field source="ActivityText" />
          <Field source="ActivityType" />
          <Field source="CostType" />
          <Field source="ToBeInvoiced" />
          <Field source="ExternalInvoice" />
          <Field source="Blocked" />
          <Field source="CostPrice" />
          <Field source="IntercompanyPrice" />
          <Field source="BillingPrice" />
        </Columns>
      </Filter>
      <Filter name="MinimalWithLink">
        <Columns>
          <Field source="ActivityNumber" />
          <Field source="ActivityText" />
          <Field source="ActivityType" />
          <Field source="CostType" />
          <Field source="Blocked" />
          <Link workspace="JobsSetup" template="More details..." title=" " icon="WorkspaceLink">
            <Waypoint pane="Dimensions" />
            <Target pane="Activities">
              <Restriction template="Selected Activity" expression="ActivityNumber=target.ActivityNumber" />
              <Focus>
                <Match field="ActivityNumber" value="ActivityNumber" />
              </Focus>
            </Target>
          </Link>
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Activity">
              <Field source="ActivityNumber" />
              <Field source="ActivityText" />
              <Field source="ActivityType" />
              <Field source="CostType" />
              <Boolean source="ToBeInvoiced" />
              <Boolean source="ExternalInvoice" />
              <Field source="EmployeeUtilization" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
              <Field source="BaseCurrencyVar" title="Base Currency" />
            </Group>
            <Group title="Dimension Combination">
              <Reference foreignKey="DimensionCombNumber_DimensionCombHeader" />
            </Group>
            <Group ref="DerivedDimensionGroup" />
            <Group title="Creation">
              <Field source="CreateFromActivityNumberVar" title="Create from Activity No." />
            </Group>
            <Group title="Outlay Settlement">
              <Boolean source="SettleViaVendor" />
            </Group>
            <Group title="Sales Orders{General}">
              <Boolean source="CreateSalesOrder" title="Create Order{Activities}" />
            </Group>
            <Group title="Print Control">
              <Field source="Statistic1" />
              <Field source="Statistic2" />
              <Field source="Statistic3" />
              <Field source="Statistic4" />
            </Group>
            <Group title="Access Level">
              <Reference foreignKey="AccessLevelName_AccessLevelHeader" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            </Group>
            <Group title="Activity Usage">
              <Field source="AllowTimeRegistration" />
              <Field source="AllowExpenseRegistration" />
              <Field source="AllowMileageRegistration" />
              <Field source="AllowPurchasing" />
              <Field source="AllowInvoicingOnAccount" />
              <Field source="AllowInvoicingPlan" />
            </Group>
            <Group title="Remarks{pl}">
              <Field source="Remark1" appearance="maximized" />
              <Field source="Remark2" appearance="maximized" />
              <Field source="Remark3" appearance="maximized" />
            </Group>
          </Column>
          <Column>
            <Group title="Status">
              <Boolean source="Blocked" />
            </Group>
            <Group title="Rates">
              <Field source="CostPrice" />
              <Field source="IntercompanyPrice" />
              <Field source="BillingPrice" />
              <Field source="MarkUpPercentage" />
              <Field source="StandardBillingPrice" />
              <Field source="StandardMarkUpPercentage" />
              <Reference foreignKey="ItemNumber_ItemInformation" />
              <Reference foreignKey="JobPriceGroupNumber_JobPriceGroup" />
              <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
                <Field source="VATCode" />
                <Field source="VATCode2" />
                <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                  <Field source="VATCode3" />
                </If>
              </If>
              <Else>
                <Field source="VATCode" title="Tax Code" />
              </Else>
              <Field source="ItemVATCode" />
            </Group>
            <Group title="Posting">
              <Field source="JobPostingReferenceName" />
              <Field source="JobPostingReferenceDescriptionVar" title="Description" />
            </Group>
            <Group title="Registrations">
              <Field source="MaxTimeSheetRegPerDay" title="Limit, Time Sheets" />
              <Field source="MaxExpenseSheetRegPerDay" title="Limit, Expense Sheets" />
              <Boolean source="EmployeeRequired" />
              <Boolean source="NumberToQuantityA" title="Quantity 1 from Quantity on Expense Sheet" />
              <Boolean source="NumberToQuantityB" title="Quantity 2 from Quantity on Expense Sheet" />
              <Field source="ExpenseJustificationHelpText" />
              <Field source="AccrualType" />
            </Group>
            <Group title="Texts">
              <Field source="JobBudgetLineTexts" />
              <Field source="TimeAndExpenseSheetLineTexts" />
              <Field source="ExternalCostLineTexts" />
              <Field source="OnAccountInvoicingLineTexts" />
            </Group>
            <Group title="Standard Job Budget Text">
              <Reference foreignKey="StandardJobTextName_StandardJobTextInformation" title="Name" />
            </Group>
            <Group title="Standard Note">
              <Reference foreignKey="NoteNumber_NoteHeader" />
            </Group>
            <Group title="Defaults for Tasks">
              <Boolean source="DefaultIncludedInFixedPrice" title="Included In Fixed Price" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
        </Row>
