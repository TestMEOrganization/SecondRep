<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="AccessInformation">
    <UpperPane>
      <Form name="Full">
        <Actions all="true" />
        <Column>
          <Group title="Password">
            <Field source="PasswordLength" />
            <Field source="NumberOfLetters" />
            <Field source="NumberOfDigits" />
            <Field source="NumberOfSpecialCharacters" />
            <Field source="NumberOfRepeats" />
            <Field source="NumberBetweenRepetition" />
            <Field source="NumberOfDaysBeforeCodeShift" />
            <Field source="StandardPassword" />
          </Group>
          <Group ref="UserUpdateGroup" />
        </Column>
      </Form>
    </UpperPane>
  </Layout>
</MDML>

