<!-- JSV, 14/07/10: Search layout generated from specification on entity (38662).-->
<!-- SBA, 01/12/10: Adding default filters (39133).-->
<!-- JSV, 09/12/10: New view For_JobCollection (39208).-->
<!-- JSV, 21/06/11: Field size in compact layout (39464).-->
<!-- PSZ, 03/08/11: New view For_JobJournal - copy of Standard with AllJobs selected (39464).-->
<!-- PSZ, 21/10/11: New view TemplateJobSelection (40048).-->
<!-- PSZ, 21/10/11: New view ExcludeTemplateJobs for use in ReportCriteria (40072).-->
<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="Find_JobHeader">
    <FilterPane>
      <Filter name="Standard">
        <ControlBar>
          <Selection default="MyJobs">
            <Option title="My Jobs" name="MyJobs" restriction="ProjectManagerNumber = UserEmployeeNumber()" />
            <Option title="Department Jobs" name="DepartmentJobs" restriction="if (UserEmployeeNumber() != '') then (LocationName = stringEnvVar('user.employeeinfo.EntityName') and Template = false) else false" />
            <Option title="All Jobs" name="AllJobs" />
          </Selection>
        </ControlBar>
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          	<Field source="ProjectManagerName" title="Project Manager" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="LocationName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="ProjectManagerName" title="Project Manager" />
          <Field source="Responsible" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
      <Filter name="For_JobCollection">
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobCollectionNumber" />
          <Field source="OnAccountNetCurrency" />
          <Field source="ProjectManagerName" title="Project Manager" />
        </Columns>
      </Filter>
      <Filter name="For_JobJournal">
        <ControlBar>
          <Selection default="AllJobs">
            <Option title="My Jobs" name="MyJobs" restriction="ProjectManagerNumber = UserEmployeeNumber()" />
            <Option title="Department Jobs" name="DepartmentJobs" restriction="if (UserEmployeeNumber() != '') then (LocationName = stringEnvVar('user.employeeinfo.EntityName') and Template = false) else false" />
            <Option title="All Jobs" name="AllJobs" />
          </Selection>
        </ControlBar>
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="ProjectManagerName" title="Project Manager" />
          <Field source="Responsible" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
      <Filter name="TemplateJobSelection">
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
      <Filter name="ExcludeTemplateJobs">
        <ControlBar>
          <Selection default="MyJobs" restriction="Template = false">
            <Option title="My Jobs" name="MyJobs" restriction="ProjectManagerNumber = UserEmployeeNumber()" />
            <Option title="Department Jobs" name="DepartmentJobs" restriction="if (UserEmployeeNumber() != '') then (LocationName = stringEnvVar('user.employeeinfo.EntityName') and Template = false) else false" />
            <Option title="All Jobs" name="AllJobs" />
          </Selection>
        </ControlBar>
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="ProjectManagerName" title="Project Manager" />
          <Field source="Responsible" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
      <Filter name="Reallocation">
        <ControlBar>
          <Selection default="MyJobs" restriction="Template = false">
            <Option title="My Jobs" name="MineOnly" restriction="ProjectManagerNumber = UserEmployeeNumber()" />
          </Selection>
        </ControlBar>
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="ProjectManagerName" title="Project Manager" />
          <Field source="Responsible" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
      <Filter name="ReallocationUnlimited">
        <ControlBar>
          <Selection default="MyJobs" restriction="Template = false">
            <Option title="All Jobs" name="AllJobs" />
          </Selection>
        </ControlBar>
        <Compact>
          <Columns>
            <Field source="JobNumber" />
            <Field source="JobName" size="large" />
            <Field source="Name1" title="Customer Name" size="large" />
          </Columns>
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="CustomerNumber" />
          <Field source="Name1" title="Customer Name" />
          <Field source="ProjectManagerName" title="Project Manager" />
          <Field source="Responsible" />
          <Field source="CompanyNumber" />
          <Field source="MainJobNumber" />
          <Field source="JobGroup" />
        </Columns>
      </Filter>
    </FilterPane>
  </Layout>
</MDML>

