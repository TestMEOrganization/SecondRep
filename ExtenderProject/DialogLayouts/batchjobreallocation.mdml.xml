              <Interval firstSource="FromItemNumber" lastSource="ToItemNumber" title="Item No." />
              <Interval firstSource="FromActivityNumber" lastSource="ToActivityNumber" title="Activity No." />
              <Field source="ActivityType" title="Activity Type" appearance="interval" />
              <Interval firstSource="FromTaskName" lastSource="ToTaskName" title="Task" />
              <Field source="OvertimeType" title="Overtime Spec." appearance="interval" />
              <Field source="AccrualType" title="Accrual Type" appearance="interval" />
              <Interval firstSource="FromFromAccrualPeriod" lastSource="ToFromAccrualPeriod" title="Accrual Period From" />
              <Interval firstSource="FromToAccrualPeriod" lastSource="ToToAccrualPeriod" title="Accrual Period To" />
              <Interval firstSource="FromReallocatedFromJobNumber" lastSource="ToReallocatedFromJobNumber" title="Reallocated from Job No." />
              <Interval firstSource="FromReallocatedFromTaskName" lastSource="ToReallocatedFromTaskName" title="Reallocated from Task" />
              <Boolean source="ReallocateOnlyOpenJobEntries" title="Open Only{pl}" />
              <Boolean source="ShowOnlyMarkedJobEntriesVar" title="Marked Only" />
            </Group>
          </Column>
          <Column>
            <Group title="Reallocate To">
              <Field source="NewJobNumber" title="Job No." />
              <Field source="NewTransactionNumber" title="Trans. No." />
              <Field source="DefaultTransactionType" title="Trans. Type" />
              <Field source="TransactionNumberSeries" title="Number Series{sg}" />
              <Field source="NewEntryDateVar" title="Entry Date" />
              <Reference foreignKey="NewItemNumber_ItemInformation" />
              <Reference foreignKey="NewActivityNumber_Activity" />
              <Field source="NewTaskName" title="Task" />
              <Field source="NewTaskTextVar" title="Task Description{1}" />
              <Field source="NewOvertimeType" title="Overtime Spec." />
              <Boolean source="NewNoOvertimeType" title="No overtime spec." />
              <Field source="NewAccrualType" title="Accrual Type" />
              <Boolean source="NewNoAccrualType" title="No accrual type" />
              <Field source="NewFromAccrualPeriod" title="Accrual Period From" />
              <Field source="NewToAccrualPeriod" title="Accrual Period To" />
            </Group>
          </Column>
        </Row>
        <Row>
          <Column>
            <Group ref="DimensionsTargetGroupIntervals" />
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="JobNumber" />
          <Field source="Reallocate" />
          <Field source="EntryDate" title="Entry Date" />
          <Field source="TransactionNumber" />
          <Field source="TransactionType" title="Trans. Type" />
          <Field source="JournalNumber" />
          <Field source="TaskName" />
          <Field source="TaskTextVar" title="Task Description{1}" />
          <Field source="ActivityNumber" />
          <Field source="ActivityText" />
          <Field source="EmployeeNumber" title="Empl." />
          <Field source="EmployeeNameVar" title="Employee Name" />
          <Field source="NumberRegistered" title="Quantity Registered" />
          <Field source="NumberTransferredTo" title="Quantity Transferred To" />
          <Field source="NumberOpen" />
          <Field source="NumberReallocated" title="Quantity Reallocated" />
          <Field source="ReallocationDate" title="Reallocation Date" />
          <Field source="ReallocatedFromJobNumber" title="Reallocated from Job" />
          <Field source="ReallocatedFromTaskName" />
          <Field source="ItemNumber" />
