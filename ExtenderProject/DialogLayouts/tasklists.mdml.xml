<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="TaskLists">
    <FilterPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Filter name="Standard">
        <Compact>
          <Description template="^1, ^2" arguments="TaskList;Description" />
        </Compact>
        <Columns>
          <Field source="TaskList" />
          <Field source="Description" />
          <Field source="ActivityNumber" />
          <Field source="JobNumber" />
          <Field source="TaskName" />
          <Field source="RevenueRecognitionTaskName" />
          <Field source="TimeBudgetingTaskName" />
          <Field source="AmountBudgetingTaskName" />
          <Field source="MileageTaskName" />
          <Field source="InvoicingPlanTaskName" />
          <Field source="JobInvoiceOnAccountTaskName1" />
          <Field source="JobInvoiceOnAccountTaskName2" />
          <Field source="WriteUpDownInvoiceSelectionTaskName" />
          <Field source="WriteUpDownJobInvAllocationTaskName" />
          <Field source="DimensionCombNumber" />
          <Field source="AccessLevelName" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Wizard name="CopyTaskListWizard" title="Copy Task List">
        <Page form="CopyTaskListWizardPage1" title="Select Task List to Copy" description="Choose a task list to copy from" />
      </Wizard>
      <Form name="Full">
        <Actions all="true">

          <Action source="CopyTaskList" wizard="CopyTaskListWizard" />
          <Action source="MDK_CopyTaskList" title="Copy Tasks to Budget" />
        </Actions>
        <Row>
          <Column>
            <Group title="Task List">
              <Field source="TaskList" title="Name" />
              <Field source="Description" />
            </Group>
            <Group title="Access Level">
              <Reference foreignKey="AccessLevelName_AccessLevelHeader" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            </Group>
            <Group title="Daily Descriptions">
              <Boolean source="UseDailyDescriptions" />
            </Group>
            <Group title="Dimension Combination">
              <Reference foreignKey="DimensionCombNumber_DimensionCombHeader" />
            </Group>
            <Group title="Derived Activity">
              <Grid>
                <Header cells="|Value|;|Overwrite|;|Description|" title="" />
                <Row cells="ActivityNumber;OverwriteActivity;DerivedActivityTextVar" title="Activity No." />
              </Grid>
            </Group>
            <Group ref="DerivedDimensionGroup" />
          </Column>
          <Column>
            <Group title="Job">
              <Field source="JobSpecificVar" title="Job Specific" />
              <Reference foreignKey="JobNumber_JobHeader" />
            </Group>
            <Group title="Standard Tasks">
              <Field source="TimeBudgetingTaskName" />
              <Field source="AmountBudgetingTaskName" />
              <Field source="TaskName" title="Registration" />
              <Field source="RevenueRecognitionTaskName" title="Revenue Recognition" />
              <Field source="MileageTaskName" title="Mileage" />
              <Field source="InvoicingPlanTaskName" title="Invoicing Plan" />
              <Field source="JobInvoiceOnAccountTaskName1" title="Invoicing On Account 1" />
              <Field source="JobInvoiceOnAccountTaskName2" title="Invoicing On Account 2" />
              <Field source="WriteUpDownInvoiceSelectionTaskName" title="Write Up/Down, Invoice Selection" />
              <Field source="WriteUpDownJobInvAllocationTaskName" title="Write Up/Down, Job Inv. Allocation" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
        </Row>
      </Form>
      <Form name="CopyTaskListWizardPage1">
        <Actions all="true" />
        <Group>
          <Field source="CopyFromTaskListVar" title="Task List" mandatory="true" />
          <Boolean source="OverwriteAtDuplicateVar" title="Overwrite when duplicated" />
          <Boolean source="OverwriteDimensionDerivationsVar" title="Overwrite Derived Dimensions" />
        </Group>
      </Form>
      <Form name="Demo">
        <Actions all="true">
          <Exclude>
            <Action source="MarkAllForDailyDescriptions" />
            <Action source="UnmarkAllForDailyDescriptions" />
          </Exclude>
          <Action source="CopyTaskList" wizard="CopyTaskListWizard" />
          <Action source="MDK_CopyTaskList" title="Copy Tasks to Budget" />
        </Actions>
        <Row>
          <Column>
            <Group title="Task List">
              <Field source="TaskList" title="Name" />
              <Field source="Description" />
            </Group>
            <Group title="Access Level">
              <Reference foreignKey="AccessLevelName_AccessLevelHeader" keyShadowTitle="Name" descriptionShadowTitle="Description" />
            </Group>
            <Group title="Daily Descriptions">
              <Boolean source="UseDailyDescriptions" />
            </Group>
            <Group title="Dimension Combination">
              <Reference foreignKey="DimensionCombNumber_DimensionCombHeader" />
            </Group>
            <Group title="Derived Activity">
              <Grid>
                <Header cells="|Value|;|Overwrite|;|Description|" title="" />
                <Row cells="ActivityNumber;OverwriteActivity;DerivedActivityTextVar" title="Activity No." />
              </Grid>
            </Group>
            <Group ref="DerivedDimensionGroup" />
          </Column>
          <Column>
            <Group title="Job">
              <Field source="JobSpecificVar" title="Job Specific" />
              <Reference foreignKey="JobNumber_JobHeader" />
            </Group>
            <Group title="Standard Tasks">
              <Field source="TimeBudgetingTaskName" />
              <Field source="AmountBudgetingTaskName" />
              <Field source="TaskName" title="Registration" />
              <Field source="RevenueRecognitionTaskName" title="Revenue Recognition" />
              <Field source="MileageTaskName" title="Mileage" />
              <Field source="InvoicingPlanTaskName" title="Invoicing Plan" />
              <Field source="JobInvoiceOnAccountTaskName1" title="Invoicing On Account 1" />
              <Field source="JobInvoiceOnAccountTaskName2" title="Invoicing On Account 2" />
              <Field source="WriteUpDownInvoiceSelectionTaskName" title="Write Up/Down, Invoice Selection" />
              <Field source="WriteUpDownJobInvAllocationTaskName" title="Write Up/Down, Job Inv. Allocation" />
            </Group>
            <Group ref="UserUpdateGroup" />
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="TaskName" title="Task" />
          <Field source="Description" />
          <Field source="Blocked" size="small" />
          <Field source="ActivityNumber" title="Derived Act. No." />
          <Field source="OverwriteActivity" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="JobPriceGroupNumber" />
          <If condition="Parent.CanEditIncludedInFixedPriceVar">
            <Field source="IncludedInFixedPrice" />
          </If>
          <Else>
            <Field source="IncludedInFixedPrice" open="false" />
          </Else>
          <Field source="AllowTimeRegistration" />
          <Field source="AllowExpenseRegistration" />
          <Field source="AllowMileageRegistration" />
          <Field source="AllowPurchasing" />
          <Field source="AllowInvoicingOnAccount" />
          <Field source="AllowInvoicingPlan" />
          <Field source="AccessLevelName" />
          <Field source="BudgetUpdateAccessLevelName" />
          <Field source="UseDailyDescriptions" />
          <Field source="Approved" />
          <Field source="ApprovedBy" />
          <Field source="ApprovalDate" />
          <Field source="NoteNumber" title="Standard Note No." />
          <Field source="NoteDescriptionVar" title="Note Description" />
          <Field source="Statistic1" />
          <Field source="Statistic2" />
          <Field source="Statistic3" />
          <Field source="Statistic4" />
          <Field source="DimensionCombNumber" title="Dim. Comb. No." />
          <Columns ref="DimensionNameOverwriteColumns" />
        </Columns>
      </Table>
      <Table name="Demo">
        <Actions all="true">
          <Action source="parent.MarkAllForDailyDescriptions" />
          <Action source="parent.UnmarkAllForDailyDescriptions" />
           <Action source="parent.MDK_CopyTaskList" title="Copy tasks to Budget" />
        </Actions>
        <Columns>
          <Field source="TaskName" title="Task" />
          <Field source="Description" />
          <Field source="Blocked" size="small" />
          <Field source="ActivityNumber" title="Derived Act. No." />
          <Field source="OverwriteActivity" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="JobPriceGroupNumber" />
          <If condition="Parent.CanEditIncludedInFixedPriceVar">
            <Field source="IncludedInFixedPrice" />
          </If>
          <Else>
            <Field source="IncludedInFixedPrice" open="false" />
          </Else>
          <Field source="AllowTimeRegistration" />
          <Field source="AllowExpenseRegistration" />
          <Field source="AllowMileageRegistration" />
          <Field source="AllowPurchasing" />
          <Field source="AllowInvoicingOnAccount" />
          <Field source="AllowInvoicingPlan" />
          <Field source="AccessLevelName" />
          <Field source="BudgetUpdateAccessLevelName" />
          <Field source="UseDailyDescriptions" />
          <Field source="Approved" />
          <Field source="ApprovedBy" />
          <Field source="ApprovalDate" />
          <Field source="NoteNumber" title="Standard Note No." />
          <Field source="NoteDescriptionVar" title="Note Description" />
          <Field source="Statistic1" />
          <Field source="Statistic2" />
          <Field source="Statistic3" />
          <Field source="Statistic4" />
          <Field source="DimensionCombNumber" title="Dim. Comb. No." />
          <Columns ref="DimensionNameOverwriteColumns" />
        </Columns>
      </Table>
    </LowerPane>
  </Layout>
</MDML>

