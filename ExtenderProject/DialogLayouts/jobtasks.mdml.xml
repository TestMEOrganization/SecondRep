<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="JobTasks">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobGroup" />
          <Field source="NotInvoiceable" />
          <Field source="InternalJob" />
          <Field source="Status" />
          <Field source="ConvertedBy" />
          <Field source="ConversionDate" />
          <Field source="ProjectManagerNumber" />
          <Field source="SalesPersonNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Wizard name="CopyJobTasksWizard" title="Copy Job Tasks">
        <Page form="CopyJobTasksWizardPage1" title="Select a Job for Copying Tasks" description="Choose a job to copy tasks from" />
      </Wizard>
      <Form name="Full">
        <Actions all="true">
          <Action source="CopyJobTasks" wizard="CopyJobTasksWizard" />
          <Action source="MDK_CopyTaskList" title="Copy Tasks to Budget" icon="Dot" />
        </Actions>
        <Row>
          <Column>
            <Group title="Job!">
              <Reference foreignKey="JobNumber_JobHeader" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
            </Group>
            <Group title="Daily Descriptions">
              <Boolean source="UseDailyDescriptions" />
            </Group>
            <Group title="Tasks">
              <If condition="TaskListHeaderCreatedByVar != ''">
                <Description template="^1 by ^2" arguments="TaskListHeaderCreationDateVar;TaskListHeaderCreatedByVar" title="Created" />
              </If>
              <Else>
                <Description template="" title="Created" />
              </Else>
              <If condition="TaskListHeaderChangedByVar != ''">
                <Description template="^1 by ^2" arguments="TaskListHeaderChangedDateVar;TaskListHeaderChangedByVar" title="Changed" />
              </If>
              <Else>
                <Description template="" title="Changed" />
              </Else>
              <Field source="TaskListHeaderVersionNumberVar" title="List Version No." />
            </Group>
          </Column>
          <Column>
            <Group title="Customer">
              <Reference foreignKey="CustomerNumber_Customer" descriptionSource="Name1" />
            </Group>
            <Group title="Standard Tasks">
              <Field source="TaskListHeaderTimeBudgetingTaskNameVar" />
              <Field source="TaskListHeaderAmountBudgetingTaskNameVar" />
              <Field source="TaskListHeaderTaskNameVar" title="Registration" />
              <Field source="TaskListHeaderRevenueRecognitionTaskNameVar" title="Revenue Recognition" />
              <Field source="TaskListHeaderMileageTaskNameVar" title="Mileage" />
              <Field source="TaskListHeaderInvoicingPlanTaskNameVar" title="Invoicing Plan" />
              <Field source="TaskListHeaderInvoiceOnAccountTaskName1Var" title="Invoicing On Account 1" />
              <Field source="TaskListHeaderInvoiceOnAccountTaskName2Var" title="Invoicing On Account 2" />
              <Field source="TaskListHeaderWriteUpDownInvSelectionVar" title="Write Up/Down, Invoice Selection" />
              <Field source="TaskListHeaderWriteUpDownJobInvAllocVar" title="Write Up/Down, Job Inv. Allocation" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="CopyJobTasksWizardPage1">
        <Actions all="true" />
        <Group>
          <Reference foreignKey="Card_CopyFromJobVar_JobHeader" keyMandatory="true" />
          <Boolean source="OverwriteAtDuplicateVar" title="Overwrite when duplicated" />
          <!-- *** RambÃ¸ll customization ***- -->
          <!-- <Boolean source="OverwriteDimensionDerivationsVar" title="Overwrite Derived Dimensions" /> -->
        </Group>
      </Form>
      <Form name="Standard">
        <Actions all="true">
          <Exclude>
            <If condition="hasWorkspace('Jobs')">
              <Action source="MarkAllForDailyDescriptions" />
              <Action source="UnmarkAllForDailyDescriptions" />
            </If>
          </Exclude>
          <Action source="CopyJobTasks" wizard="CopyJobTasksWizard" />
          <Action source="MDK_CopyTaskList" title="Copy Tasks to Budget"/>
        </Actions>
        <Row>
          <Column>
            <Group title="Standard Tasks">
              <Field source="TaskListHeaderTimeBudgetingTaskNameVar" />
              <Field source="TaskListHeaderAmountBudgetingTaskNameVar" />
              <Field source="TaskListHeaderTaskNameVar" title="Registration" />
              <Field source="TaskListHeaderRevenueRecognitionTaskNameVar" title="Revenue Recognition" />
              <Field source="TaskListHeaderMileageTaskNameVar" title="Mileage" />
              <Field source="TaskListHeaderInvoicingPlanTaskNameVar" title="Invoicing Plan" />
              <Field source="TaskListHeaderInvoiceOnAccountTaskName1Var" title="Invoicing On Account 1" />
              <Field source="TaskListHeaderInvoiceOnAccountTaskName2Var" title="Invoicing On Account 2" />
              <Field source="TaskListHeaderWriteUpDownInvSelectionVar" title="Write Up/Down, Invoice Selection" />
              <Field source="TaskListHeaderWriteUpDownJobInvAllocVar" title="Write Up/Down, Job Inv. Allocation" />
            </Group>
          </Column>
          <Column>
            <Group title="Daily Descriptions">
              <Boolean source="UseDailyDescriptions" />
            </Group>
            <Group title="User" name="UserUpdateGroup">
              <If condition="TaskListHeaderCreatedByVar != ''">
                <Description template="^1 by ^2" arguments="TaskListHeaderCreationDateVar;TaskListHeaderCreatedByVar" title="Created" />
              </If>
              <Else>
                <Description template="" title="Created" />
              </Else>
              <If condition="TaskListHeaderChangedByVar != ''">
                <Description template="^1 by ^2" arguments="TaskListHeaderChangedDateVar;TaskListHeaderChangedByVar" title="Changed" />
              </If>
              <Else>
                <Description template="" title="Changed" />
              </Else>
              <Field source="TaskListHeaderVersionNumberVar" title="List Version No." />
            </Group>
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="TaskName" title="Task!" />
          <Field source="Description" />
          <Field source="Blocked" size="small" />
          <Field source="ActivityNumber" title="Derived Act. No." />
          <Field source="OverwriteActivity" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="JobPriceGroupNumber" />
          <If condition="Parent.CanEditIncludedInFixedPriceVar">
            <Field source="IncludedInFixedPrice" />
          </If>
          <Else>
            <Field source="IncludedInFixedPrice" open="false" />
          </Else>
          <Field source="AllowTimeRegistration" />
          <Field source="AllowExpenseRegistration" />
          <Field source="AllowMileageRegistration" />
          <Field source="AllowPurchasing" />
          <Field source="AllowInvoicingOnAccount" />
          <Field source="AllowInvoicingPlan" />
          <Field source="AccessLevelName" />
          <Field source="BudgetUpdateAccessLevelName" />
          <Field source="UseDailyDescriptions" />
          <Field source="Approved" />
          <Field source="ApprovedBy" />
          <Field source="ApprovalDate" />
          <Field source="NoteNumber" title="Standard Note No." />
          <Field source="NoteDescriptionVar" title="Note Description" />
          <Field source="TaskGroup1" size="medium" />
          <Field source="TaskGroup2" size="medium" />
          <Field source="TaskGroup3" size="medium" />
          <Field source="TaskGroup4" size="medium" />
          <Field source="TaskGroup5" size="medium" />
          <Field source="TaskGroup6" size="medium" />
          <Field source="DimensionCombNumber" title="Dim. Comb. No." />
          <Columns ref="DimensionNameOverwriteColumns" />
        </Columns>
      </Table>
      
      <Table name="Standard">
        <Actions all="true">
          <If condition="hasWorkspace('Jobs')">
            <Action source="parent.MarkAllForDailyDescriptions" />
            <Action source="parent.UnmarkAllForDailyDescriptions" />
           <Action source="MDK_CopyTaskList" title="Copy Tasks to Budget"/>
            
          </If>
        </Actions>
        <Columns>
          <Field source="TaskName" title="Task" />
          <Field source="Description" />
          <Field source="Blocked" size="small" />
          <Field source="ActivityNumber" title="Derived Act. No." size="small" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="OverwriteActivity" />
          <Field source="UseDailyDescriptions" />
          <If condition="parent.CanEditIncludedInFixedPriceVar">
            <Field source="IncludedInFixedPrice" />
          </If>
          <Else>
            <Field source="IncludedInFixedPrice" open="false" />
          </Else>
          <Field source="AllowTimeRegistration" />
          <Field source="AllowExpenseRegistration" />
          <Field source="AllowMileageRegistration" />
          <Field source="AllowPurchasing" />
          <Field source="AllowInvoicingOnAccount" />
          <Field source="AllowInvoicingPlan" />
        </Columns>
      </Table>
      <Table name="Tasks_Custom">
        <Actions all="not HasPMReadOnly()">
          <!-- <If condition="hasWorkspace('Jobs')">
						<Action source="parent.MarkAllForDailyDescriptions" />
						<Action source="parent.UnmarkAllForDailyDescriptions" />
					</If> -->
          <Action source="parent.CopyJobTasks" wizard="CopyJobTasksWizard" disabled="HasPMReadOnly()" />
           <Action source="parent.MDK_CopyTaskList" title="Copy Tasks to Budget" icon="Dot" disabled="HasPMReadOnly()" />
           
        </Actions>
        <Columns>
          <Field source="TaskName" title="Task" />
          <Field source="Description" mandatory="true" />
          <Field source="Blocked" size="small" />
          <Field source="ActivityNumber" title="Derived Act. No." size="small" />
          <Field source="ActivityTextVar" title="Activity Name" />
          <Field source="UseDailyDescriptions" />
          <!-- <If condition="parent.CanEditIncludedInFixedPriceVar">
            <Field source="IncludedInFixedPrice" />
          </If> -->
        </Columns>
      </Table>
    </LowerPane>
  </Layout>
</MDML>

