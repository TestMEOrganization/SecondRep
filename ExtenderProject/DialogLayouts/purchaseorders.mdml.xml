<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="PurchaseOrders">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="Purch. Order No. ^1" arguments="PurchaseOrderNumber" />
        </Compact>
        <Columns>
          <Field source="PurchaseOrderNumber" />
          <Field source="PurchaseOrderStatusCode" />
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="PurchaseProcessNumber" />
          <Field source="SupplierNumber" />
          <Field source="Name1" />
          <Field source="Name2" />
          <Field source="Name3" />
          <Field source="ZipCode" />
        </Columns>
      </Filter>
      <Filter name="InVendorContext">
        <Compact>
          <Description template="Purch. Order No. ^1" arguments="PurchaseOrderNumber" />
        </Compact>
        <Columns>
          <Field source="PurchaseOrderNumber" />
          <Field source="PurchaseProcessNumber" />
          <Field source="PurchaseOrderStatusCode" />
          <Field source="JobNumber" />
          <Field source="JobName" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Wizard name="CopyPurchaseOrderWizard" title="Copy Purchase Order">
        <Page form="CopyPurchaseOrderPage1" title=" " description="The lines from the selected purchase order will be added to the current purchase order" />
      </Wizard>
      <Form name="Custom">
        <Actions>
          <If condition="not (workspace() like 'VendorInvoices')                  and not (workspace() like 'Vendors')                  and not (workspace() like 'CompanyVendors')">
            <Update />
            <Delete />
            <Print />
            <PrintThis />
            <Action source="SubmitPurchaseOrder" />
            <Action source="ApprovePurchaseOrder" />
            <Action source="ClosePurchaseOrder" />
            <Action source="ReopenPurchaseOrder" />
            <Action source="CopyPurchaseOrder" wizard="CopyPurchaseOrderWizard" />
            <Action source="AttachDocument" />
          </If>
        </Actions>
        <Column>
          <Group title="Purchase Orders">
            <Field source="PurchaseOrderNumber" />
            <Field source="PurchaseOrderStatusCode" />
            <Field source="OurReference" title="Our Ref." />
            <Reference foreignKey="PurchaserEmployeeNumber_Employee" />
            <Reference foreignKey="RequisitionerEmployeeNumber_Employee" />
            <Pair firstSource="JobName" secondSource="JobNumber" secondSize="small" title="Job" />
        	<Field source="Purchaser" shadowTitle="On behalf of" />
          </Group>
          <Group title="Contact Info">
          	<Field source="Text4" title="Dir. Telephone" shadowTitle="Remember to fill in" />
          	<Field source="Text5" title="Email Address" shadowTitle="Remember to fill in" />
          </Group>
          <Group title="Remarks for print">
            <Field source="Remark1" appearance="maximized" shadowTitle="Remark 1" >
            	<Style height="3" />
            </Field>
            <Field source="Remark2" appearance="maximized" shadowTitle="Remark 2" >
            	<Style height="3" />
            </Field>
            <Field source="Remark3" appearance="maximized" shadowTitle="Remark 3" >
            	<Style height="3" />
            </Field>
          </Group>
          <Group title="Price, Currency" style="textAlignment">
            <Field source="Currency" />
            <Field source="PriceListNumber" title="Item Price List" />
            <Field source="PurchaseOrderSumCurrency" title="Line Sum" />
            <Field source="InvoiceDiscountPercentage" />
            <Field source="InvoiceDiscount" title="Invoice Discount" />
            <Field source="CostPercentage" />
            <Field source="Costs" />
            <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
              <Field source="InvoiceVATVar" title="Invoice Tax Sum" />
              <Field source="SumVAT1CurrencyVar" title="Invoice Tax, Level 1" />
              <Field source="SumVAT2CurrencyVar" title="Invoice Tax, Level 2" />
              <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                <Field source="SumVAT3CurrencyVar" title="Invoice Tax, Level 3" />
              </If>
            </If>
            <Else>
              <Field source="InvoiceVATVar" title="Invoice Tax" />
            </Else>
            <Field source="InvoiceAmountVar" title="Invoice Amount" />
          </Group>
          <Group title="Terms and Conditions">
            <Field source="VendorPopup5" title="Purchase Terms" size="vast" />
            <Field source="DeliveryTerms" title="Delivery Terms" size="vast" />
            <Field source="DeliveryMode" size="vast" />
            <Field source="PaymentTerms" title="Payment Terms" size="vast" />
            <Field source="Remark7" title="Destination" size="vast" /> 
            <Field source="VendorPopup4" title="PO Type" size="vast" />
            <Field source="PreferredDeliveryDate" title="Pref. Deliv. Date" size="vast" />
          </Group>
          <Group title="Remarks for internal use">
            <Field source="Remark4" appearance="maximized" shadowTitle="Remark 4" >
            	<Style height="3" />
            </Field>
            <Field source="Remark5" appearance="maximized" shadowTitle="Remark 5" >
            	<Style height="3" />
            </Field>
            <Field source="Remark6" appearance="maximized" shadowTitle="Remark 6" >
            	<Style height="3" />
            </Field>
          </Group>
          <Group>
            <Link workspace="C_PurchaseProcesses" template="Purchase Process: ^1" title="" tooltip="Open Current Purchase Process" arguments="PurchaseProcessNumber" appearance="maximized" icon="WorkspaceLink">
              <Waypoint pane="PurchaseProcessFilter">
                  <Restriction template="Selected Purchase Process" expression="PurchaseProcessNumber=target.PurchaseProcessNumber" />
                <Focus>
                  <Match field="PurchaseProcessNumber" value="PurchaseProcessNumber" />
                </Focus>
              </Waypoint>
              <Target pane="PurchaseProcesses_Card" >
                  <Restriction template="Selected Purchase Process" expression="PurchaseProcessNumber=target.PurchaseProcessNumber" />
                  <Focus>
                    <Match field="PurchaseProcessNumber" value="PurchaseProcessNumber" />
                  </Focus>
              </Target>
            </Link>
          </Group>
        </Column>
        <Column>
          <Group title="Vendor's Order">
            <Field source="SupplierOrderNumber" title="Order No." />
            <Field source="YourReference" title="Your Reference" />
            <Field source="OurCustomerNumber" />
          </Group>
          <Group title="Dispatching Address">
            <Field source="SupplierNumber" size="large" shadowTitle="Vendor Number" />
            <Field source="Name1" appearance="maximized" shadowTitle="Name" />
            <Field source="Name2" appearance="maximized" shadowTitle="Address 1" />
            <Field source="Name3" appearance="maximized" shadowTitle="Address 2" />
            <ZipCity citySource="PostalDistrict" zipSource="ZipCode" appearance="maximized" zipShadowTitle="Zip Code" cityShadowTitle="Postal District" />
            <Field source="Name4" appearance="maximized" shadowTitle="Address 3" />
            <Field source="Name5" appearance="maximized" shadowTitle="Address 4" />
            <Field source="Attention" title="Attn." />
            <Field source="Country" />
            <Field source="Telephone" />
            <Field source="Telefax" />
            <Field source="Telex" />
            <Field source="ElectronicMailAddress" />
