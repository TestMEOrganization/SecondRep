            <Field source="PayerName4Var" appearance="maximized" />
            <Field source="PayerName5Var" appearance="maximized" />
            <Field source="PayerAttVar" title="Attn." />
            <Field source="PayerCountryVar" title="Country" />
          </Group>
          <Group title="Status">
            <Field source="LastPrintingDate" title="Date Printed" />
            <Field source="NumberOfPrintouts" title="Quantity" />
            <Field source="Submitted" />
            <Field source="SubmittedBy" />
            <Field source="DateSubmitted" title="Date" />
