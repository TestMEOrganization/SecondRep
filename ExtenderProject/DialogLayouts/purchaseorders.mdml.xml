            <Field source="DeliveryName4Var" appearance="maximized" />
            <Field source="DeliveryName5Var" appearance="maximized" />
            <Field source="DeliveryAttVar" title="Attn." />
            <Field source="DeliveryCountryVar" title="Country" />
          </Group>
          <Group title="Billing Address">
            <Field source="PaymentSupplier" title="Vendor" />
            <Field source="PayerName1Var" appearance="maximized" />
            <Field source="PayerName2Var" appearance="maximized" />
            <Field source="PayerName3Var" appearance="maximized" />
