            <Field source="ClosingDate" title="Date" />
          </Group>
          <Group ref="DimensionGroup" />
          <Group ref="UserUpdateGroup" />
        </Column>
        <Column>
          <If condition="not hasWorkspace('PurchaseProcesses')">
            <Group>
              <Link workspace="PurchaseProcesses" template="More details..." tooltip="Detailed purchase order" appearance="maximized" icon="WorkspaceLink">
