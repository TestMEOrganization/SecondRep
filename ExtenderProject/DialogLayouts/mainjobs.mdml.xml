              <Field source="SelectInvoiceOnMainJob" title="Main Job Invoice Selection" />
              <Field source="CombinePrintoutLines" title="Combine Invoice Printout Lines" />
            </Group>
            <Group title="Invoicing Status">
              <Field source="ApprovedForInvoiceEditing" title="Approved for Editing" />
              <Field source="ApprovedForInvoicing" />
              <Pair firstSource="ApprovedForInvoicingBy" secondSource="ApprovedForInvoicingDate" title="Approved By" firstSize="medium" />
            </Group>
            <Group title="Price" style="compact_amountAlignment">
              <Field source="MainJobInvoicingPrincipleVar" title="Pricing Principle" />
              <Field source="MainJobFixedPriceCurrencyVar" title="Fixed Price on Main Job" />
              <Field source="FixedPriceInclSubjobsCurrencyVar" title="Fixed Price incl. Subjobs" />
              <Field source="FixedPriceMainJobInvoicingCurrencyVar" title="Fixed Price for Main Job Inv." />
            </Group>
          </Column>
          <Column>
            <Group title="Links">
              <Link workspace="Jobs" template="Open Main Job" appearance="maximized" tooltip="Open the main job in the jobs workspace" icon="WorkspaceLink">
                <Target pane="Jobs_Filter">
                  <Restriction template="Main Job: ^1 (^2)" arguments="JobName;JobNumber" expression="target.JobNumber = JobNumber" />
                  <Focus>
                    <Match field="JobNumber" value="JobNumber" />
                  </Focus>
                </Target>
              </Link>
              <Link workspace="Jobs" template="All Sub Jobs" appearance="maximized" tooltip="Open list of sub jobs in the jobs workspace" icon="WorkspaceLink">
                <Target pane="Jobs_Filter">
                  <Restriction template="Sub Jobs for: ^1 (^2)" arguments="JobName;JobNumber" expression="target.MainJobNumber = JobNumber" />
                </Target>
              </Link>
            </Group>
            <Group title="Payment">
              <Field source="PaymentTerms" title="Pmt. Terms" />
              <Boolean source="ToBeBlanketInvoiced" />
              <Boolean source="VAT" />
              <Field source="CompanyVATCode" />
              <Field source="Currency" />
              <Field source="AllocationPercentageSumVar" title="Allocation %" />
            </Group>
          </Column>
        </Row>
      </Form>
