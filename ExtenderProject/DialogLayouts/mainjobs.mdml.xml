<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="MainJobs">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1 (^2), ^3" arguments="JobName;JobNumber;Name1" />
        </Compact>
        <Columns>
          <Field source="JobNumber" />
          <Field source="JobName" />
          <Field source="JobGroup" />
          <Field source="NotInvoiceable" />
          <Field source="InternalJob" />
          <Field source="Status" />
          <Field source="ConvertedBy" />
          <Field source="ConversionDate" />
          <Field source="ProjectManagerNumber" />
          <Field source="SalesPersonNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Job">
              <Reference foreignKey="JobNumber_JobHeader" />
              <Field source="JobGroup" />
              <Field source="Status" />
              <Field source="Responsible" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
              <Field source="DepartmentNumber" />
              <Field source="Closed" title="Closed" />
              <Field source="InvoiceLayout" />
            </Group>
            <Group title="Invoicing Status">
              <Field source="ApprovedForInvoiceEditing" title="Approved for Editing" />
              <Field source="ApprovedForInvoicing" />
              <Field source="ApprovedForInvoicingBy" title="Approved by" />
              <Field source="ApprovedForInvoicingDate" title="Approval Date" />
            </Group>
            <Group title="Company">
              <Reference foreignKey="CompanyNumber_CompanyInformation" />
              <Field source="BaseCurrency" />
            </Group>
            <Group title="Settling Company">
              <Reference foreignKey="SettlingCompany_CompanyInformation" />
            </Group>
          </Column>
          <Column>
            <Group title="Main Job Invoicing">
              <Field source="MainJobInvoicing" title="Includes Subjobs" />
              <Field source="SelectInvoiceOnMainJob" title="Main Job Invoice Selection" />
              <Field source="CombinePrintoutLines" title="Combine Invoice Printout Lines" />
            </Group>
            <Group title="Price">
              <Field source="MainJobInvoicingPrincipleVar" title="Pricing Principle" />
              <Field source="MainJobFixedPriceCurrencyVar" title="Fixed Price on Main Job" />
              <Field source="FixedPriceInclSubjobsCurrencyVar" title="Fixed Price incl. Subjobs" />
              <Field source="FixedPriceMainJobInvoicingCurrencyVar" title="Fixed Price for Main Job Inv." />
            </Group>
            <Group title="Customer">
              <Reference foreignKey="CustomerNumber_Customer" descriptionSource="Name1" />
              <Reference foreignKey="PaymentCustomer_Customer" />
            </Group>
            <Group title="Payment">
              <Field source="PaymentTerms" title="Pmt. Terms" />
              <Boolean source="ToBeBlanketInvoiced" />
              <Boolean source="VAT" />
              <Field source="CompanyVATCode" />
              <Field source="Currency" />
              <Field source="AllocationPercentageSumVar" title="Allocation %" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Standard">
        <Actions all="true">
          <Exclude>
            <Action source="AllocateEvenly" title="" tooltip="" />
          </Exclude>
          <Action source="parent.Create" title="Create New Sub Job" wizard="CreateSubJobWizard" disabled="JobNumber = '' defaultTo true" />
        </Actions>
        <Row>
          <Column>
            <Group title="Main Job Invoicing" style="textAlignment">
              <Field source="MainJobInvoicing" title="Includes Subjobs" />
              <Field source="SelectInvoiceOnMainJob" title="Main Job Invoice Selection" />
              <Field source="CombinePrintoutLines" title="Combine Invoice Printout Lines" />
              <Field source="AllocationPercentageSumVar" title="Allocation %" />
            </Group>
            <Group title="Invoicing Status">
              <Field source="ApprovedForInvoiceEditing" title="Approved for Editing" />
              <Field source="ApprovedForInvoicing" />
              <If condition="ApprovedForInvoicingBy != ''">
                <Description template="^1 by ^2" arguments="ApprovedForInvoicingDate;ApprovedForInvoicingBy" title="Approved" />
              </If>
            </Group>
          </Column>
          <Column>
            <Group title="Price" style="compact_amountAlignment">
              <Field source="MainJobInvoicingPrincipleVar" title="Pricing Principle" />
              <Field source="MainJobFixedPriceCurrencyVar" title="Fixed Price on Main Job" />
              <Field source="FixedPriceInclSubjobsCurrencyVar" title="Fixed Price incl. Subjobs" />
              <Field source="FixedPriceMainJobInvoicingCurrencyVar" title="Fixed Price for Main Job Inv." />
            </Group>
          </Column>
          <Column>
            <Group title="Links">
              <Link workspace="Jobs" template="All Sub Jobs For This Job" icon="WorkspaceLink" appearance="maximized" tooltip="Open list of sub jobs in the jobs workspace">
                <Target pane="Jobs_Filter">
                  <Restriction template="Sub Jobs for: ^1 (^2)" arguments="JobName;JobNumber" expression="target.MainJobNumber = JobNumber" />
                </Target>
              </Link>
            </Group>
            <Group title="Job Setup">
              <Field source="InvoiceLayout" />
              <Boolean source="ToBeBlanketInvoiced" />
              <Field source="Closed" title="Closed" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="Custom_subjobs">
        <Actions all="not HasPMReadOnly()">
          <Exclude>
            <Action source="AllocateEvenly" title="" tooltip="" />
          </Exclude>
          <Action source="parent.Create" title="Create New Sub Job" wizard="CreateSubJobWizard_ram" disabled="HasPMReadOnly() or JobNumber = '' defaultTo true" />
        </Actions>
        <Row>
          <Column>
            <Group title="Main Job Invoicing" style="textAlignment">
              <Field source="MainJobInvoicing" title="Includes Subjobs" />
              <Field source="SelectInvoiceOnMainJob" title="Main Job Invoice Selection" />
              <Field source="CombinePrintoutLines" title="Combine Invoice Printout Lines" />
              <Field source="AllocationPercentageSumVar" title="Allocation %" />
            </Group>
            <Group title="Invoicing Status">
              <Field source="ApprovedForInvoiceEditing" title="Approved for Editing" />
              <Field source="ApprovedForInvoicing" />
              <If condition="ApprovedForInvoicingBy != ''">
                <Description template="^1 by ^2" arguments="ApprovedForInvoicingDate;ApprovedForInvoicingBy" title="Approved" />
              </If>
            </Group>
          </Column>
          <Column>
            <Group title="Price" style="compact_amountAlignment">
              <Field source="MainJobInvoicingPrincipleVar" title="Pricing Principle" />
              <Field source="MainJobFixedPriceCurrencyVar" title="Fixed Price on Main Job" />
              <Field source="FixedPriceInclSubjobsCurrencyVar" title="Fixed Price incl. Subjobs" />
              <Field source="FixedPriceMainJobInvoicingCurrencyVar" title="Fixed Price for Main Job Inv." />
            </Group>
          </Column>
          <Column>
            <Group title="Links">
              <Link workspace="Jobs" template="All Sub Jobs For This Job" icon="WorkspaceLink" appearance="maximized" tooltip="Open list of sub jobs in the jobs workspace">
                <Style linkForegroundColor="rgb=161,191,54">
                  <Label linkForegroundColor="rgb=161,191,54" />
                </Style>
                <Target pane="Jobs_Filter">
                  <Restriction template="Sub Jobs for: ^1 (^2)" arguments="JobName;JobNumber" expression="target.MainJobNumber = JobNumber" />
                </Target>
              </Link>
            </Group>
            <Group title="Payment">
              <Field source="PaymentTerms" title="Pmt. Terms" />
              <Boolean source="ToBeBlanketInvoiced" />
              <Boolean source="VAT" />
              <Field source="CompanyVATCode" />
              <Field source="Currency" />
              <Field source="AllocationPercentageSumVar" title="Allocation %" />
            </Group>
          </Column>
        </Row>
      </Form>
      <Form name="MainJobReadonly">
        <Row>
          <Column>
            <Group title="Main Job">
              <Reference foreignKey="JobNumber_JobHeader" />
              <Reference foreignKey="ProjectManagerNumber_Employee" />
              <Field source="JobGroup" />
              <Field source="Status" />
              <Field source="Responsible" size="medium" />
              <Field source="DepartmentNumber" />
              <Field source="Closed" title="Closed" />
