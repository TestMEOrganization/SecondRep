            </Group>
            <Group title="Customer">
              <Reference foreignKey="CustomerNumber_Customer" descriptionSource="Name1" title="Ship to Customer" />
              <Reference foreignKey="PaymentCustomer_Customer" />
            </Group>
          </Column>
          <Column>
            <Group title="Main Job Invoicing">
