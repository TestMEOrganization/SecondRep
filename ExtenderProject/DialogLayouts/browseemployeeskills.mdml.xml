<!-- JSV, 24/08/11: Use reference element where possible (39874).-->
<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="BrowseEmployeeSkills">
    <FilterPane>
      <Filter name="Standard">
        <Columns>
          <Field source="NameOfUser" />
          <Field source="SkillRequirementKey" />
          <Field source="EmployeeCategoryNumber" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Define>
        <Include fragment="GD_Dimensions" />
      </Define>
      <Form name="Custom">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Employee Criteria{1}">
              <Interval firstSource="FromEmployeeNumber" lastSource="ToEmployeeNumber" title="Employee No." />
              <Reference foreignKey="EmployeeCategoryNumber_EmployeeCategory" />
              <Boolean source="PrimaryEmployeeCategory" title="Primary Employee Category" />
              <Interval firstSource="FromCompanyNumber" lastSource="ToCompanyNumber" title="Company No." />
              <Field source="EmployeeType" title="Employee Type" appearance="interval" />
              <Field source="DepartmentNumber" title="Department" appearance="interval" />
            </Group>
            <Group ref="DimensionsTargetGroupIntervals" title="Employee Criteria, Dimensions" />
          </Column>
        </Row>
      </Form>
      <Form name="Full">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Skill Requirements">
              <Grid>
                <Header cells="|Skill No.|; |Name|;       |Level|;      |Description{0}|;  |Requirement Type|" />
                <Row cells="SearchSkill1; SearchSkill1NameVar; SearchSkill1Level; SearchSkill1DescrVar;SearchSkill1RequirementType" />
                <Row cells="SearchSkill2; SearchSkill2NameVar; SearchSkill2Level; SearchSkill2DescrVar;SearchSkill2RequirementType" />
                <Row cells="SearchSkill3; SearchSkill3NameVar; SearchSkill3Level; SearchSkill3DescrVar;SearchSkill3RequirementType" />
                <Row cells="SearchSkill4; SearchSkill4NameVar; SearchSkill4Level; SearchSkill4DescrVar;SearchSkill4RequirementType" />
                <Row cells="SearchSkill5; SearchSkill5NameVar; SearchSkill5Level; SearchSkill5DescrVar;SearchSkill5RequirementType" />
                <Row cells="SearchSkill6; SearchSkill6NameVar; SearchSkill6Level; SearchSkill6DescrVar;SearchSkill6RequirementType" />
                <Row cells="SearchSkill7; SearchSkill7NameVar; SearchSkill7Level; SearchSkill7DescrVar;SearchSkill7RequirementType" />
                <Row cells="SearchSkill8; SearchSkill8NameVar; SearchSkill8Level; SearchSkill8DescrVar;SearchSkill8RequirementType" />
                <Row cells="SearchSkill9; SearchSkill9NameVar; SearchSkill9Level; SearchSkill9DescrVar;SearchSkill9RequirementType" />
                <Row cells="SearchSkill10;SearchSkill10NameVar;SearchSkill10Level;SearchSkill10DescrVar;SearchSkill10RequirementType" />
              </Grid>
              <Field source="AcceptSkillViolations" title="Skill Deviation Acceptance" />
            </Group>
            <Group>
              <Label title="" />
            </Group>
          </Column>
          <Column>
            <Group title="Employee Criteria{1}">
              <Interval firstSource="FromEmployeeNumber" lastSource="ToEmployeeNumber" title="Employee No." />
              <Reference foreignKey="EmployeeCategoryNumber_EmployeeCategory" />
              <Boolean source="PrimaryEmployeeCategory" title="Primary Employee Category" />
              <Interval firstSource="FromCompanyNumber" lastSource="ToCompanyNumber" title="Company No." />
              <Field source="EmployeeType" title="Employee Type" appearance="interval" />
              <Field source="DepartmentNumber" title="Department" appearance="interval" />
              <Field source="MinNumberOfHoursAvailable" title="Min. Hours Available" appearance="interval" />
              <Field source="AcceptOfOvertime" title="Overtime Acceptance" appearance="interval" />
              <Field source="OvertimeThreshold" title="Overtime Threshold" appearance="interval" />
              <Interval firstSource="FromDate" lastSource="ToDate" title="Date Range{1}" />
            </Group>
            <Group ref="DimensionsTargetGroupIntervals" title="Employee Criteria, Dimensions" />
          </Column>
        </Row>
      </Form>
      <Form name="Employees">
        <Actions all="true" />
        <Row>
          <Column>
            <Group title="Skill Requirements">
              <Grid>
                <Header cells="|Skill No.|; |Name|;       |Level|;      |Description{0}|;  |Requirement Type|" />
                <Row cells="SearchSkill1; SearchSkill1NameVar; SearchSkill1Level; SearchSkill1DescrVar;SearchSkill1RequirementType" />
                <Row cells="SearchSkill2; SearchSkill2NameVar; SearchSkill2Level; SearchSkill2DescrVar;SearchSkill2RequirementType" />
                <Row cells="SearchSkill3; SearchSkill3NameVar; SearchSkill3Level; SearchSkill3DescrVar;SearchSkill3RequirementType" />
                <Row cells="SearchSkill4; SearchSkill4NameVar; SearchSkill4Level; SearchSkill4DescrVar;SearchSkill4RequirementType" />
                <Row cells="SearchSkill5; SearchSkill5NameVar; SearchSkill5Level; SearchSkill5DescrVar;SearchSkill5RequirementType" />
                <Row cells="SearchSkill6; SearchSkill6NameVar; SearchSkill6Level; SearchSkill6DescrVar;SearchSkill6RequirementType" />
                <Row cells="SearchSkill7; SearchSkill7NameVar; SearchSkill7Level; SearchSkill7DescrVar;SearchSkill7RequirementType" />
                <Row cells="SearchSkill8; SearchSkill8NameVar; SearchSkill8Level; SearchSkill8DescrVar;SearchSkill8RequirementType" />
                <Row cells="SearchSkill9; SearchSkill9NameVar; SearchSkill9Level; SearchSkill9DescrVar;SearchSkill9RequirementType" />
                <Row cells="SearchSkill10;SearchSkill10NameVar;SearchSkill10Level;SearchSkill10DescrVar;SearchSkill10RequirementType" />
              </Grid>
              <Field source="AcceptSkillViolations" title="Skill Deviation Acceptance" />
            </Group>
            <Group>
              <Label title="" />
            </Group>
          </Column>
          <Column>
            <Group title="Employee Criteria{1}">
              <Interval firstSource="FromEmployeeNumber" lastSource="ToEmployeeNumber" title="Employee No." />
              <Reference foreignKey="EmployeeCategoryNumber_EmployeeCategory" />
              <Boolean source="PrimaryEmployeeCategory" title="Primary Employee Category" />
              <Interval firstSource="FromCompanyNumber" lastSource="ToCompanyNumber" title="Company No." />
              <Field source="EmployeeType" title="Employee Type" appearance="interval" />
              <Field source="DepartmentNumber" title="Department" appearance="interval" />
            </Group>
            <Group ref="DimensionsTargetGroupIntervals" title="Employee Criteria, Dimensions" />
          </Column>
        </Row>
      </Form>
    </UpperPane>
    <LowerPane>
      <Table name="Full">
        <Actions all="true" />
        <Columns>
          <Field source="EmployeeNumber" />
          <Field source="TableEmployeeNameVar" title="Name" />
          <Field source="TablePrimaryCategoryNumberVar" title="Primary Category No.{1}" />
          <Field source="TableCategoryNameVar" title="Category Name" />
          <Field source="SkillPercentage" />
          <Field source="HoursAvailable" />
          <Field source="RequiredOvertime" />
          <Field source="PotentialOvertime" />
          <Field source="PotentialLoadPercentage" />
          <Field source="LevelPercentage" />
          <Field source="NumberOfViolations" />
          <Field source="NumberOfOverqualifications" />
        </Columns>
      </Table>
    </LowerPane>
  </Layout>
</MDML>

