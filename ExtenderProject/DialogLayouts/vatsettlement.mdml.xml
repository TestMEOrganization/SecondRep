              <Field source="BasisWithoutInvestmentTax" title="Without Investment Tax" />
            </Group>
            <Group title="Status">
              <If condition="CreatedByName != ''">
                <Description template="^1 by ^2" arguments="DateMade;CreatedByName" title="Created" />
              </If>
              <Else>
