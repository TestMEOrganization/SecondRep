<MDML xmlns="http://www.deltek.com/ns/mdml" version="0.59">
  <Layout source="VATSettlement">
    <FilterPane>
      <Filter name="Standard">
        <Compact>
          <Description template="^1" arguments="Name" />
        </Compact>
        <Columns>
          <Field source="Name" />
          <Field source="FromMonth" />
          <Field source="FromYear" />
          <Field source="ToMonth" />
          <Field source="ToYear" />
          <Field source="ApprovedBy" />
          <Field source="ApprovedDate" />
          <Field source="Settlement" />
        </Columns>
      </Filter>
      <Filter name="Approved">
        <ControlBar>
          <Selection default="ApprovedVATSettlements">
            <Option name="ApprovedVATSettlements" title="Approved VAT Settlements" restriction="ApprovedDate != date(nulldate)" />
          </Selection>
        </ControlBar>
        <Compact>
          <Description template="^1/^2 - ^3/^4" arguments="FromMonth;FromYear;ToMonth;ToYear" />
        </Compact>
        <Columns>
          <Field source="FromMonth" />
          <Field source="FromYear" />
          <Field source="ToMonth" />
          <Field source="ToYear" />
          <Field source="ApprovedBy" />
          <Field source="ApprovedDate" />
          <Field source="Settlement" />
        </Columns>
      </Filter>
      <Filter name="ApprovedSamePeriod">
        <ControlBar>
          <Selection default="ApprovedVATSettlements">
            <Option name="ApprovedVATSettlements" title="Approved VAT Settlements" restriction="FromYear = parent.FromYear                  and FromMonth = parent.FromMonth                   and ToYear = parent.ToYear                  and ToMonth = parent.ToMonth                  and not Reopened                  and ApprovedBy != ''" />
          </Selection>
        </ControlBar>
        <Compact>
          <Description template="Approved by ^1 (^2)" arguments="ApprovedBy;ApprovedDate" />
        </Compact>
        <Columns>
          <Field source="ApprovedBy" />
          <Field source="ApprovedDate" />
          <Field source="Settlement" />
        </Columns>
      </Filter>
    </FilterPane>
    <UpperPane>
      <Form name="Full">
        <Actions>
          <Print />
          <PrintThis />
          <Action source="PrintSettlement" />
          <If condition="ApprovedBy=''">
            <Action source="CreateSettlement" />
            <Action source="ApproveSettlement" />
          </If>
          <Else>
            <Action source="ReopenSettlement" />
          </Else>
        </Actions>
        <Row>
          <Column>
            <Group title="Tax Reporting Unit">
              <Field source="Name" title="Name" />
              <Field source="CurrencyVar" title="Currency" />
            </Group>
            <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
              <Group title="Sales{Amount}, Tax level 1">
                <Field source="SalesSubjectToVAT" title="Taxable" />
                <Field source="SalesExport" title="Export{noun}" />
                <Field source="NonVATExemptSalesLevel1Var" title="Taxable + Export" />
                <Field source="SalesFreeOfVAT" title="Tax Exempt{VAT}" />
                <Field source="TotalSales" title="Total Turnover" />
              </Group>
              <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
                <Group title="Sales{Amount}, Tax level 3">
                  <Field source="SalesSubjectToVATLevel3" title="Taxable" />
                  <Field source="SalesExportLevel3" title="Export{noun}" />
                  <Field source="NonVATExemptSalesLevel3Var" title="Taxable + Export" />
                  <Field source="SalesFreeOfVATLevel3" title="Tax Exempt{VAT}" />
                  <Field source="TotalSalesLevel3" title="Total Turnover" />
                </Group>
              </If>
            </If>
            <Group title="Taxes">
              <Field source="OutgoingSum" title="Tax Payable" />
              <Field source="AcquisitionVATSum" title="Acquisition Tax" />
              <Field source="InvestmentTax" title="Investment Tax" />
              <Field source="TaxesTotalVar" title="Total Taxes" />
            </Group>
            <If condition="maconomy:booleanSysPar('ShowThreeVATLevels')">
              <Group title="EU Revenue, Tax Level 3">
                <Field source="EUPurchaseLevel3" title="EU Purchase" />
                <Field source="EUSaleLevel3" title="EU Sale" />
                <Field source="EUVATFreeSalesLevel3" title="Tax Exempt Sales" />
              </Group>
            </If>
            <Group title="Deduction">
              <Field source="IncomingSum" title="Tax Receivable" />
              <Field source="OtherTaxes" title="Taxes" />
              <Field source="TaxAllowanceTotalVar" title="Total Allowance" />
            </Group>
            <Group title="Reconciliation Tax">
              <Field source="ReconciliationSum" title="Ratio Tax" />
              <Field source="SalesTax" title="Sales Tax{sg}" />
            </Group>
            <Group title="Tax Settlement">
              <Field source="Settlement" title="Tax Settlement" />
            </Group>
          </Column>
          <Column>
            <Group title="Period">
              <PeriodYearInterval firstPeriodSource="FromMonth" firstYearSource="FromYear" lastPeriodSource="ToMonth" lastYearSource="ToYear" title="Mth/Yr" />
            </Group>
            <If condition="maconomy:booleanSysPar('UseMultipleVATLevels')">
              <Group title="Sales{Amount}, Tax level 2">
                <Field source="SalesSubjectToVATLevel2" title="Taxable" />
                <Field source="SalesExportLevel2" title="Export{noun}" />
                <Field source="NonVATExemptSalesLevel2Var" title="Taxable + Export" />
                <Field source="SalesFreeOfVATLevel2" title="Tax Exempt{VAT}" />
